SET GRAMCHK-S-BOUNDARY  = (Pron Interr) OR (Pron Rel) OR ("muhto") OR ("de" Adv) OR MO OR ("\;") OR (":") OR ("-") OR ("–") OR CS OR PUNCT-LEFT-RIGHT;
