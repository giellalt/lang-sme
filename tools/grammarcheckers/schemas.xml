<?xml version="1.0"?>
<locatingRules xmlns="http://thaiopensource.com/ns/locating-rules/1.0">
  <!-- Having this file in the same dir as errors.rnc and errors.xml
       lets Emacs show validation errors. -->
  <typeId id="errors" uri="errors.rnc"/>
  <documentElement localName="errors" typeId="errors"/>
  <uri pattern="errors.xml" typeId="errors"/>
</locatingRules>
