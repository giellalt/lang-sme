SELECT Adv IF (0 OVDAL OR MANNEL)(-1 VFIN LINK 2 PUNCT) ;
    ## Lea álki áddet manin dat vulge ovdal - dalle go olles stuorraservodat badjelgeahčai sámi eallima ..
    
    
SELECT Adv IF (0 OVDAL OR MANNEL)(-1 VFIN LINK 2 (Num Nom) LINK 1 Gen LINK 1 CLB) ;
