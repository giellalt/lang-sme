SELECT Adv IF (0 OVDAL OR MANNEL)(-1 CLB LINK 2 PrfPrc LINK 1 Acc OR Gen LINK 1 Ill );
    ## Buot dát leat Strovbmasullovuonas Bearalvági gielddas ja lei 1902 njuolggadusaid láhkavuođuin , ovdal lihttoláigohuvvon Finnmárkku fylkkamánnái ..
    
SELECT Pr IF (0 OVDAL OR MANNEL)(-1C VFIN LINK 2 Acc OR Gen LINK 1 Acc OR Gen LINK 1 (Pl Loc) OR (Sg Com)) ;
