SELECT Po IF (0 OVDAL OR MANNEL) (-1 (Dem Sg Gen) OR (Dem Sg Acc) LINK -1 Acc OR Gen LINK 0 Sem/Time OR Sem/Measr OR Sem/Amount OR Sem/Year)(1C Nom LINK 1 VFIN);
