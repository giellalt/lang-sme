!  =====================================
!! Phonological converter for North Sámi regular text
!! =====================================

!! - Downcase uppercase letters
  A -> a ,
  Á -> á ,
  E -> e ,
  I -> i ,
  O -> o ,
  U -> u ,
  Y -> y ,
  Æ -> æ ,
  Ø -> ø ,
  Å -> å ,
  B -> b ,
  C -> c ,
  Č -> č ,
  D -> d ,
  Đ -> đ ,
  F -> æ ,
  G -> g ,
  H -> h ,
  J -> j ,
  K -> k ,
  L -> l ,
  M -> m ,
  N -> n ,
  Ŋ -> ŋ ,
  P -> p ,
  Q -> k ,
  R -> r ,
  S -> s ,
  Š -> š ,
  T -> t ,
  Ŧ -> ŧ ,
  V -> v ,
  W -> w ,
  X -> x ,
  Z -> z ,
  Ž -> ž ;
