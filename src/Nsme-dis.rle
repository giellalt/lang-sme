SELECT:killAllahtenotCS ("ahte" CS) ; 

#!! * __vaiCCCNP__ vai as CC or CS
SELECT:vaiCCCNP (CC @CNP) (0 ("vai")) (*-1 Inf BARRIER REAL-V OR CLB)(*1 Inf BARRIER REAL-V);
	#$ Buriid dahkat vai baháid dahkat.

#!! * __vaiCC__ remove vai as CC 
REMOVE:vaiCC CC (0 ("vai"))(NEGATE *-1 Qst OR Interr OR ("go" Pcle))(NEGATE 1 Qst OR Neg)(NEGATE 2 ("go" Pcle))(*1 VFIN LINK NOT 0 N)(NEGATE *1 ("?"));
