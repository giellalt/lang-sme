#!! * __Rel__ selects Rel
SELECT:Rel Rel (*-1 NOT-AUX-V BARRIER S-BOUNDARY LINK *-1 NP-HEAD-NOM BARRIER S-BOUNDARY)(*1C NP-HEAD-NOM BARRIER S-BOUNDARY);
	#$ ...ahte olbmot besset oaidnit maid sii sáhttet vuordit.

#!! * __RelSg1, RelSg2__ select Rel
SELECT:RelSg1 (Rel Sg) (-1C NP-HEAD-SG OR (Num Sg))(NEGATE *-2 NUM BARRIER NOT-NPMOD)(*1 VFIN)(*1 TRANS-V);
REMOVE:RelSg2 (Rel Sg) (*-1 (N Sg) BARRIER WORD LINK *-1 CC BARRIER NOT-NPMOD LINK -1 N);
	#$ Mun oasttán peanna ja bliánta, maid mun dárbbašan skuvllas.
	#$ Nubbi maid fuobmáimet lei ahte... 

