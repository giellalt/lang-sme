SELECT:SelImprtExclSg (Imprt Sg2) IF (-2 Nom + Sg)(-1 COMMA)(*1 ("^excl"));

#!! * __ImprtComma__ 
SELECT:ImprtComma Imprt (-1 COMMA LINK -1 Imprt LINK *-1 BOS BARRIER NOT-ADV);

#!! * __ImprtNotVGen__
REMOVE:ImprtNotVGen VGen (-1 BOS OR PUNCT OR REAL-CLB)(0 Imprt)(NOT *1C VFIN BARRIER NOT-ADV-PCLE OR CLB);

#!! * __NotImprtInd__ 
