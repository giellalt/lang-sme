SELECT:InterrIfPot Interr (*-1 BOC BARRIER NOT-PCLE)(0 Rel)(*1 TRANS-V + Pot BARRIER S-BOUNDARY OR VFIN-POS);
	#$ Na maid dal juo dagažat ja maid it dagaš.

REMOVE:KillInterr Interr ;

#!! * __munPl3__ removes Pron Pers Pl3 if there is no verb agreement
REMOVE:munPl3 Pers (NEGATE *-1 PL1-V BARRIER NOT-ADV-PCLE)(0 ("mii"))(*1 SG3-V BARRIER PL1-V OR SV-BOUNDARY LINK NOT 0 Pl1);
