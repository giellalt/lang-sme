#!! * __CVPA__ A @CNP A 	
REMOVE:CVPA (@CVP) (-1C A)(1C A LINK *1 N BARRIER NOT-NPMOD);
	#$ E-boasta lea hui álkes ja jođánis vuohki sáddet reivviid birra máilmmi.


#!! * __CVPAccAdv__ Acc @CNP Adv Acc	
REMOVE:CVPAccAdv (CC @CVP) (-1 Acc)(*1 Acc BARRIER NOT-NPMODADV);

SELECT:CVP (@CVP) (*-1C VFIN BARRIER S-BOUNDARY)(*1C NP-HEAD-NOM BARRIER NOT-NPMOD LINK *1 VFIN BARRIER S-BOUNDARY)(NEGATE 0 go LINK *1 Inf BARRIER VFIN) ;
	#$ Áhčči vulggii, ovdalgo mun bohten.
	#$ Mun mannen maŋŋilgo mii leimmet čorgen. # fungerer ikke enda pga manglende verbdisambiguering


#!! * __killAllCNP__ removes all remaining @CNP
REMOVE:killAllCNP @CNP (0 @CVP) ;

