SELECT:PxSg3AccSTV (N Acc PxSg3) IF (-1 STRICT-TRANS-V - ("muitalit") LINK NOT 0 Attr)(NEGATE *-2 Acc BARRIER SV-BOUNDARY OR CC LINK NOT 0 Loc) ;
