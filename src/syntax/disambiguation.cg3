SELECT:PxSg1coord1 (PxSg1)  IF (*-1 SG1-V - ("leat") LINK NOT 0 PrfPrc)(NEGATE 0 Dem OR PrfPrc)(1 ("ja") OR ("dahje") OR ("dehe") OR COMMA LINK 1 PxSg1) ;
