LIST @ARG = @SUBJ @OBJ @SPRED @OPRED ;
LIST @-FARG = @-FOBJ @-FSUBJ ;

LIST @ADVL = @ADVL @-FADVL ;
LIST @-FADVL = @-FADVL ;

SET @ARG/ADVL = @ARG OR @ADVL - (CC @ADVL) ;
SET @-FARG/ADVL = @-FARG OR @-FADVL ;

SET @CLAUSE = @ARG/ADVL OR @-FARG/ADVL OR VFIN OR NONVFIN ;

