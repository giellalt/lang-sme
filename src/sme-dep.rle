LIST @>Num = @>Num ; 
LIST @>P = @>P ; 
LIST @A< = @A< ; 
LIST @ADVL = @ADVL ; 
LIST @APP->Pron = @APP->Pron ;
LIST @APP->Pron = @APP->Pron ; 
LIST @APP-ADVL< = @APP-ADVL< ; 
LIST @APP-N< = @APP-N< ; 
LIST @APP-Num< = @APP-Num< ; 
LIST @APP-Pron< = @APP-Pron< ; 
LIST @CNP = @CNP ; 
LIST @COMPL-CS< = @COMPL-CS< ; 
LIST @CVP = @CVP ; 
LIST @FS-ACC>> = @FS-ACC>> ; 
LIST @FS-ADVL = @FS-ADVL ; 
LIST @FS-OBJ = @FS-OBJ ; 
LIST @FS-STA = @FS-STA ; 
LIST @HAB = @HAB ; 
LIST @ICL-ADVL = @ICL-ADVL ; 
LIST @ICL-AUX< = @ICL-AUX< ; 
LIST @ICL-OBJ = @ICL-OBJ ; 
LIST @ICL-STA = @ICL-STA ; 
LIST @N< = @N< ; 
LIST @Num< = @Num< ; 
LIST @OBJ = @OBJ ; 
LIST @PCLE = @PCLE ; 
LIST @SUBJ = @SUBJ ; 
LIST @V = @V ; 
LIST @XADVL< = @XADVL< ; 


LIST @COMPL-CS< = @COMPL-CS< ;


LIST @ARG = @SUBJ @OBJ @SPRED @OPRED ;
LIST @-FARG = @-FOBJ @-FSUBJ ;

LIST @XADVL = @ADVL @-FADVL ;


SET @ARG/ADVL = @ARG OR @XADVL - (CC @XADVL) ;
SET @-FARG/ADVL = @-FARG OR @-FADVL ;

SET @CLAUSE = @ARG/ADVL OR @-FARG/ADVL OR VFIN OR NONVFIN ;


LIST @AUX = @-FAUXV @+FAUXV <aux> ;
LIST @MV = @-FMAINV @+FMAINV <mv> ;



