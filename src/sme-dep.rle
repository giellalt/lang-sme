LIST @ICL-ADVL = @ICL-ADVL ;
LIST @ICL-AUX< = @ICL-AUX< ;

LIST @AUX = @-FAUXV @+FAUXV <aux> ;
LIST @MV = @-FMAINV @+FMAINV <mv> ;

CORRECTIONS

SUBSTITUTE (V) (<mv> V) TARGET @MV ;
SUBSTITUTE (V) (<aux> V) TARGET @AUX ;

SUBSTITUTE (@-FMAINV) (@ICL-AUX<) TARGET NONVFIN (*-1 @AUX BARRIER NOT-ADV-PCLE) ;
SUBSTITUTE (@+FAUXV) (@ICL-AUX<) TARGET VFIN (*-1 @AUX BARRIER NOT-ADV-PCLE) ;

SUBSTITUTE (@+FMAINV) (@V) TARGET V ;
SUBSTITUTE (@-FMAINV) (@V) TARGET V ;
SUBSTITUTE (@+FAUXV) (@V) TARGET V ;
SUBSTITUTE (@-FAUXV) (@V) TARGET V ;



MAPPINGS

### hab

SETPARENT @HAB TO (0* V-3);


### pop

SETPARENT @>P + Gen TO (1 (Po @ADVL));

### np

SETPARENT @>N TO (*1 N BARRIER @CLAUSE) ;
SETPARENT @N< OR @APP-N< TO (*-1 N BARRIER @CLAUSE OR @CNP) ;
SETPARENT @APP->Pron TO (*1 Pron BARRIER @CLAUSE) ;
SETPARENT @APP-Pron< TO (*-1 Pron BARRIER @CLAUSE) ;

### adjp

SETPARENT @>A TO (*1 A OR Adv BARRIER @CLAUSE) ;
SETPARENT @A< TO (*-1 A OR Adv BARRIER @CLAUSE) ;

SETPARENT @ADVL< OR @APP-ADVL< TO (*-1 Adv) ;
SETPARENT @>ADVL TO (*1 Adv) ;

### nump

SETPARENT @>Num TO (*1 Num BARRIER @CLAUSE) ;
SETPARENT @Num< OR @APP-Num< TO (*-1 Num BARRIER @CLAUSE) ;

### vp

#SETPARENT @-FMAINV OR @-FAUXV TO (*-1 @+FAUXV OR @-FAUXV) ;
SETPARENT @ICL-AUX< TO (*-1 @AUX) ;

### clause level

SETPARENT @ARG/ADVL - @SUBJ TO (*1 @MV BARRIER S-BOUNDARY) ;
SETPARENT @ARG/ADVL - @SUBJ TO (*-1 @MV BARRIER S-BOUNDARY) ;

SETPARENT @SUBJ TO (*1 VFIN BARRIER S-BOUNDARY) ;
SETPARENT @SUBJ TO (*-1 VFIN BARRIER S-BOUNDARY) ;

SETPARENT @-FSUBJ TO (*1 NONVFIN BARRIER S-BOUNDARY) ;
SETPARENT @-FSUBJ TO (*-1 NONVFIN BARRIER S-BOUNDARY) ;

SETPARENT @-FOBJ TO (*1 NONVFIN BARRIER S-BOUNDARY) ;
SETPARENT @-FOBJ TO (*-1 NONVFIN BARRIER S-BOUNDARY) ;



### subjunction

SETPARENT @COMPL-CS< TO (*-1 CS) ;
SETPARENT CS + @CVP TO (*1 VFIN OR NONVFIN) ;

### coordination

SETPARENT @CNP TO (*-1 N OR Pron) ;
SETPARENT CC + @CVP TO (*-1 VFIN OR NONVFIN) ;

SETPARENT (CC @ADVL) TO (*1 WORD LINK 1 CC) ;



SETPARENT @SUBJ TO (*-1 @SUBJ BARRIER NPNH LINK 1 CC);
SETPARENT @APP-N< TO (*-1 @APP-N< BARRIER NPNH LINK 1 CC);
SETPARENT @OBJ TO (*-1 @OBJ BARRIER NPNH LINK 1 CC);
SETPARENT @ADVL TO (*-1 @ADVL BARRIER NPNH LINK 1 CC);
SETPARENT @+FMAINV TO (-2 @+FMAINV LINK 1 CC);
SETPARENT @-FMAINV TO (-2 @-FMAINV LINK 1 CC);



