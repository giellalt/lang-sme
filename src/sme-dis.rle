# Miscellaneous rules related to accusative
# -----------------------------------------
 # The REMOVE Acc rules in this section are rules that do not make reference
 # to an unambiguous Acc in the environment. Rules that do so come at the end 
 # of the accusative chapter.

SELECT (N Sg Acc) IF (-1 ("čáhkket"))(0 ("sadji"));
	## Njálmmálaš duođaštusat fertejit čáhkket saji čálalaš kultuvrra
	## duođaštusaide.

REMOVE Acc IF (-1 Attr)(NOT -1 VERB)(*1 (N Nom) OR (N Acc) 
	BARRIER NPNH OR COMMA OR CC LINK 1 CC LINK NOT *1 Po BARRIER NPNH);
	## ...muhto dálá máilmmi váivvit ja riggodaga fillehus hávkada sáni.

REMOVE Acc IF (*-1 COPULAS BARRIER NON-ADV-PCLE)(*1 NP-HEAD-NOM BARRIER NPNH);
	## ...ahte dat lea maiddái divššohasa dáhttu ja ahte...

REMOVE Acc IF (*-1 Nom BARRIER NON-ADV-PCLE LINK *-1 COPULAS BARRIER NON-ADV-PCLE)
	(*1 CS OR EOS BARRIER V-NON-AUX);
	## Leat go don juvddalaččaid gonagas?

REMOVE Acc IF (*-1 COPULAS BARRIER NON-ADV-PCLE)(*1 Po BARRIER NPNH LINK *1 Nom
	BARRIER NPNH);
	## ...šattai dan oainnu mielde lunddolaš, ahte...

REMOVE Acc IF (*-1 Pass BARRIER VFIN OR Inf OR CS OR CP OR CC OR CLB);
	## ...mii lea celkojuvvon profehta Jesaja bokte.

REMOVE Nom IF (*-1C Nom BARRIER CS OR CP OR CC OR CLB OR go OR ("/"))
	(*-1 V-NON-AUX BARRIER CS OR CP OR CC OR CLB LINK NOT 0 PrsPrc)
	(NOT 0 Num LINK -1 N OR ("§"))(NOT 1 CC LINK 1 Nom)
	(NOT *-1 VFIN BARRIER CS OR CP LINK *-1 VFIN BARRIER CS OR CP);
	## Na mun váccán joatkkaskuvlla doppe.
 # Two finite verbs, two Nom: "Nu guhkás go Gonagas mearrida gusto láhka... 

REMOVE Nom IF (-1 (N Nom))(*1 V-NON-AUX BARRIER NON-ADV-PCLE);
	## Lagamus oapmahaččas lea vuoigatvuohta journála geahččat.

REMOVE Gen IF (NOT *-1 Pr OR Num BARRIER NPNH)(1 CC)(2 NP-BORDER);
	## Čuovvavaččat ovdanbuktit váldofáddájoavkkuid ja plánejuvvon 
	## doaibmabijuid.

REMOVE (N Gen) IF (NOT *-1 Pr OR Num BARRIER NPNH)(1 (Pron Pers Gen));
	## Gonagas sáhttá addit láhkaásahusaid dán birra.


# Topicalised object
# ------------------

SELECT Acc IF (-1 BOS OR CS)(0 ("mii"))(1 (N Pl Gen))(2 NP-BORDER); 
	## Maid ipmašiid doppe dagat?

REMOVE Nom IF (NOT *-1 (V Pl3) BARRIER CS)(NOT -1 ("§") OR ("b"))(0 NUMERALS)
	(NOT 1 CC)(NOT *1 (V Pl3) BARRIER CS);
	## Bargu ollašuhttit máŋga dáin doaibmabijuin lea juo álggahuvvon.


# Accusative between Aux and main verb
# ------------------------------------

# After negation
# - - - - - - - -
SELECT Acc IF (*-1 (V Neg) BARRIER CS OR CP)(NOT *-1 ConNeg BARRIER Neg 
	LINK NOT 0 ("leat"))(NOT 0 Nom)(*1 ConNeg OR PrfPrc BARRIER NON-ADV-PCLE 
	LINK NOT *1 Acc);
	## Na muhto don it leat dasa nama čállán!


# In front of an infinitive
# - - - - - - - - - - - - - 
SELECT Acc IF (*-1 VFIN BARRIER NPNHAI)(NOT 0 VERB)(NOT 0 Nom OR Adv OR TIME)
	(*1 Inf BARRIER NON-ADV-PCLE OR CLB);
	## Dasgo Herodes lei bidjan olbmuid váldit Johanasa gitta.

SELECT Acc IF (*-1 VFIN BARRIER CS OR CP OR PUNCT LINK *-1 Nom BARRIER CS OR CP)
	(NOT 0 PrfPrc OR Nom OR Adv OR TIME)(1 Inf);
	## Almmolaš buohcceviessu dahje guovlopsykiatralaš guovddáš galgá
	## divššu čađahit.

SELECT Acc IF (*-1 VFIN BARRIER NPNH)(1 CC)(*2 NP-HEAD BARRIER NPNH LINK 1 Inf);
	## ...ahte lea ferten niesteseahka dahje lávkka guoddit.


# In front of a participle
# - - - - - - - - - - - - -
SELECT Acc IF (*-1 LEAT-FIN-NON-IMP BARRIER NPNHAI)(NOT 0 Adv OR Nom OR TIME)
	(*1 ACT-PRFP BARRIER NON-ADV LINK NOT *1 ("leat" PrfPrc) BARRIER CS OR CP);
	## Mun lean dan dahkan.
	## Váfistan dihte bargovugiid lea komitea viiddiduvvon. # *1 ACT-PRFP!


# Accusative postverbally
# -----------------------

SELECT Acc IF (*-1 V-NON-AUX-PASS BARRIER NON-ADV-PCLE OR REALCLB)(0 Num)
	(*1 (N Gen) BARRIER NPNH);
	## Mun máksen 100 ruvnno duvle.

SELECT Acc IF (*-1C V-NON-AUX-PASS BARRIER NPNHA or CLB OR ("nugo"))
	(NOT 0 (Sg Nom) LINK *-1 (V Sg3) BARRIER VFIN OR CS OR CP)
	(NOT -1 PrfPrc LINK NOT *-1 ("leat") BARRIER CS OR CP)
	(NOT 0 PrfPrc OR Adv OR VFIN OR Loc OR TIME)(1C NP-BORDER);
	## Sámegillii galbet universitehta lea šaddan realitehta.

SELECT Acc IF (*-1C V-NON-AUX-PASS BARRIER NPNHAIIP OR COMMA)
	(NOT -1 PrfPrc OR PrsPrc LINK -1 BOS OR CS)(0 NP-HEAD)
	(NOT 0 PrfPrc or Adv or Dem or Nom OR Loc OR TIME) 
	(*1C NP-BORDER OR (Num Nom) OR PUNCT BARRIER WORD)(NOT 1 Po) ;
	## Divššohasbearráigeahčči sáhttá váldit meannudeapmái áššiid, mat... 

 # The (Num Nom) is not optimal, we need to consider the Num Ord and Num Card
 # tagging. Now, 22. is given Num Ord and 22.10. is given Num Nom, this gives
 # the correct result, but is not satisfying, linguistically.

SELECT Acc IF (*-1C Po BARRIER NPNHAIIP LINK -1 N OR Pron LINK *-1C V-NON-AUX-PASS 
	BARRIER NPNHAIIP LINK 0 V-NON-AUX-PASS)(0 NP-HEAD)
	(NOT 0 VFIN OR PrfPrc or Adv OR TIME)(1 NP-BORDER OR ADVLCASE OR Adv)
	(NOT 1 Po);
	## Boazosápmelaččat eai sáhte atnit dološ áiggi rájes geavaheami
	## riektevuođđun bohccuid guođoheapmái Aursunddas.        

 # Extended copy of the previous rule.
 # This rule is to tolerate a PP between the verb and the object.


REMOVE Nom IF (*-1 V-NON-AUX-PASS BARRIER NPNHA OR PUNCT OR CLB OR CS OR CP 
	LINK 0 VFIN LINK *-1 NP-HEAD-SG-NOM OR NP-HEAD-PL-NOM BARRIER NON-ADV 
	OR CLB)(NOT *-1 CC BARRIER NPNH LINK -1 Nom); 
	## Seammá gusto jus mánná molsu skuvlla.

SELECT Acc IF (*-1C V-NON-AUX-PASS BARRIER NPNHAIIP OR COMMA)(0 NP-HEAD)
	(NOT 0 PrfPrc OR Adv OR Dem OR Inf )(1 CC)(*2 VFIN BARRIER NON-ADV-PCLE);  
	## Bearašsuodjaluskantuvrrat fállet divššu ja rávvejit bearrašiid.
 # Could this rule be modified so that it accepts a subject between 0 and verb? 

REMOVE Gen IF (*-1 V-NON-AUX-PASS BARRIER NON-ADV-PCLE)(1 Pers);
	## Sii bilkidit ja doarrádallet din mu dihtii.

REMOVE Gen IF (*-1 V-NON-AUX-PASS BARRIER NON-ADV-PCLE)(1 Dem);
	## De gohčui doalvut iežas don beallái jávrri.

SELECT Acc IF (NOT *-1 Acc BARRIER CS OR CP)(NOT *-1 CC BARRIER NPNH LINK -1 Acc)
	(0 NP-HEAD)(NOT 0 VFIN)(1 (A Ess))(NOT *1 Actio BARRIER NON-ADJ-CC);
	## ...go beaktilis almmolaš beroštumit dagahit dan govttolažžan.

SELECT Acc IF (*-1 ACC-LOC-VERB BARRIER NON-ADV-PCLE LINK NOT 0 Pass)
	(*1 Loc BARRIER NON-NP);
	## Sáhttá viežžat dieđuid divššohasa oapmahaččain.

SELECT Acc IF (*-1 ACC-ILL-VERB BARRIER NON-ADV-PCLE LINK NOT 0 Pass)
	(*1 Ill BARRIER NPNH);
	## Ii oktage bija duohpakeahtes duogŋasa boares biktasii.

REMOVE Gen IF (*-1 V-NON-AUX-PASS BARRIER NPNHA)(0 N)(1 (Pron Indef))
	(NOT *1 Ill BARRIER NPNH LINK 1 Acc);
	## ...čájehit vuollegašvuođa juohke ovtta divššohasa eallimii.

REMOVE Gen IF (*-1 V-NON-AUX-PASS BARRIER NPNHA OR Acc LINK NOT 0 ILL-VERB 
	OR ACC-ILL-VERB OR VGen)(NOT 1 Acc)(*1 Ill BARRIER NPNH LINK NOT 0 ("láhki"));
	## Dasto son vuolggahii sin Betlehemii ja dajai:...
  
REMOVE Gen IF (*-1 Nom BARRIER NON-ADV-PCLE LINK *-1 V-NON-AUX-PASS BARRIER 
	NON-ADV-PCLE LINK NOT 0 ILL-VERB LINK NOT 0 PrsPrc)(*1 Ill BARRIER NPNH);
	## Bijatgo don sohkkara dedjii?

REMOVE Gen IF (*-1 V-NON-AUX-PASS BARRIER NPNHA OR CLB LINK NOT 0 LOC-VERB)
	(NOT 0 Dem OR Prop)(NOT *1 Acc OR Nom BARRIER NPNH OR CC)
	(*1 Loc BARRIER NPNH);
	## Sii manne vissui ja oidne máná etniinis. 
	## Skuvla galggai oahpahit dárogiela vissis guovlluin.  

SELECT Acc IF (*-1 Loc BARRIER NPNH LINK -1 LOC-VERB)(1 NP-BORDER OR CC OR CLB);
	## Divššohasas lea vuoigatvuohta biehttalit vuostáváldimis vara dahje
	## varrabuktagiid.

SELECT Acc IF (*-1 ("dihte") BARRIER NON-NP OR PUNCT LINK -1 Actio)
	(NOT 0 Inf)(1 NP-BORDER OR PUNCT);
	## ...ja ovdanbuktin dihte ođđa čoavddusevttohusaid.

SELECT Acc IF (*-1 V-NON-AUX-PASS BARRIER NPNHA)(1 (VAbess));
	## In mun dáhto vuolggahit sin borakeahttá ruoktut.


# Accusative sentence-finally
# ---------------------------

REMOVE Gen IF (NOT *-1 Pr OR Num OR CC BARRIER NPNH)(0 NOTIME OR Pron)
 	(*1 CS OR CP OR EOS BARRIER NON-ADV-PCLE) ;
	## Ferte čájehit várrugasvuođa go lea sáhka sihkkumis dakkár dieđuid.

 # CC in the preceding rule because of coordination after Pr. The following rule 
 # compensates for it.

SELECT Acc IF (NOT *-1 Pr OR Num BARRIER NPNH)(0 NOTIME OR Pron)
	(NOT 0 Nom OR Loc OR VERB)
	(*1 CLB BARRIER NON-ADV-PCLE LINK NOT *1 Gen BARRIER NPNH);
	## Jus divššohas roasmmohuvvá dahje oažžu duođalaš váttisvuođaid,...
	## Liggen dihtii viesuideaset, ferte ollusat muoraid boaldit.

REMOVE Gen IF (NOT *-1 Pr OR Num BARRIER NPNH)(0 NOTIME OR Pron)
 	(*1 PUNCT BARRIER NON-ADV-PCLE LINK 1 NP-BORDER) ;
	## Dohkkehit persovnnalaš sávaldagaid geavahit álbmotdálkkodeami
	## ja guvhlláriid.

REMOVE Gen IF (NOT *-1 Pr OR CC BARRIER NPNH)(-1 Gen)(0 NOTIME OR Pron)
	(*1 CLB BARRIER NON-ADV-PCLE)(NOT 1 COMMA LINK 1 Gen LINK 1 CC LINK 1 Gen);
	## ...ahte vuhtiiváldit divššohasa dárbbuid, beroštumiid ja riekte-
	## sihkarvuođa dearvvašvuođabálvalusa ektui.


# Numeral as object after postverbal subject
# ------------------------------------------

SELECT Acc IF (-1 (N Nom) LINK *-1 VFIN BARRIER NPNHAIIP LINK 0 V-NON-AUX-PASS)
	(0 Num)(NOT 1 Po);
       ## Lassin nammadii direktevra guokte bargi ja ozai čálli lávdegoddái.


# Objects with object predicatives
# --------------------------------
 # Note that a rule for accusative predicates following accusative subjects and 
 # copulas is found right before the nominative rule block.

SELECT Acc IF (*-1 OPRED-VERB BARRIER NPNHA LINK NOT 0 Pass OR Actio)
	(NOT *1 Acc BARRIER NPNH OR PUNCT OR CC OR ("/"));
	## Dážat eai atnán sámiid buoret go beanan.

SELECT Acc IF (*-1 OPRED-VERB BARRIER NON-NP OR PUNCT LINK NOT 0 Pass OR Actio)
	(NOT *-1C Acc BARRIER CC OR CS)(NOT *-1 Num BARRIER NPNH LINK -1 Acc)
	(1 Ess);
	## Lea deaŧalaš movttiidahttit vuolit gráda studeanttaid válljet 
	## sámi guoskevaš čuolbmačilgehusaid ja eamiálbmotáššiid váldofágafáddán.

SELECT Acc IF (*-1 OPRED-VERB BARRIER NON-NP OR PUNCT LINK NOT 0 Pass OR Actio)
	(NOT *-1C Acc BARRIER CC OR CS)(1 PUNCT OR CLB OR EOS);
	## ...ja muđui atná son doavggusin oađđadagaid (duolji, ránu) ja 
	## biktasiid.

SELECT Acc IF (*-1 CC BARRIER NPNH LINK -1 Ess LINK -1 Acc)(NOT 0 PrsPrc)
	(1 A OR Ess);
	## ...dahkan doaimmaid vuoiggaleabbun ja sosiálabálvalusaid álkibut.

REMOVE Acc IF (*-1 Acc BARRIER NON-NP OR Inf OR CLB LINK *-1 OPRED-VERB 
	BARRIER NPNH)
	(NOT -1 ("/") OR CC LINK -1 Acc);
	## ...go doavttir lea dahkan biehttaleami apotehka vuoigatvuođa vuostá.

REMOVE Acc IF (*-1 Acc BARRIER NON-NP LINK *-1 OPRED-VERB BARRIER NON-NP)
	(NOT *-1 COMMA OR ("/") OR (PUNCT LEFT) OR CC BARRIER NPNH LINK -1 Acc)
	(NOT 0 Interr);
	## Dát sáhttá dáhpáhuvvat go atná konkrehtalaš daguid/hilgumiid ovtta
	## dahje nuppi riekteáddejumi boađusin.
 

# Accusative subjects of infinite verbs
# -------------------------------------

SELECT Acc IF (*-1 V-NON-AUX-PASS BARRIER NON-NP)(NOT 0 Gen LINK *1 (N Acc) 
	BARRIER NPNH OR CC)(*1 (Actio Ess) BARRIER NON-NP LINK NOT -1 CC);
	## De oinnii olbmá čohkkámin tuolloviesus.


# Accusative in apposition
# ------------------------
SELECT (N Acc) IF (1C (Prop Acc));
	## Jesus válddii fárrosis Biehtára, Jáhkoba, ja su vielja Johanasa. 

SELECT Acc IF (*-1 (PUNCT LEFT) BARRIER NON-NP OR ABBR LINK -1C Acc)
	(1 (PUNCT RIGHT));
SELECT Acc IF (-2C Acc)(-1 (PUNCT LEFT))(NOT 1 Acc)
	(*1 (PUNCT RIGHT) BARRIER NON-NP);
	## ...ja muđui atná son doavggusin oađđadagaid (duolji, ránu) ja 
	## biktasiid.


# Accusative in coordination
# --------------------------

# First part
# - - - - - -
SELECT Acc IF (0 N)(1 CC)(*2C NP-HEAD-ACC BARRIER NPNHA OR Gen LINK 1 NP-BORDER);
	## Dalle skáhppo biergasiid nugo firpmiid ja buođđomuoraid.

SELECT Acc IF (1 CC)(2C Acc)(3C NP-BORDER);
	## Bovde váivvážiid ja rámbbiid, lápmásiid ja čalmmehemiid.

SELECT Acc IF (*-1 V-NON-AUX-PASS BARRIER NPNHA OR CONTRA)(1 CC OR CLB)
	(*2C Acc BARRIER NON-ADJ LINK 1 CC OR CLB OR NP-BORDER);
	## Dát guoská bargguide ásahit mastera ja bachelora,..

SELECT Acc IF (*-1 V-NON-AUX-PASS BARRIER NPNHA)(1 CC)(2 Acc)(3 CLB);
	## ...váldi mearridit mihtuid ja deattuid,...

SELECT Acc IF (*-1C V-NON-AUX-PASS BARRIER NPNHAII)(1 CLB)(2 Acc)(3 CC OR CLB)
	(NOT 4 Gen LINK *1 N BARRIER NPNH OR CC)
	(NOT 4 Indef LINK 1 Gen LINK *1 N BARRIER NPNH OR CC);
	## Jesus válddii fárrosis Biehtára, Jáhkoba ja Johanasa.

SELECT Acc IF (*-1C V-NON-AUX-PASS BARRIER NPNHAII)(0 N)(1 CC)
	(*2C NP-HEAD-ACC BARRIER NPNH OR Gen);
	## Mii guoská váldofáddájovkui mii gieđahallá dutkama ja oahpahusa, de...

SELECT Acc IF (1 ("/") LINK *1 NP-HEAD-ACC BARRIER NPNH LINK 1 NP-BORDER);
	## Lea dárbu ásahit sámegiela vierisgiellaoahpahusa/álgooahpahusa.
 
SELECT Acc IF (*-1 V-NON-AUX BARRIER NPNHAII)(0 N)(1 COMMA)
	(*2 NP-HEAD-ACC BARRIER NPNHA LINK 1 CC LINK 1 NP-HEAD-ACC BARRIER NPNHA 
	LINK 1 NP-BORDER);
         ## Jos juohká dážaid, sámiid ja láddelaččaid iešguđet sadjái, ...

SELECT Acc IF (1 COMMA)(*2 Acc BARRIER NPNH LINK 1 CC LINK 1 Acc LINK 1 NP-BORDER);
	## ...váfistan dihte Romssa universitehta sámi dutkama, oahpahusa ja
	## gaskkusteami.


# Second part
# - - - - - -
SELECT Acc IF (*-1 CC BARRIER NPNH LINK -1C Acc)(NOT 0 Inf)(1 NP-BORDER)
	(NOT 0 Dem LINK 1 PrfPrc LINK *1 N BARRIER NPNH);
	## ...ja atnigohte meahci ja vuovddi.
 
SELECT Acc IF (-1 CC OR COMMA BARRIER NPNH LINK *-1 VFIN BARRIER CS OR CP LINK 
	*-1 (Pron Interr) BARRIER VFIN OR CC OR CS OR CP LINK -1 NP-HEAD-ACC)
	(1 NP-BORDER OR CC OR CLB);
	## Divššohasbearráigeahčči galgá addit sutnje dieđuid gii jearrá, ráđiid 
	## ja bagadallama. 

SELECT Acc IF (*-1 CC BARRIER NPNH LINK -1C Acc)(1 COMMA);
	## Álgoálbmogii galgá sihkkarastit "rivttiid ja vejolašvuođaid, maid
	## našunála lágat sihkkarastet eará ássiide".

SELECT Acc IF (*-1 CC OR COMMA BARRIER NPNH LINK -1C NP-HEAD-ACC)(1 COMMA OR CC)
	(*2 NP-HEAD-ACC BARRIER NPNH LINK 1 CLB OR CC OR NP-BORDER);
 	## ...váfistan dihte Romssa universitehta sámi ja eamiálbmot-
	## guskevaš dutkama, oahpahusa ja gaskkusteami.

SELECT Acc IF (*-1 COMMA BARRIER NPNH LINK -1C Acc LINK *-1 V-NON-AUX-PASS 
	BARRIER NPNHAII)(1 CC OR COMMA);
        ## Jos juohká dážaid, sámiid ja láddelaččaid ieš guđet sadjái, ...

SELECT Acc IF (-2C Acc)(-1 ("/"));
	## Dát sáhttá dáhpáhuvvat go atná konkrehtalaš daguid/hilgumiid 
	## okta dahje nuppi riekteáddejumi boađusin.


# Third part
# - - - - - -
SELECT Acc IF (*-1 CC BARRIER NPNH LINK -1C Acc LINK -1 COMMA BARRIER NPNH 
	LINK -1C Acc)(1 NP-BORDER OR Gen);
	## Ferte vuhtiiváldit divššohasa dárbbuid, beroštumiid ja riektesihkar-
	## vuođa dearvvašvuođabálvalusa ektui. 


# Not accusative
# --------------

REMOVE Acc IF (-1 ("juohke"))(0 ("okta"))(*1 N BARRIER NON-ADJ LINK NOT 0 Acc);
	## ...čájehit vuollegašvuođa juohke ovtta divššohasa eallimii.

REMOVE (N Acc) IF (*-1C (N Acc) BARRIER Inf OR Actio OR CS OR CP OR CC OR CLB 
	OR ("/") LINK NOT *-1 Interr BARRIER CS)(NOT 0 Prop LINK -1 (N Acc));
	## Mánáin lea vuoigatvuohta oažžut dárbbášlaš dearvvašvuođaveahki
	## maiddái dearvvašvuođaiskkadeami hámis...

REMOVE Acc IF (*1C (N Acc) BARRIER NPNH OR COMMA OR CC OR ("/"))
	(NOT 0 (N Acc) LINK 1 (Prop Acc));
	## Divššohasas lea vuoigatvuohta iežas journála geahččat. 

REMOVE Acc IF (NOT 0 Dem OR Interr)(*1C (Pron Acc) BARRIER CC OR CP OR CS OR CLB OR 
	Neg OR Actio OR go LINK NOT 0 Interr OR Refl)(NOT 1 Indef);
	## Golbma olbmá jearahit du.

REMOVE Acc IF (NOT *-2 Acc BARRIER WORD)(-1 CC)(NOT 0 Interr)
	(*1 N BARRIER NPNH OR CC OR COMMA);
	## Buot borramuš ja siidda gálvvu, ja... 

REMOVE Acc IF (NOT *-1 V-NON-AUX BARRIER CS)
	(NOT 0 Dem LINK *1 (N Acc) BARRIER NPNH)  
	(NOT *1 V-NON-AUX BARRIER CS OR CP);
        ## Sápmelaččaid historjá álbmogin lea duháhiid jagiid boaris, muhto
        ## dálá ássanguovlluin sápmelaččat eai leat ássan nu guhká.
  # We need a full verb for an obj. 

REMOVE Acc IF (NOT *-1 V-NON-AUX BARRIER CS OR CP)(NOT 1 Ger)
	(*1 V-NON-AUX BARRIER CS OR CP LINK *1C Acc BARRIER CC OR CS OR CP);
	## Bivdi sáhttá vuordit guoli boahtit buđđui.

REMOVE Acc IF (*-1C Acc BARRIER CS OR CP OR REALCLB)(NOT *-1 CC BARRIER NPNH 
	LINK -1 Acc)(*1C A BARRIER NPNH LINK 1 NP-BORDER)(NOT 1 CC);
	## ...maid sáhttá geavahit eamiálbmotjoavkkuid miellahtuid buorrin.

REMOVE Acc IF (NOT *-1 V-NON-AUX BARRIER CS)(NOT 0 TIME)
	(*1 V-PASS BARRIER V-NON-AUX OR CS OR CP);
	## ...ahte sámi nuoraide láhččojuvvo vejolašvuohta oassálastit... 

REMOVE Acc IF (*-1 V-PASS BARRIER VFIN OR Inf)(NOT 0 TIME)
	(NOT *1 V-NON-AUX BARRIER CS OR CP);
	## Dál nannejuvvui girku sámeguovlluin.

REMOVE Acc IF (0C HNOUN OR (Pron Pers))(NOT 1 CC OR PUNCT OR CLB)
	(*1C (N Acc) BARRIER NPNH LINK 1 NP-BORDER OR ADVLCASE);
	## Lea dárbu nannet hálddahuslaš bargiid vejolašvuođa oahppat sámegiela.
	## Dál lea stuorát dáhttu go ovdal atnit sápmelaččaid dološ 
	## geavahanávkkástallama dakkarin. 

REMOVE Acc IF (0C HNOUN OR (Pron Pers))(NOT 1 CLB)
	(*1 Acc BARRIER NPNH OR CC LINK 0C N LINK 1 CLB);
	## Válljedettiin journálaovddasvástideaddji berre árvvoštallat su 
	## fágagealbbu.
	## ...de rahpá čoahkkima ovddit áigodaga Sámedikki čoahkkinjođiheaddji.

SELECT Gen IF (*-1 ("dihte") BARRIER NPNH LINK -1 Actio)(NOT 0 Inf LINk -1 CC)
	(NOT 1 CC)(*1 (N Acc) BARRIER NPNH);
	## ...váfistan dihte Romssa universitehta sámi dutkama.



# Illative
# ========
 # Sg Ill = Prt Sg3 (for some verbs), Pl Ill = Prt Du2 (for some verbs)
 
SELECT Ill IF (*-1 VFIN OR Inf BARRIER CC OR CLB OR CS OR CP)(0 (Prt Sg3))
	(NOT 0 Adv);
	## Dat lea sivalaš helveha dollii.  
 # The last condition because of "oktii" etc.

REMOVE (Prt Du2) IF (*-1 VFIN OR Inf BARRIER CS OR CP);
	## Fágabirrasat main lea gieddebárgu, bovdejuvvojit fállat giedde-
	## bárgguid sámi nuoraide. 

REMOVE (Du2) IF (*1 VFIN BARRIER CC OR CS OR CP OR REALCLB);
	## Jahkásaš bušeahttarámma dán prošeavtta doaimmaide gártá 70.000 ruvnno.

SELECT Ill IF (-2C Ill)(-1 COMMA OR CC);
	## ...vuoigatvuohta guohtumii, bivdui, guollebivdui, jna.

SELECT Ill IF (1 CC OR COMMA)(*2C Ill BARRIER NPNH);
	## Gohccái ja čujuheapmái nammaduvvui gonagaslaš vuovdefáldi.



