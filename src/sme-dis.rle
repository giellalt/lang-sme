ADD (@CS-COMPL) TARGET Nom IF (*-2 ("nu") BARRIER NOT-NP LINK 1 A OR Adv)(-1 ("go"));
	## Nu guhkás go vejolaš galgá dat čuovvoluvvot.
   
MAP (@CS-COMPL) TARGET Nom IF (*-1 ("go") BARRIER NOT-NP LINK NOT -1 VFIN)
	(1 PUNCT)(2 EOS);
	## Ovdabarggut állanit seamma beallái go "láhkateavstta lunddolaš lohkan".

MAP (@CS-COMPL) TARGET Nom IF (*-1 ("go") BARRIER NOT-NP LINK NOT -1 VFIN)
	(1 CC)(NOT *1 VFIN-NOT-IMP);
	## Sin mearridanorgánain berre mearridanváldi leat nu viiddis go vejolaš 
	## ja nu čadni go vejolaš.

MAP (@CS-COMPL) TARGET Nom IF (*-1 ("go") BARRIER NOT-NP OR Nom 
	LINK NOT -1 VFIN OR BOS)(1 CLB LINK NOT *1 VFIN BARRIER CC);
	##  Muhto buođđogeažit ja giđđabuođut ráhkaduvvojit muđui seammá 
	## vugiid mielde go geassebuođut, ja danne heive buođđun birra 
	## obbalohkái juo muitaluvvot.

MAP (@CS-COMPL) TARGET Nom IF (*-1 ("go") BARRIER NOT-NP OR Nom 
	LINK *-1 Nom BARRIER CS OR CONTRA)(NOT *-1 go BARRIER NOT-NP 
	LINK -1 VFIN LINK *-1 BOS OR CS BARRIER NOT-CC)(1 EOS OR CLB)
	(NOT 1 CLB LINK *1 VFIN BARRIER CC)
	(NOT 1 CLB LINK *1 NP-HEAD-NOM BARRIER NPNH)
	(NOT 2 Interr LINK *1 VFIN LINK *1 VFIN BARRIER CS); 
	## ...muhto galjebut dat lea go guv-čalmmit.
	

MAP (@CS-COMPL) TARGET Nom IF (-1 ("dego"))
	(NOT *1 VFIN-NOT-IMP BARRIER CS OR CLB);
	## Sii leat dego eŋgelat almmis.

MAP (@CS-COMPL) TARGET Nom IF (*-1 ("dego") BARRIER NOT-NP)(1 EOS OR CLB);
	## Son oahppaladdá min allagasas dego beaivváža badjáneapmi.

MAP (@CS-COMPL) TARGET (N Nom) IF (*-1 ("go") BARRIER NPNH LINK -1 A)
	(NOT *1 VFIN BARRIER NOT-ADV-PCLE);
	## Máhttájeaddji ii leat stuorit go oahpaheaddjis.

MAP (@CS-COMPL) TARGET (Pron Nom) IF (*-1 ("go") BARRIER NPNH LINK -1 A)
	(NOT *1 VFIN BARRIER NOT-ADV-PCLE);
	## Dalle dat viežžá čieža eará vuoiŋŋa vel, bahábuid go son ieš. 

MAP (@CS-COMPL) TARGET Nom IF (*-1 ("dego") BARRIER NOT-NP)(1 (Pron Interr))
	(*2 VFIN LINK NOT *1 VFIN);
	## Dat lea dego olmmái gii manai olgoriikii.

MAP (@CS-COMPL) TARGET Nom IF (*-1 ("dego") BARRIER NOT-NP)
	(NOT 0 Attr LINK *1 Nom BARRIER NPNH)(NOT *1 VFIN-NOT-IMP BARRIER CS OR CLB)
	(NOT 1 ("go") OR ("dego"));
	## Dego njamaheaddji eadni dikšumin mánáidis...

MAP (@CS-COMPL) TARGET Nom IF (*-1 ("dego") BARRIER NPNH)(0 Sg)(1 CC)
	(*2 (Sg Nom) OR (Sg3 Nom) BARRIER NPNH LINK *1 V-SG3 BARRIER NOT-ADV-PCLE);
	## Dat guhte ii biso mu siste, bálkestuvvo olggos dego oaksi ja son goldná.

MAP (@CS-COMPL) TARGET Nom IF (*-1 COMMA BARRIER NPNHAI LINK -1 (@CS-COMPL))(1 COMMA OR CC)
	(*2 Nom BARRIER NPNH);
	## Seamma gusto uhcit fuđđožiidda nu go neahti, čeavrris, mievri ja rieban.
	## ledje beakkán ja dovddus olbmot nu go Diaz , Maria Hætta , Anne Gaup , Erik ja vel muhtin eará dovddus olbmot

MAP (@CS-COMPL) TARGET Nom IF (*-1 CC BARRIER NPNHAI LINK -1 (@CS-COMPL))
	(NOT *1 VFIN OR go OR ("dego") BARRIER CS);
	## Jesus lea gievrrat go Sáhtán ja bahá vuoiŋŋat. 

MAP (@CS-COMPL) TARGET Acc IF (-2 Acc)(-1 ("dego"));
	## Mun vuolggahan din dego sávzzaid gumppiid gaskii.

ADD (@CS-COMPL) TARGET Nom IF (*-1 ("go") BARRIER NOT-NP LINK NOT -1 VFIN)
	(1 EOS);
	## Dat galgá čuovvoluvvot nu guhkás go vejolaš.


