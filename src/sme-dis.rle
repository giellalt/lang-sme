# Secure Accusative rules
# =======================

# Here we put rules that we want to work before the main Genitive chapter.
# Many of them use the strict transitive verb sets (they really want acc objects, so we give them that).


SELECT Acc IF (*-1 MOVEMENT-VERB BARRIER CS)(0 PLACE)(NOT 1 Po)(NOT -1 CC LINK *1 VFIN) ;
SELECT Acc IF (0 PLACE)(NOT 1 Po)(*1 MOVEMENT-VERB BARRIER CS);
SELECT Acc IF (*-1 ("vázzit") OR ("viehkat") BARRIER CS)(0 EDUCATION)(NOT 1 Po) (NOT 1 ROOM LINK 0 Loc)(NOT -1 CC LINK *1 VFIN) ;

SELECT Acc IF (0 EDUCATION)(NOT 1 Po)(*1 ("vázzit") OR ("viehkat") BARRIER CS) ;
	## Mun váccán vákkiid, vuvddiid ja váriid.
	## Sii vázzet skuvlla.
	## Mun váccán skuvlla gávpogis.
	## Mun váccán skuvlla šiljus. # This is not Acc, but Gen.


# Accusative or genitive in front of ALU and in front of adjectives
# -----------------------------------------------------------------

# Exceptional accusative attributes in front of ALU nouns.

SELECT Acc IF (0 Num LINK *1 MEASURE BARRIER NOT-A LINK 1 ALU);
SELECT Acc IF (0 Num LINK 1 ALU);
SELECT Acc IF (0 Num LINK 1 Num LINK 1 ALU);


# Not-so-exceptional?

SELECT Acc IF (0 NUMERALS)(1 MEASURE)(2 ALU OR A);
	## Visti lei vihtta mehter alu.

SELECT Gen IF (-1 (Num Acc))(0 MEASURE)(1 ALU OR A);
	## Min viessu lea vihtta mehtara alu.
	## Sus lea njeallje mehtara guhkkosaš biila.
	
SELECT Acc IF (0 NUMERALS BARRIER NOTCOMMA)(*1 (Num Acc) LINK 1 MEASURE LINK 1 ALU OR A);
	## Guokte golbma mehtera guhku, ja guhkitge, sáhttá vuojahat leat.
	
SELECT Acc IF (1 (Num Acc) LINK 1 (Num Acc) LINK 1 MEASURE LINK 1 ALU OR A);

SELECT Acc IF (1 COMMA LINK 1 (Num Acc) LINK 1 COMMA LINK 1 (Num Acc) LINK 1 MEASURE LINK 1 ALU OR A);
    ## Čuollun sáhttá leat dušše okta fierbmi gurgaluvvon, muhto dábálaččat lea čuollu guhkit, golbma, njeallja fierbmebaji guhku.

SELECT Acc IF (-1 (Num Acc))(0 MEASURE)(1 Pr);
	## Ádjá viessu lea guokte miilla bajábeal márkana.

SELECT Acc IF (-1 (Num Acc))(0 TIME)(1 ("boaris"));
	## Mánná lea golbma jagi boaris.

SELECT Gen IF (-1 (Num Gen))(0 MEASURE)(1 A OR ALU);
	## Visti lei viđa mehter alu.
	## Dohko manai viđa mehter govdosaš geaidnu.

SELECT Gen IF (-1 (Num Gen))(0 TIME)(1 ("boaris"));
	## Son lea viđa jagi boaris.

SELECT Acc IF (-1 (Num Acc))(0 TIME)(1 ("maŋŋil"));
	## Dat dáhpáhuvai golbma jagi maŋŋil.

SELECT Acc IF (0 Num)(1 (N Acc))(2 ("velggolaš"));
SELECT Acc IF (-1 Num)(1 ("velggolaš"));
	## Ánde lea munnje vihttalot ruvnno velggolaš.

SELECT Gen IF (0 ("okta"))(1 ALU);
	## Dábálaččat leat joddut sullii ovtta guhku.

SELECT Acc IF (0 Num)(1 ("gitta"))(2 (Num Acc));
	## Dát ledje gáddenuohtit mat ledje 180 gitta 200 sala guhku, ja 15 gitta 
	## 20 sala čieŋu.


