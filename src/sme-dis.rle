 	## journálaid.


# Infinitive after predicative
# - - - - - - - - - - - - - - -
 # There are two rules in the beginning, conflate when I see the effect.
 # The barrier may be too weak.

SELECT Inf IF (-1 A BARRIER VFIN OR CP LINK *-1 ("leat") OR ("šaddat") 
	BARRIER NOT-ADV)(NOT 0 (N Pl Nom));
	## Eará guovlluin lei vejolaš nannet dárogiela.

SELECT Inf IF (*-1 N BARRIER VFIN OR CS OR CP LINK *-1 ("leat") OR ("šaddat") 
	BARRIER NPNHAI)(NOT 0 (N Pl Nom));
