	## elektrovnnalaš journála, ja das maiddái bidjat gáibádusaid oahpahussii.

SELECT Nom IF (-1 ("okta" Nom))(0 ("eará"));
	## Ii oktage eará go báhpat oaččo borrat.

SELECT Acc IF (*-1 (V Actio Loc) BARRIER NPNHA)(NOT *1 N BARRIER NPNH);
	## Álbmotčeardan geaid eallindilli hehttii sin ráhkadeames 
	## makkárge sierra stáhta.

SELECT Gen IF (0C Dem)(1 TIME)(1C Gen);
	## Maid Biret-Elle áigu dán dálvve dahkat? 

SELECT Gen IF (-1C NP-HEAD-ACC)(0 NP-HEAD-GEN)(*1 N BARRIER NPNH OR PUNCT 
	LINK 0 ADVLCASE);
	## Mii leat gullan dan su iežas njálmmis.

SELECT Gen IF (-1 Po)(*1 N BARRIER NPNH LINK 0 ADVLCASE)(NOT 0 N LINK 1 Indef);
	## Mii gullat sin sárdnumin Ipmila stuora daguid birra min iežamet gillii.

REMOVE Acc IF (-1C (Pron Interr Acc));
	## Mii hálidit muđui váldit ovdan ahte mii geat leat bargan prošeavttain 
	## leat deattuhan bidjat fuomášumi dasa maid min iežamet sajádagat 
	## mearkkašit guorahallanfáttá ektui.

REMOVE (Dem Acc) IF (*1C (N Gen) BARRIER NPNH LINK 0 HNOUN);
	## Fertejitgo máksit daid vuoivasiid ovddas?

SELECT Gen IF (NOT *-1 V-TRANS-ACT BARRIER CC OR CLB OR CS OR CP)
	(NOT -1 Loc)(0 Num)(*1 (N Sg Gen) BARRIER NPNH LINK 1 N);
	## ..., ahte unnimus 135 jagi geavahus lea doarvái.


# Other rules for nouns and pronouns
# ==================================

SELECT (N Sg Nom) IF (*-1C V-SG3 BARRIER NPNHA OR PrfPrc LINK NOT *-1 (Sg Nom) 
