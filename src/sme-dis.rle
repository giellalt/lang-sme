SELECT PrfPrc IF (-1 (@SUBJ) LINK *-1 REALCOPULAS BARRIER NOT-NP)(NOT 0 Actio LINK -1 PrsPrc)(NOT 0 Actio LINK 1 CC LINK 1 Actio LINK *1 N BARRIER NOT-NP);
