 # Coordinated subjects

SELECT V-PL3 IF (NOT *-1 VFIN OR ConNeg BARRIER NOT-ADV-PCLE)
	(*1 Nom BARRIER NPNHA LINK 1 CC LINK *1 Nom BARRIER NPNH); 
