    ## Máhttájeaddji ii leat stuorit go oahpaheaddjis, ii ge bálvaleaddji
    ## stuorit johtileabbot go hearrás.

SELECT (Ind ConNeg) IF (*-1 (Neg Ind) BARRIER VFIN OR ConNeg OR PrfPrc OR CS OR CP)
	(NOT 0 N LINK *1 ConNeg BARRIER NOT-ADV-PCLE)
	(NOT 0 Loc LINK -1 (Neg Foc) LINK -1 Loc);
