SELECT Loc IF (-1 ACTIVITY-VERB LINK 0 Pass LINK NOT 0 COM-VERB OR TOGETHER-VERB)(0 HUMAN OR ORGANIZATION LINK NOT *1 VFIN BARRIER CP OR CS);
