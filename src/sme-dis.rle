SELECT Nom IF (*-1 COMMA BARRIER NPNHAI LINK -1C Nom)(NOT 0 VFIN)(1 CC);
	## ...geat leat divššohasa lagašolbmot, fuolaheaddji dahje veahkefuolaheddji. 

SELECT Nom IF (1 CC)(2C (Pron Nom))(3 (Pron Nom));
