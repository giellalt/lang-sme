	# In this cycle we apply some safe verb rules.

	# ConNeg forms
	# ------------

SELECT (Imprt ConNeg) IF (*-1 (Neg Imprt) BARRIER ConNeg OR SV-BOUNDARY2);
	## Ale ge jorggit selggiid sutnje gii bivdá dus loanas.

REMOVE (Ind ConNeg) IF (-1 ConNeg)(*-2 (Neg Ind) BARRIER ConNeg OR PrfPrc OR SV-BOUNDARY2)(0 A)(1 go);
  	## Máhttájeaddji ii leat stuorit go oahpaheaddjis, ii ge bálvaleaddji
  	## stuorit go hearrás.
  	## Máhttájeaddji ii leat stuorit go oahpaheaddjis, ii ge bálvaleaddji
  	## stuorit johtileabbot go hearrás.

SELECT (Ind ConNeg) IF (*-1 (Neg Ind) BARRIER ConNeg OR PrfPrc OR SV-BOUNDARY2)(NOT 0 N LINK *1 ConNeg BARRIER NOT-ADV-PCLE)(NOT 0 Loc LINK -1 NEGFOC LINK -1 Loc);
	## Eai dat dohkke šat maninge.

SELECT (Cond ConNeg) IF (*-1 (Neg Ind) BARRIER ConNeg OR SV-BOUNDARY2);
	## Ii go son ollu eanet gárvvohivčče din?

REMOVE ConNeg IF (NOT *-1 Neg BARRIER SV-BOUNDARY2 OR go)(NOT -1 Rel LINK -1 Neg)(NOT -2 Rel LINK -1 Neg)(NOT *-1 (@CVP) BARRIER NOT-AA LINK *-1 ConNeg BARRIER VFIN);
REMOVE (ConNegII) IF (NOT *-1 (Neg Imprt) BARRIER SV-BOUNDARY2);
	## Dasgo mii oinniimet su nástti badjáneamen ja leat boahtán...
	## Stáhta galgá fuolahit ahte gávdno divššohasbearráigeahčči 
	## juohke fylkkas.

SELECT (Imprt ConNeg) IF (*-1 CC BARRIER NOT-ADV LINK *-1 (Imprt ConNeg) BARRIER VERB);
SELECT (Ind ConNeg) IF (*-1 CC BARRIER NOT-ADV LINK *-1 (Ind ConNeg) BARRIER VERB);
	## Allet nappo moraš ja cealkke...
	##

SELECT ConNeg IF (*-1 PrfPrc BARRIER NOT-ADV-PCLE LINK *-1 Neg BARRIER SV-BOUNDARY2)(0 ("leat"))(NOT *1 PrfPrc BARRIER NOT-ADV-PCLE);
	## Ii dál láhppán gal leat.
	# Need more advanced rule to select ConNeg when there is more
	# material between Neg and ConNeg.

REMOVE ConNeg IF (NOT *-1 (Neg) BARRIER CLB OR V-IND-FIN)(NOT *-1 Nom LINK -1 Neg)(NOT *-1 (@CVP) BARRIER NOT-AA LINK *-1 ConNeg BARRIER VFIN); 
	## Juohke muorra mii ii šaddat buriid šattuid, čullojuvvo ja 
	## bálkestuvvo dollii.
	## Gávpot mii lea vári alde, ii sáhte leat čihkosis.

	# Imperative
	# ----------

REMOVE Imprt IF (1 ("go" Pcle));
REMOVE Imprt IF (*-1 (Neg Ind) BARRIER CLB OR V-IND-FIN);
REMOVE Imprt IF (*-1 ConNeg BARRIER COMMA OR SV-BOUNDARY2);
	## Go bohciida soahpameahttunvuohta das ahte galget go journála... 
	## Mánát eai gillen dálu čorget.
	## Na muhto don it leat dasa nama čállán!

REMOVE Imprt IF (-1 A LINK NOT 0 Ess);
REMOVE Imprt IF (-1 N OR Po);
	## Čuorvvas gullo Ramas. 
	## Boarrásis leai dat vel eanaš ealáhussan ge.
 # This will work fine when headings no longer interfere.

REMOVE Imprt IF (*1C VFIN BARRIER NOT-ADV-PCLE OR CLB);
	## Lágas ii duššaduvvo oktage bustávaš dahje čuoggáš, ovdal go...

REMOVE Imprt IF (*-1 ("\(") BARRIER ("\)"))(0 N);
	## Láigolihtus eai čuovo makkárge sierravuoigatvuođat gáddái (buo.
	## riehkit, stárat).

SELECT Imprt IF (-1 BOS OR PUNCT OR REALCLB)(0 ("leat"))(1 A);
	## Leage buorre!
	
SELECT Imprt IF (-1 BOS OR PUNCT OR REALCLB)(1 ("dál"))(2 COMMA)(3 Rel OR Adv);
	## Gullet dál, maid veardádus gilvi birra mearkkaša...
	

	# Infinitive
	# ----------

REMOVE Pl1 IF (-1 VFIN LINK NOT 0 N)(0 Inf);
	## Bargu lei árvvoštallat dan.

SELECT Inf IF (*-1 (Neg Sup) BARRIER S-BOUNDARY2); 
SELECT Inf IF (*-1 ("amas" CS) BARRIER VERB);
	## ...amaset sin čalmmit oaidnit,... 
	## ...amas lojes hearggit livvut. 

SELECT Inf IF (*-1 NP-HEAD-SG-NOM BARRIER NOT-ADV-PCLE LINK *-1 CC BARRIER NPNHA LINK *-1 Inf BARRIER NOT-ADV);
	## Dearvvašvuođaveahkkedárbu galgá árvvoštallojuvvot ja diehto addojuvvot.
 # This rule has to come before the ones below.
 
SELECT Inf IF (*-1 INFV BARRIER SV-BOUNDARY2 OR Neg LINK NOT 0 N)(NOT *1 Inf BARRIER SV-BOUNDARY2);
  	## Bissuin galgá heahkkahuhttit, ii nie váigadit goddit; sáhttá leat váiga.
 
SELECT Inf IF (-1 BOS)(*1 ("ii" V IV Neg Ind Sg3) BARRIER V);
	## Viežžat viervván rivgo čiektageregin ii leat ásti, iige sáváháhttin.

SELECT Inf IF (*-1 ("veara") BARRIER NOT-ADV LINK -1 COPULAS); 	

	# Rules that prevent later selection of Inf for a finite verb in the frame 						  
	# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - 
	# INFV...CC... ____	
 
REMOVE Inf IF (*-1 NP-HEAD-PL-NOM BARRIER NPNHA LINK -1 CC)(NOT *-1 A BARRIER NOT-ADV-PCLE)(NOT *-1 INFV BARRIER VERB);
REMOVE Inf IF (*-1 (Num Nom) BARRIER NOT-NA LINK -1 CC LINK NOT *-1 Inf OR INFV BARRIER VFIN);
	## ...ja ollugat biehttalit iežaset sámevuođa.
	## ...ja máŋga Davviálbmogiid fakultehta studeantta leat oasi oahpus gazzan Romssa universitehtas.

REMOVE Inf IF (-1 CC LINK NOT *-1 Inf BARRIER S-BOUNDARY2);
REMOVE Inf IF (-3 (A Attr))(-2 CC)(-1 PrfPrc)(0 N);
	## Áirasat geat vuostálastet eavttuhusa jienastit nu ahte deaddilit
	## boalu "vuostá" ja čuožžilit.
	## Mearrasápmelaččat ledje áŋgiris ja oahppan bivdit.

	# Going for Pl1 instead of Inf 
	# - - - - - - - - - - - - - - 

REMOVE Inf IF (-1C (@ADVL) LINK *-1 BOS BARRIER NPNH);
	## Vulobealde guorahallat fas mo sáhtášii deattuhit dákkár jávohisvuođa.

REMOVE Pl1 IF (-1 BOS)(0 Inf)(NOT 0 Qst);

	# Now we select Inf 
	# - - - - - - - - -

 # The first rule seems much better without the NOT 0 Adv.

SELECT Inf IF (*-1 INFV BARRIER SV-BOUNDARY2 OR Neg LINK NOT 0 N);#(NOT 0 Adv);
