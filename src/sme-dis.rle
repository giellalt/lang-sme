# Mappings of minor categories
# ----------------------------

MAP (@CC) TARGET CC ;
	## Na muhto don it leat dasa nama čállán!

MAP (@CS) TARGET CS ;
	## Leago guhkes áigi dassá go Máreha oidnet?

MAP (@INTERJ) TARGET Interj ;
	## Maid, iigo leat boahtán?

MAP (@INTERJ) TARGET ("surgat") IF (-1 ("vuoi") OR ("hei"));
	## Vuoi surgat dan gándda!

MAP (@INTERJ) TARGET ("beargalat") IF (*-1 Nom BARRIER CS OR CP LINK NOT 0 (Dem Nom));
	## Gosa don leat beargalat mu reaga dahkan?

MAP (@PCLE) TARGET Pcle ;
	## Amma mii eat leat máksán?


# Determiner mappings
# -------------------

MAP (@DN>) TARGET (Dem Sg Nom) IF (*1C (N Sg Nom) BARRIER NP-BORDER);
	## Váidalusas galgá leat namahuvvon dat dilálašvuohta.

MAP (@DN>) TARGET (Dem Nom) IF (*1 Num OR Pron OR A OR N BARRIER NP-BORDER LINK 0 Nom)
	(NOT 1 Interr OR COMMA OR Dem);
	## Jogo gikse dat guolit?

MAP (@DN>) TARGET (Dem Acc) IF (*1 Num OR Pron OR A BARRIER NP-BORDER OR (N Acc) 
	LINK 0 Acc)(NOT *1 CC BARRIER WORD);
	## Mun lean oastán ja klipsen dáid njiŋŋelasaid.

MAP (@DN>) TARGET (Dem Acc) IF (*1 (N Acc) BARRIER NP-BORDER);
	## Naba daid stuora dorskiid, gosa daid bidjet?

MAP (@DN>) TARGET (Dem Gen) IF (*1 Num OR Pron OR A OR N BARRIER NP-BORDER 
	LINK 0 Gen OR ADVLCASE)(NOT 1 Po);
	## Mun bessen dan bargui.

MAP (@DN>) TARGET (Dem Attr) IF (*1 N BARRIER NP-BORDER);
	## Makkár vaikkuhusat leat jus veahkki ii addojuvvo?

MAP (@DN>) TARGET (Dem Sg Com) IF (*1 (N Sg Com) BARRIER NP-BORDER);
	## Dat ásahuvvojedje aiddo dainna ulbmiliin, ahte...

MAP (@DN>) TARGET (Dem Pl Ill) IF (*1 (N Pl Ill) BARRIER NP-BORDER);
	## De lassánii maid sisafárren dáidda guovlluide.

MAP (@DN>) TARGET (Dem Pl Loc) IF (*1 (N Pl Loc) BARRIER NP-BORDER);
	## ...ahte eamiálbmotriekti sáhttá leat okta dain speadjaliin,...

MAP (@DN>) TARGET (Dem Pl Com) IF (*1 (N Pl Com) BARRIER NP-BORDER);
	## ...de offiseara bijai muhtun ustibiid Jesusa lusa dáiguin sániiguin...

MAP (@DN>) TARGET (Dem Pl Com) IF (*1 (N Pl Com) BARRIER NOT-AA);
	## Ain ferte joatkit daiguin sierra doaibmabijuiguin.


# Attribute mappings
# ------------------

MAP (@ADV-A) TARGET ("beare") OR ("eanet" Adv) OR ("erenoamáš") OR ("earenoamáš") OR 
	("hirbmat") OR ("hui") OR ("menddo") OR ("mii" Sg Gen) OR ("nu") OR ("oba") 
	OR ("oppa" Adv) OR ("veaháš" Adv) IF (1 A)
	(NOT 0 ("eanet") LINK 1 ("uhcit") OR ("unnit"));
	## Soai leaba goappašagat nu jallat.
	## In dieđe man buorit dat leat.

MAP (@ADV-A) TARGET ("mihá") OR ("ollu" Adv) IF (1 Comp OR COMPAR);
	## Dakkár almmuhemiid leat mihá divraseabbo ráhkadit.

MAP (@ADV-A) TARGET ("buot") IF (1 Superl);
	## Guhtemuš báhkkon lea buot mávssolaččamus?

MAP (@ADV-A) TARGET ("mii" Sg Gen) IF (1 (Pron Pers Nom))
	(*1 (A Sg Nom) BARRIER V-NOT-AUX);
	## Leatgo don fuomášan dan máilmmi, man dat lea váralaš?

MAP (@ADV-ADV) TARGET ("eanet" Adv) OR ("erenoamáš") OR ("earenoamáš") OR 
	("hirbmat") OR ("hui") OR ("menddo") OR ("mii" Sg Gen) OR ("nu") OR ("oba") 
	OR ("oppa") IF (1 Adv)(NOT 1 ("fal") OR ("maiddái") OR ("mo"))
	(NOT 0 ("mii" Sg Gen) LINK 1 Po)(NOT 0 ("eanet") LINK 1 ("uhcit") OR ("unnit"));
	## Man dávjá mun livččen dáhtton čohkket du mánáid.

MAP (@ADV-ADV) TARGET Adv IF (0 Indef)(1 VGen)(NOT 1 Sg3);
	## Dát ađa lea uhccán vuošša.

MAP (@ADVL) TARGET ("ihkinassii") IF (-1 ("gosa"));
	## Mun áiigun čuovvut du gosa ihkinassii manažat.

MAP (@PronN>) TARGET ("juohke");
	## Galgá gávdnot divššohasbearráigeahčči juohke fylkkas. 

MAP (@PronN>) TARGET (Pron Attr) ;
	## Leago guhkki eará dáluide?

MAP (@PronN<) TARGET ("eatnat") IF (-1 ("giittos"));
	## Giittos eatnat luoikamis.

MAP (@PronN<) TARGET (Indef Pl Nom) IF (*-1 PPRON-DU-PL OR (N Pl) LINK 0 Nom
	BARRIER CS)(NOT -1 CC OR CLB);
	## Soai leaba goappašagat nu jallat.

MAP (@PronN<) TARGET (Refl Sg Nom) IF (*-1 MUN OR DON OR (Pron Sg3 Nom) OR 
	(Pron Sg Nom) OR (N Sg Nom) BARRIER CS OR CP OR REALCLB);
	## Dasgo mun lean ieš gohčohallama vuloš. 

MAP (@PronN>) TARGET (Refl Sg Nom) IF (*1 MUN OR DON OR (Pron Sg3 Nom) OR 
	(Pron Sg Nom) OR (N Sg Nom) BARRIER NOT-ADV-PCLE);
	## Ieš han don dadjet dan. 

MAP (@PronN<) TARGET (Refl Pl Nom) IF (*-1 PPRON-DU-PL OR (N Pl) BARRIER CS OR CP
	OR REALCLB LINK 0 Nom);
	## Mii han leat ieža gullan dan su iežas njálmmis.

MAP (@PronN>) TARGET (Refl Pl Nom) IF (*1 PPRON-DU-PL OR (N Pl) 
	BARRIER NOT-ADV-PCLE LINK 0 Nom)(NOT *1 VFIN OR Inf BARRIER NOT-ADV-PCLE);
	## ...muhto ieža dii ehpet guoskkat daid suorpmain ge.

MAP (@PronN>) TARGET Indef IF (1 N)(NOT 0 Nom LINK 1 Acc)(NOT 0 ADVLCASE)
	(NOT 1 (N Sg Gen) LINK *1 N BARRIER NPNH OR CC)
	(NOT 1 VFIN OR Ess OR ("veara"));
	## Son galgá beassat buktit oaivilis buot gažaldagaide.

MAP (@PronN>) TARGET Indef IF (0 Nom)(*1 (N Nom) BARRIER NOT-ADJ)
	(NOT 0 Pl LINK 1 Inf);
	## Goabbáge báddi galgá leahkit 60 alen guhku.

MAP (@PronN>) TARGET Indef IF (1 A-CASE)(NOT 1 ("veara"));
	## Sáhttáge ollu buriid dadjat bohtosiid birra.

MAP (@PronN>) TARGET Indef IF (NOT 0 ("iešguhtet") OR ADVLCASE)(1 Num);
	## Buot oktanuppelohkái máhttájeaddji vulge Galileai.

MAP (@PronN>) TARGET ("buot") IF (1 Dem OR Indef);
	## Buot dáid mun attán dutnje.
	## Sii ledje eanet sivalaččat go buot earát Jerusalemis.

MAP (@PronN>) TARGET Indef IF (*1 N BARRIER NP-BORDER OR REALCLB)
	(NOT *-1 ("leat") BARRIER NOT-ADV-PCLE LINK *1 Indef LINK *1 PrfPrc
	BARRIER VERB)
	(NOT *-1 INF-VERB BARRIER NOT-ADV-PCLE LINK *1 Indef LINK *1 Inf
	BARRIER VERB)(NOT 0 ADVLCASE)(NOT 1 (Pers Gen) LINK *1 N BARRIER NOT-NP OR CLB 
	LINK NOT 0 Loc)(NOT 1 Adv LINK 1 N)
	(NOT 1 (N Sg Gen) LINK *1 N BARRIER NPNH)(NOT 1 VFIN OR Ess OR ("veara"));
	## Ollu dáin doaibmabijuin sáhttá álggahit dálá rámmaid siskkobealde.

MAP (@PronN>) TARGET ("mii" Interr Pl Nom) IF (*1C (N Pl Nom) BARRIER NP-BORDER);
	## ...dan hárrái, mat riektegáldut leat, mo daid berre dulkot, ja ...

MAP (@PronN>) TARGET Interr IF (NOT 0 ("gii"))(1 N)
	(NOT 0 ("mii") LINK NOT 1 (Pl Gen))(NOT 0 (Sg Nom) LINK 1 V-SG3)
	(NOT 1 Gen LINK *1 N BARRIER NPNH);
	## Guđe sihkarvuođanjuolggadusat ledje biddjon...

MAP (@PronN>) TARGET Interr IF (0 ("guhtemuš"))(*1 (Pers Loc) BARRIER NOT-PCLE);
	## Guhtemuš ba dis sáhttá...

MAP (@PronN>) TARGET ("buot") IF (*1 (N Pl Nom) OR (N Pl Acc) BARRIER NP-BORDER);
	## Ámtamánnii gulai "bearráigeahččat buot min gullevaš eatnamiid, gittiid 
	## ja opmodagaid".

MAP (@AN>) TARGET (A Attr) ;
	## In fuobmán dadjat ahte Márehis lea ođđa irgi.
	## Dalle gusto vuosttas, nubbi ja goalmmát teakstaoassi seammaláhkái.

# buorre
MAP (@AN>) TARGET Nom IF (0 ("buorre"))(*1 (N Nom) BARRIER NPNH);
	## Leatgo buorit guolit?

MAP (@AN>) TARGET Acc IF (0 ("buorre"))(*1 (N Acc) BARRIER NPNH);
	## Doaivvutgo oažžut buriid guliid?

MAP (@AN>) TARGET (Sg Gen) IF (0 ("buorre"))
	(*1 (N Sg Gen) OR (N Sg Ill) OR (N Sg Loc) BARRIER NPNH);
	## Dearvvašvuođabargiidlága § 40 vuosttas lađđasa mielde galgá 
	## journála čállojuvvot buori fidnodábi mielde.

MAP (@AN>) TARGET (Pl Gen) IF (0 ("buorre"))
	(*1 (N Pl Gen) OR (N Pl Ill) OR (N Pl Com) BARRIER NPNH);
	## Giittán maiddái universitehtalektora Fredrik Harhoff 
	## buriid ságastallamiid ovddas.

MAP (@AN>) TARGET (Sg Com) IF (0 ("buorre"))(*1 (N Sg Com) BARRIER NPNH);
	## Jos mii addet buriin dáhtuin...

MAP (@AN>) TARGET (Pl Ill) IF (0 ("buorre"))(*1 (N Pl Ill) BARRIER NPNH);
	## Son lea sivdnidan min Kristus Jesusis buriide daguide.

MAP (@AN>) TARGET (Pl Loc) IF (0 ("buorre"))(*1 (N Pl Loc) BARRIER NPNH);
	## Son ferte leat dovddus buriin daguin. 

MAP (@AN>) TARGET (Pl Com) IF (0 ("buorre"))(*1 (N Pl Com) BARRIER NPNH);
	##

MAP (@AN>) TARGET Ess IF (0 ("buorre"))(1 (N Ess));
	## Maŋemus son šattai buorrin oahpaheaddjin.

MAP (@PrcN>) TARGET PrfPrc IF (-1 Dem OR Num OR Attr)(NOT -1 Pers)(0 Pass)(1 N);
	## ..maid almmolaš dohkkehuvvon dearvvašvuođabargit addet,..
	## Galgá leat okta čohkkejuvvon divššohasjournála. 

MAP (@PrcN>) TARGET PrfPrc IF (*-1 PrfPrc BARRIER NPNHA LINK 0 V-NOT-AUX 
	LINK NOT 0 Actio)(*1C N BARRIER NOT-ADJ);
	## Ii leat boahtán mannan váhkus.

MAP (@PrcN>) TARGET PrfPrc IF (NOT *-1 ("leat") BARRIER PrfPrc OR (Actio Ess))
	(NOT *-1 PrfPrc LINK 0 (Actio Nom) LINK NOT *-1 Nom BARRIER CS LINK 
	NOT 0 ("leat"))(0 Pass)(*1C N BARRIER NPNH); 
	## ...dan mii lei dáhpáhuvvan bosttahuvvon olbmuide.

MAP (@PrcN>) TARGET PrfPrc (-1 CLB OR go)(1 TIME);
	## Dalle šattašii liikka buorre dálki go mannan sotnabeaivve.

MAP (@PrcN>) TARGET PrfPrc IF (*-1 BOS OR REALCLB BARRIER NOT-NP)(NOT 0 ("leat"))
	(*1 N BARRIER NPNH LINK NOT 0 TIME LINK NOT 1 TIME);
	## Ovdamearkan oaivvilduvvon fálaldahkii lea sámi kulturdieđu lohkanbaji.

MAP (@PrcN>) TARGET PrfPrc IF (*-1 Po BARRIER NOT-NP LINK *-1 BOS OR REALCLB 
	BARRIER NOT-NP)(NOT 0 ("leat"))(*1 N BARRIER NPNH LINK NOT 0 TIME LINK
	NOT 1 TIME);
	## Sámiid várás oaivvilduvvon doaimmaid hárrái galggašii...
 # For those cases where the Po is homonymous with verb, and therefore a barrier
 # in the preceding rule.

MAP (@PrcN>) TARGET PrfPrc IF (*-1 Pr BARRIER NOT-NP LINK -1 NP-BORDER LINK 
	NOT 0 ("leat"))(*1 (N Gen) BARRIER NOT-NP);
	## 1978 láhka balansere gaskal sámi vuoigatvuođaid boarásmuvvan oainnu,...

MAP (@PrcN>) TARGET PrfPrc IF (*-1 V-NOT-AUX BARRIER CS OR CP OR AUX 
	LINK NOT 0 A OR N)(0 Pass)(1 N)(NOT 1 TIME);
	## Vuoigatvuohta dearvvašvuođaveahkkái gusto riikka almmolaš ruhtaduvvon 
	## spesialistadearvvašvuođabálvalussii.

MAP (@PrcN>) TARGET PrfPrc IF (-1 Adv LINK *-1 COPULAS BARRIER NOT-ADV-PCLE LINK 
	*-1 Nom BARRIER V-NOT-AUX OR CC OR CLB OR CS OR CP)
	(*1 Nom BARRIER NPNHA LINK 1 NP-BORDER);
	## Ruonáeana lea čielgasit ráddjejuvvon geográfalaš guovlu.

MAP (@PrcN>) TARGET PrfPrc IF (NOT *-1 Neg BARRIER VERB)(NOT 0 AUX)(1C (N Nom));
	## Son lea oahppan olmmoš.

MAP (@PrcN>) TARGET PrfPrc IF (0 ("mannat"))(1 TIME);
	## Ii leat boahtán mannan váhkus.

MAP (@PrcN>) TARGET PrfPrc IF (-1 ("nu"))(0 ("gohčodit" Pass));
	## Dáin áššiin sáhttet Sámedikkit várra nu gohčoduvvon friija 
	## árvvoštallama mielde mearridit.

MAP (@PrcN>) TARGET PrfPrc IF (*-1 CC BARRIER NPNH LINK -1 Acc LINK NOT 0 PrfPrc)
	(1 (N Acc));
	## Divššohasas, gii dárbbaša guhkit áiggi ja oktiiheivehuvvon dearvvašvuođa-
	## bálvalusaid,...

MAP (@PrcN>) TARGET PrsPrc IF (*1 N BARRIER NOT-ADJ);
	## Boađe boahtti vahku.

MAP (@PrcN>) TARGET PrsPrc IF (1 CC)(2 Attr)(3 N);
	## Guoski dahje lagas eatnamat.

MAP (@ActioN>) TARGET (Actio Nom) IF (-1 Gen)(1 N)(NOT 1 PrfPrc);
	## Sii orro sin iežaset dahkan darfegođiid siste.
	## Sámegiela geavaheapmi journálain lea lassánan.
	## Bálvalusaid ovdánahttin riikkarájáid rastá.

 # Since we cannot at present distinguish between long and short Actios,
 # this rule hits not only the short Actio in the first example, which is 
 # correct, but also the long Actio in the second example, which is wrong.

 # Problem: At present (051106), the rule hits neither, which also makes it hard
 # to document the tag. It hits the third sentence, though, together with @GN>.

ADD (@TITLE) TARGET TITLE IF (1 Prop);
	## Stuorra giitu maiddái professor Birger Stuevold Lassenii.
 # ADD so that argument mappings can apply later.


MAP (@MEASURE) TARGET MEASURE IF (-1 Num)(1 ALU OR A);
	## Min viessu lea vihtta mehtara alu.
	## Sus lea njellje mehtara guhkkosaš biila.

MAP (@MEASURE) TARGET MEASURE IF (-1 Num)(1 Adv OR Pr);
	## Ádjá viessu lea guokte miilla bajábeal márkana.

MAP (@MEASURE) TARGET TIME IF (-1 Num)(1 ("boaris") OR ("nuorra"));
	## Mánná lea golbma jagi boaris.

MAP (@MEASURE) TARGET TIME IF (-1 Num)(1 ("maŋŋil"));
	## Dat dáhpáhuvai golbma jagi maŋŋil.

MAP (@MEASURE) TARGET N IF (-1 Num)(1 ("velggolaš"));
	## Ánde lea minnje vihttalot ruvnno velggolaš.

MAP (@MEASURE) TARGET (N Sg Acc) IF (-1 (Num Acc))(0 MEASURE)(1 (N Acc));
	##


# Genitive mappings
# -----------------

MAP (@GN>) TARGET (Pron Sg1 Gen) IF (1 (Refl PxSg1));
	## Orru lossat geardduhit mu iežan ávžžuhusa.

MAP (@GN>) TARGET (Pron Sg2 Gen) IF (1 (Refl PxSg2));
	## Diet lea du iežat ášši.

MAP (@GN>) TARGET (Pron Sg3 Gen) IF (1 (Refl PxSg3));
	## Mii han leat gullan dan su iežas njálmmis. 

MAP (@GN>) TARGET (N Sg Gen) IF (1 (Refl PxSg3));
	## Dát lea Hearrá iežas dahku.

MAP (@GN>) TARGET (Pron Du1 Gen) IF (1 (Refl PxDu1));
	##

MAP (@GN>) TARGET (Pron Du2 Gen) IF (1 (Refl PxDu2));
	##

MAP (@GN>) TARGET (Pron Du3 Gen) IF (1 (Refl PxDu3));
	##

MAP (@GN>) TARGET (Pron Pl1 Gen) IF (1 (Refl PxPl1));
	## Min iežamet posišuvdna lea nu ahte...

MAP (@GN>) TARGET (Pron Pl2 Gen) IF (1 (Refl PxPl2));
	## Bestojupmi ii leat din iežadet dahku.

MAP (@GN>) TARGET (Pron Pl3 Gen) IF (1 (Refl PxPl3));
	## Sii orro sin iežaset dahkan darfegođiid siste.
 
MAP (@GP>) TARGET Gen IF (NOT 0 Num LINK 1 Ill)
	(NOT 0 (V Actio) LINK 1 ("dihte") OR ("dihtii"))(1 Po);
	## Na mun gal hupmen dan birra.

MAP (@GP>) TARGET Gen IF (NOT 0 Cmpnd)(1 CC)(*2 Gen BARRIER NPNHA LINK 1 Po)
	(NOT 0 (V Actio) LINK 1 CC LINK 1 (V Actio) LINK 1 ("dihte") OR ("dihtii"));
	## Divššohas galgá maiddái oažžut dieđuid vejolaš riskkaid ja 
	## lassiváikkuhusaid birra.

MAP (@GP>) TARGET Gen IF (1 (Neg Foc))(2 Gen)(3 Po);
	## Ii leat váldojuvvon ovdan Ruoŧa iige Suoma bealde.

MAP (@GP>) TARGET Gen IF (0 TIME)(1 Num)(2 Po);
