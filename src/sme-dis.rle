LIST MOD-ASP = "dáidit" "fertet" "galgat" "gillet" "lávet" "sáhttit" "viggat" ;
# Verbs that never have arguments of their own - so we leave out "áigut", for ex.

SET AUX = COPULAS | Neg | MOD-ASP;

