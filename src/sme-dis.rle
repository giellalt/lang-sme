	## Seamma olmmoš áimmuhuššá máŋga dáin doaimmain dahje rollain.

SELECT (Pl Loc) IF (1 CC)(*2 Loc BARRIER NPNH LINK NOT 0 Com);
	## Son lea ođđa mediain ja skuvllas.

SELECT (Sg Com) IF (*-1 CC BARRIER NPNH LINK -1 Com LINK NOT 0 Loc);
	## Ii lean dárbu dábálaš eanavuovdimii, oktan opmodatregistreremiin
	## ja čuoldimiin.

SELECT (Sg Com) IF (1 CC)(*2 Com BARRIER NPNH LINK NOT 0 Loc);
	## Fága galgá ovddiidit ovttasbarggus Sámedikkiin ja sámi dearvvašvuođa-
	## ja sosiálabargiiguin.


# Essive
# ======

SELECT Ess IF (-1C Nom)(0 Nom);
	## Gáibadussan lea ahte olbmos lea sámegiella ruovttogiellan.

SELECT Ess IF (*-1 Acc BARRIER NPNHA LINK *-1 OPRED-VERB BARRIER NPNHA);
	## Máret anii Máhte jallan.

SELECT Ess IF (1 CC)(*2C Ess BARRIER NON-NP);
