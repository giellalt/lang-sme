SELECT Attr IF (0 ("oppalaš"))(*1 N BARRIER NPNH);
	## Sámi álbmoga bálvalusfálaldagaid dási oppalaš lokten eaktuda, ahte...

     # osku
     # - - -
REMOVE Actor IF (0 ("osku")); 

	# riŋget
	# - - - -
SELECT V IF (0 ("riŋga") LINK 0 Ill)(0 Ill)(NOT *-1 VERB BARRIER S-BOUNDARY LINK 0 FMAINV OR Inf)(NOT *1 VERB BARRIER S-BOUNDARY LINK 0 FMAINV OR Inf);
	## Sire riŋgii sutnje, go lei geargan bihtáid bargamis.
	
	# vašálaš
