# Subject and subject predicate
# -----------------------------

SELECT (@SUBJ) IF (0 (N Pl Nom))(*-1 COPULAS BARRIER NPNH)
	(*-1 V-PL3 BARRIER VFIN LINK *-1 (Sg Nom) BARRIER NON-ADV-PCLE
	LINK NOT *-1 CC BARRIER NPNH LINK -1 Nom); 
	## Erenoamáš ulbmiljoavku leat doavttergrádastudeanttat.
 
SELECT (@SUBJ) IF (*-1 CC BARRIER NPNH LINK -1 Nom)
	(*1 V-PL3 BARRIER NON-ADV-PCLE);
	## Muhto buođđogeažit ja giđđabuođut ráhkaduvvojit muđui seammá vugiid 
	## mielde.

SELECT (@SUBJ-QH) IF (0 Nom);
	## olbma čáppa, doaimmalaš nieidda livžo áhčiset riegádanbeaivve.

SELECT (@SPRED) IF (*-1 COPULAS BARRIER VERB OR CS OR CP OR go LINK *-1 Nom 
	BARRIER VERB OR CS OR CP)(NOT 0 Prop)(NOT 0 Actio LINK 1 N);
	## Dat lea oahpaheaddjin doppe.

SELECT (@SPRED) IF (*-1 COPULAS BARRIER NPNH OR COMMA LINK 0 PrfPrc 
	LINK *-1 ("leat") BARRIER NON-ADV-PCLE LINK *-1 
	Nom BARRIER VERB OR CS OR CP);
	## Arkelaus lei šaddan Judea gonagassan áhčis Herodesa maŋŋil.

SELECT (@SPRED) IF (*1 COPULAS BARRIER NON-ADV-PCLE OR CLB LINK *1 Prop OR Pers 
	BARRIER NON-ADV-PCLE OR CLB LINK 0 Nom);
	## Salmai riegadii Boas, gean eadni lei Rahab.

SELECT (@SPRED) IF (*-1 BOS OR REALCLB OR PUNCT BARRIER NON-CC)(0 A)
	(1 (Pers Nom));
	## Heittogat dat leat maiddái.
	## Ávdugasat sii geat moraštit.

SELECT (@SPRED) IF (*-1 (Pers Nom) BARRIER NPNHA OR CLB LINK *-1 COPULAS 
	BARRIER NON-ADV-PCLE)(NOT 0 Interr);
	## Leat go don árgi bajándálkkis?
 
SELECT (@SPRED) IF (*-1 ("leat" Sg3) BARRIER VFIN)(0 A);
	## Dutnje lea buoret ahte don massát ovtta lahtu.

SELECT (@SPRED) IF (*-1 COPULAS BARRIER NON-ADV-PCLE LINK 0 PrfPrc OR ConNeg LINK 
	*-1 Neg BARRIER NON-ADV-PCLE)(0 A);
	## Ii dál leat álkii bargui beassat.

SELECT (@SPRED) IF (0 (Sg Nom))(*1 COPULAS BARRIER NON-ADV-PCLE OR CLB LINK 0 (Pl3) 
	LINK *1 (Pl Nom) BARRIER NPNHA);
	## Erenoamáš ulbmiljoavku leat doavttergrádastudeanttat.
	
SELECT (@SPRED) IF (-1 ("mii" Pron Interr Sg Gen))(0 A);
	## Man guhkki lea skuvlii?
	## In dieđe man buorit dat leat.


# Verbal functions
# ----------------

SELECT (@+FAUXV) IF (*1 Inf OR PrfPrc OR (Actio Ess) BARRIER CS OR CP OR VFIN);
	## Máhtte áigu heaitit vuola juhkamis.

SELECT (@+FAUXV) IF (*1 ConNeg BARRIER CS OR CP OR Neg);
	## Allos dattege šatta nu go mun dáhtun, muhto nugo don dáhtut.

REMOVE (@+FAUXV) IF (NOT *1 Inf OR PrfPrc OR (Actio Ess) OR ConNeg BARRIER CS OR CP);
	## Maid Máret áiggui goarrumis?

SELECT (@-FAUXV) IF (*1 Inf OR PrfPrc OR (Actio Ess) BARRIER CS OR CP);
	## In beassan dal deatnojohtima oaidnit.

REMOVE (@-FAUXV) IF (0 (@-FMAINV));
	## Sápmelaččat ledje orron dáin guovlluin 1600-logu birrasiin. 


