	(NOT 1 CC LINK *1 Gen BARRIER NOT-NP LINK 1 Po)
	(NOT 1 COMMA LINK 1 Gen LINK 1 CC LINK 1 Gen LINK 1 Po)
	(NOT 0 Actio LINK 1 ("dihte") OR ("dihtii"));
