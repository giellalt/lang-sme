# Nominative predicatives
# -----------------------

REMOVE A IF (NOT *-1 COPULAS OR OPRED-VERB BARRIER V-NON-AUX)(0 N)(1 CLB)
	(NOT 2 A OR PrsPrc);
	## Ulbmil lea ovddidit guovddáža eamiálbmotdutkamiid guovddázin.
 # This rule has to follow identification of subject.

SELECT (Sg Nom) IF (*-1 COPULAS BARRIER VERB OR CLB OR CP OR CS OR PUNCT OR go OR 
	NP-HEAD-NOM LINK 0 Sg3)(0 NP-HEAD)(NOT 0 PrfPrc OR Actor OR (Prop Loc))
	(NOT 0 Loc LINK *-1 Nom BARRIER CS OR CP OR CLB OR PUNCT)
	(NOT 0 A LINK 0 Ill)(NOT 0 Adv LINK 1 Pr)(1C NP-BORDER); #(NOT 0 Ess)
	## Dasgo dus boahtá oaivámuš gii lea mu álbmoga Israela báimman.
	## Dat gal lei beare unnán. 

SELECT (Sg Nom) IF (*-1 COPULAS BARRIER NPNHA OR CC OR (Sg Nom))
	(*-1 V-SG3 BARRIER V-NON-AUX)(NOT 0 Loc)(*1C NP-BORDER BARRIER WORD);
	## Go šattai eahket, de...

SELECT (Pl Nom) IF (*-1 COPULAS BARRIER VERB OR CLB OR CP OR CS OR PUNCT OR go OR 
	NP-HEAD-NOM LINK 0 Pl3)(0 A OR NP-HEAD)(NOT 0 Inf)(1C NP-BORDER);
	## Vuollel 7% Finnmárkku eatnamiin leat vuovddit. 
	## Olmmošlassáneami geažil šadde eanebut geat galge juogadit buriid.

SELECT (Pl Nom) IF (*-1 COPULAS BARRIER VERB OR CLB OR CP OR CS OR PUNCT OR go OR 
	NP-HEAD-NOM LINK 0 Pl3)(0 A OR NP-HEAD)(NOT 0 Inf)(1 CLB)(2 NON-NP);
	## ...main leat mielde dušše priváhta oasálaččat, ii leat...

SELECT (Pl Nom) IF (-1 (Indef Attr) LINK *-1 COPULAS BARRIER NON-ADV-PCLE 
	LINK 0 Pl3)(1 CLB)(2 NON-NP OR CC);
	## ...seammás go nuppe dáfus leat veháš várrugasat, ja dadjet ahte...

SELECT Nom IF (*-1 COPULAS BARRIER NPNHA OR PrfPrc LINK *-1 MUN 
	BARRIER NON-ADV-PCLE)(*-1 V-SG1 BARRIER V-NON-AUX)(NOT 0 PrfPrc)
	(NOT 0 Loc LINK *-1 Nom BARRIER CS OR CP OR CLB OR PUNCT)
	(NOT 1 N OR A)(NOT *1 PrfPrc BARRIER VFIN OR CS);
	## Vaikko mun lean friddja in ge lean geange vuollásaš,...

SELECT Nom IF (*-1 COPULAS BARRIER NPNHA OR PrfPrc LINK *-1 DON 
	BARRIER NON-ADV-PCLE)(*-1 V-SG2 BARRIER V-NON-AUX)(NOT 0 PrfPrc)
	(NOT 0 Loc LINK *-1 Nom BARRIER CS OR CP OR CLB OR PUNCT)(NOT 1 N)
	(NOT *1 PrfPrc BARRIER VFIN OR CS);
	## Mii diehtit ahte don leat Israela oahpaheaddji.

SELECT Nom IF (*-1 COPULAS BARRIER V-NON-AUX OR CS OR CC)(NOT *-1 Nom BARRIER NPNHA)
	(NOT 0 VERB OR Adv OR Loc OR Ill OR Ess)
	(*1 CLB OR CS BARRIER NP-HEAD OR (A Nom));
	## ...maidda lágas eai leat sierra njuolggadusat, muhto...

SELECT Nom IF (-2 Nom)(-1 COPULAS)(0 N OR A)(NOT 0 PrfPrc OR (Prop Loc))
	(1 EOS OR ("go"));
	## ...ahte dat leat earalágan go eará joavkkut.
 # Hits here because "earalágan" does not agree?


# Nominative as objects in existential clauses
# --------------------------------------------
SELECT Nom IF (*-1 COPULAS BARRIER NPNHA OR Nom LINK *-1 Loc BARRIER N OR A OR 
	V-NON-AUX OR CLB OR CC OR CS OR CP)(NOT 0 PrfPrc)(1 NP-BORDER)
	(NOT 0 Attr OR PrsPrc LINK *1 N BARRIER NON-ADJ-CC)
	(NOT 0 Adv LINK *1 Nom BARRIER CC OR CLB OR CS OR CP);
	## Dat bulle ja goldne go dain ii lean ruohtas.

SELECT Nom IF (*-1 Loc BARRIER VERB OR CLB OR CC OR CP OR CS OR Nom 
	LINK *-1 COPULAS BARRIER VERB OR Nom OR CLB OR CC OR CP OR CS LINK
	NOT *-1 Nom BARRIER VERB OR CC OR CLB OR CS OR CP)(NOT 0 PrfPrc)
	(NOT 0 Adv LINK *1 Nom BARRIER CC OR CLB OR CS OR CP)
	(NOT 0 VFIN LINK -1 CLB)(1 NP-BORDER); 
	## Ovdal go kristtalašvuohta bođii Sápmái lei noaiddis erenoamáš
	## rolla oaidnilin ja dálkkodeaddjin.

REMOVE Acc IF (*-1 COPULAS BARRIER PrfPrc OR Inf OR (Actio Ess) OR VFIN 
	OR CC OR CLB OR CS OR CP)(1 CS OR EOS);
	## Dain áššiin lea Ruonáeatnama ruovttustivrras ráđđeaddi váldi. 

REMOVE Nom IF (NOT 0 Dem)(1C Nom)(NOT 1 PUNCT OR Num OR Interr)
	(NOT *-1 COPULAS BARRIER V-NON-AUX OR CS OR CP)(NOT *1 COPULAS BARRIER
	V-NON-AUX OR CS OR CP LINK NOT *1 V-NON-AUX BARRIER CS OR CP);
	## Divššohasjournála láhkaásahus.
	## ...maid buot journála gevaheaddji dearvvašvuođabargit áddejit.


SELECT (Pl Nom) IF (*-1 COPULAS BARRIER VERB OR CS OR CP OR go LINK 0 Pl3)(1 EOS);
	## Váikkuhusat divššohasa lagamuččaide leat mearkkašahtti stuorrát.
	 
SELECT (Pl Nom) IF  (*-1 COPULAS BARRIER NPNHAI OR PrfPrc)
	(*-1 V-DU-PL BARRIER V-NON-AUX OR CS)(*-1 PPRON-DU-PL BARRIER CS OR CP 
	LINK NOT *-1 (Pl Nom) BARRIER NON-ADV-PCLE);
	## Ehpet go dii leat ollu mávssolaččabut go dat?

SELECT (Pl Nom) IF  (*-1 COPULAS BARRIER NPNHAI OR PrfPrc)
	(*-1 V-DU-PL BARRIER V-NON-AUX OR CS)
	(*-1 (N Sg Nom) BARRIER CS LINK *-1 CC BARRIER NPNH LINK -1 (N Sg Nom));
	## Anne ja Biret leaba goappašagat nu jallat.


# Nominative in coordination
# --------------------------

SELECT Nom IF (*-1 CC BARRIER NPNHAI LINK -1 Nom LINK NOT 0 Acc 
	LINK NOT *-1 V-NON-AUX BARRIER NPNHA)(NOT 0 VFIN)(1C NP-BORDER);  
	## Divššohas dahje su ovddasteaddji gii oaivvilda ahte...

SELECT Nom IF (-2C HNOUN-NOM)(-1 CC)(NOT 0 Cmpnd OR Actor OR Adv)
	(NOT 0 Attr LINK 1 (N Nom))
	(NOT 0 PrfPrc LINK -1 CC LINK *-1 PrfPrc BARRIER VERB);
	## Eamiálbmogat ja riekti - muhtun prinsihpalaš vuolggasajit.

SELECT Nom IF (-2 (Pron Nom))(-1 CC);
	## Jesus Kristus ieš ja Ipmil.

SELECT Nom IF (-1 CC LINK *-1C (Num Nom) BARRIER NON-NA)(0 Num);
	## Journála divodeapmi ja sihkkun.
	## Joavkkus leat guokte Riikaoasselávdegotti ovddasteaddji ja 2 Sámedikkis.

SELECT Nom IF (-2C Nom)(-1 CC)(1 Gen LINK *1 Po BARRIER NPNH);
	## Dearvvašvuođaásahusas mearriduvvo divvun dahje sihkkun dearvvaš-
	## vuođabargiidlága § 44 mielde.

SELECT Nom IF (*-1 CC BARRIER NPNH LINK *-1 Nom BARRIER WORD LINK NOT 0 PrfPrc)
	(1 CS OR EOS OR (PUNCT RIGHT));
	##  ...Jáhkoba ja Jovssesa eadni, ja Salome. 

SELECT Nom IF (1 CC)(*2C Nom BARRIER NPNHAI LINK 1 NP-BORDER OR CLB);
	## 3) Oččodeapmi, diehtojuohkin ja ásahusovttasbargu.

SELECT Nom IF (NOT 0 Loc)(1 COMMA)(*2 NP-HEAD-NOM BARRIER NPNH LINK 1 CC LINK
	*1 NP-HEAD-NOM BARRIER NPNH LINK 1 CC OR COMMA OR NP-BORDER);
	## ...dikšu, dearvvašvuođabisuheaddji, veajuidahtti dahje dikšun-
	## ja fuolahanulbmilat.

SELECT Nom IF (1 COMMA)(*2C NP-HEAD-NOM BARRIER NPNH LINK 1 COMMA LINK
	*1 NP-HEAD-NOM BARRIER NPNH);
	## Romssa universitehtas lea sámi dutkan, gelbbolašvuođahuksen, 
	## oahpahus ja eará gaskkustepmi leamaš...

SELECT Nom IF (1 ("/"))(*2C Nom BARRIER NPNH);
SELECT Nom IF (-2C Nom)(-1 ("/"));
	## Vuoroheapmi lea: isit/eamit,...
	## Lea ráhkaduvvon diehtojuohkinmateriála/oččodanmateriála. 

SELECT Nom IF (*-1 COMMA BARRIER VFIN LINK -1C NP-HEAD-NOM)(1 COMMA)
	(*2 NP-HEAD-NOM BARRIER NPNH);
	## ..persovdnarievtti dábálaš láhkavuogádagat, bearašriekti, árberiekti,... 

SELECT Nom IF (1 COMMA)(*2 NP-HEAD-NOM BARRIER NPNH OR CC OR COMMA 
	LINK 1 EOS OR CP OR CS);
	## Jáhkobii riegádii Jovsset, Márjjá boadnja.

SELECT Nom IF (-2C Nom)(-1 COMMA)(1 CLB OR CS OR CP OR EOS);
	## Olusat dadjet munnje dan beaivvi: Hearrá, Hearra!

SELECT Nom IF (*-1 (V Pl3) BARRIER NPNH LINK NOT *-1 (Pl Nom) OR (Pl3 Nom) 
	BARRIER CS OR CP)(1 CC)(*2 Nom BARRIER NPNH);
	## ...maid liikká fertejedje leansmánni ja guokte albmá čujuhit.

SELECT Nom IF (*-1 COMMA BARRIER NPNHAI LINK -1C Nom)(1 CC);
	## ...geat leat divššohasa lagašolbmot, fuolaheaddji dahje veahke-
	## fuolaheddji.  

SELECT Nom IF (1 CC)(2C (Pron Nom))(3 (Pron Nom));
	## Gonagas Agrippa ja dii buohkat geat lehpet dás minguin!

SELECT Nom IF (1 CC)(2C N-NOM)(2 HNOUN);
	## Jos dát leažža dušše olbmuid dáhttu ja dahku,...

SELECT Nom IF (-2 Nom)(-1 (Neg Foc));
	## Eai ovdabarggut, teoria iige geavat doarjjo klausula áddejumi. 


# Not nominative
# --------------
REMOVE Nom IF (*-1 (Pron Nom) BARRIER COPULAS OR CLB OR CC OR CS OR CP
	LINK NOT 0 Interr)(NOT *-1 (Pron Attr) BARRIER NPNH)(0 N);
	## Ávdugasat sii geat duddjojit ráfi!

REMOVE Nom IF (*-1 V-NON-AUX BARRIER CLB OR CC OR CS OR CP LINK *-1 (Pron Nom)
	BARRIER CLB OR CC OR CS OR CP)(NOT *-1 PrsPrc BARRIER NPNH);
	## ...vai ollašuvašii dat mii lea celkojuvvon profehta Jesaja bokte.

REMOVE Nom IF (*-1 VFIN BARRIER NON-ADV-PCLE OR REALCLB LINK *-1C Nom 
	BARRIER NON-ADV-PCLE)(*1 V-NON-AUX BARRIER CLB OR CS OR CP OR go);
	## ...jus sierranas sivat eai eará cealkke.
	## Dii galgabehtet vuđolaččat bargat.

REMOVE Nom IF (*-1 Inf BARRIER VFIN OR PrfPrc OR CS OR CP OR REALCLB OR go LINK 
	0 V-NON-AUX)(NOT *1 VFIN BARRIER CS);
	## Universitehtii lea ain hastalussan jorgalit dárogielmateriála.

REMOVE Nom IF (*-1 BOS OR CS OR REALCLB BARRIER NON-NP)(NOT 1 Prop)
	(*1C Nom BARRIER NPNH OR CC OR CLB OR PUNCT LINK *1 VFIN BARRIER NON-NP)
	(NOT 1 (Interr Nom));
	## Romssa universitehta vuosttaš čielgadeapmi sámi dutkama birra dahkkui...

REMOVE Nom IF (*-1 COPULAS BARRIER NPNHA LINK *-1 Loc BARRIER NON-ADV-PCLE)
	(*1 Nom BARRIER NPNH OR CC OR PUNCT);
	## Sámedikkiin lea ráđđeaddi váldi buot sámi beroštumiide guoskevaš
	## áššiin.

SELECT Adv IF (NOT 0 Attr OR VFIN OR Inf OR VGen OR N OR Loc OR Ill)
	(*1 A OR N BARRIER NPNH)(NOT 1 CLB OR CC);
	## Farisealaččat ja erenoamážit sin čáláoahppavaččat nimmoredje.



# Accusative
