	# - - - - - -
SELECT (@APP-N<) IF (*-1 COMMA BARRIER APP-BOUNDARY LINK -1 (@SUBJ) OR (@SPRED))(1 COMMA OR EOS)(NOT *-1 ("sihke") OR ("nugo") LINK -1 VERB) ;
SELECT (@APP-Pron<) IF (*-1 COMMA BARRIER APP-BOUNDARY LINK -1 (@SUBJ) OR (@SPRED))(1 COMMA OR EOS)(NOT *-1 ("sihke") OR ("nugo") LINK -1 VERB) ;
	## Mu viellja, Tore, sáhttá boahtit deike.
	## Mon lean Will Turner, Sáppatbátti Turnera bárdni!

SELECT (@APP-Num<) IF (-1 MEASURE LINK -1 Num LINK *-1 V-TRANS-ACT-NOT-ACT BARRIER SV-BOUNDARY2) ;
	## Juohke heasta borrá sullii 6 kilu suinniid beaivái.
	

