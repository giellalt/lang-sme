SELECT Inf IF (*-2 ("leat") BARRIER VERB)(-1 Ess);
	## ...lea Sámedikkiin dáhpin geavahit... 

SELECT Inf IF (-1 ("leat"))(1 ("dego"));
	##

SELECT Inf IF (-1 ("mo"));
	## Dakkár doaimmat berrejit ráhkadit njuolggadusaid, mo gieđahallat
 	## journálaid.


# Infinitive after predicative
# - - - - - - - - - - - - - - -
 # There are two rules in the beginning, conflate when I see the effect.
