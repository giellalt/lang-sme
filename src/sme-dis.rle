REMOVE (Prop Pl Nom) IF (-1 BOS)(NOT *1 V-PL3 BARRIER CS);	     
	## Unnit nuppástusaiguin.	

REMOVE (Prop Pl Acc) IF (-1 BOS)(0 CNOUN);
	## Gunaid galgá leat várrogas atnit.

SELECT Prop IF (-1 BOS)(0 Loc)(1 COMMA)(2 MANNU)(3 Num) ;
SELECT Prop IF (-1 BOS)(0 Loc)(1 COMMA)(2 Num) ;
	## Deanus, borgemánu 31. b. 2005.
	
SELECT Prop IF (-1 BOS)(0 ("Sápmi" Prop))(1 ORGANIZATION);
