SELECT (A Sg Nom) IF (*-1 COPULAS BARRIER PrfPrc OR CS OR MO LINK 0 Sg1 OR Sg3)
	(NOT 0 N OR Inf)(NOT 1 VERB OR Num OR A); 
	## Lea ártet go it gallet fulkkiin.

SELECT (A Sg Nom) IF (*-1 COPULAS BARRIER VERB OR CS OR MO LINK *-1 (Neg Sg1) OR 
	(Neg Sg2) OR (Neg Sg3) BARRIER NON-ADV-PCLE)(NOT 0 N)(NOT 1 N OR A);
	## Ii leat vuogas geavahit hálduiváldima dálá fásta opmodagaide.

SELECT (A Pl Nom) IF (*-1 COPULAS BARRIER PrfPrc OR CS OR MO LINK 0 V-DU-PL)
	(NOT 0 N OR Inf)(NOT 1 A OR VERB); 
	## Mánnáguoddi-spagáin leat spahká-oaivvit sakka guhkibut.
	## Dán oktavuod1as leat dolos1 geavahusa njuolggadusat miellagiddevac1c1at.

SELECT (A Pl Nom) IF (*-1 COPULAS BARRIER VERB OR CS OR MO LINK *-1 (V Neg) 
	BARRIER NON-ADV-PCLE LINK 0 V-DU-PL)(NOT 0 N)(NOT 1 VERB OR A); #(NOT 1 N);
	## Sis-Finnmárkku dikkit man1n1ilgo Norga badjelasas válddii sorjjas-
	## meahttunvuod1a dán guovllus, eai s1at lean seamma dehálac1c1at 
	## sápmelac1c1aid gaskkas go ovdal.


REMOVE VFIN IF (*-1 COPULAS BARRIER NON-ADV-INDEF)(0 Attr)(1 N OR EOS);
	## Joddu suhppejuvvo go doaris lea gárvvis.	

SELECT A IF (*-1 COPULAS BARRIER NON-ADV-INDEF)(NOT 0 VERB OR Ord OR N)(1 N OR EOS); 
	## ...de dilli orru leamen earalágan.

SELECT A IF (-2 (N Sg Nom))(-1 ("leat" Sg3))(NOT 0 N OR PrfPrc OR Inf)
	(NOT 0 Ill LINK 1 PrfPrc)(NOT 0 Adv LINK 1 A); 
	## Sin váilevas1 oahppu ja giellamáhttu lea guhkit áiggi leamas1
	## head1us1teaddjin. 	

REMOVE Attr IF (0 A)(1 COMMA)(NOT 2 A OR PrsPrc);
	## Jos dat ii leat vejolas1, de mii fertet mannat.

REMOVE Attr IF (0 A)(NOT 1 A OR N OR Actio OR CC OR COMMA); 
	## Dat galgá c1uovvoluvvot nu guhkás go vejolas1.

REMOVE Attr IF (0 A-CASE OR Adv)(1 CC)(NOT *2 N BARRIER NP-BORDER);
	## ...berre mearridanváldi leat nu viiddis go vejolas1 ja nu
	## c1adni go vejolas1.

REMOVE (A Gen) IF (*-1 COPULAS BARRIER NON-ADV-PCLE)(1 Inf); 
	## Lea hávski oaz1z1ut reivve.


SELECT (A Sg Nom) IF (*-1 COPULAS BARRIER NON-ADV LINK 0 V-SG3)(NOT -1 Gen)
	(NOT 0 Inf OR Attr)(NOT 0 Adv LINK *1 VERB BARRIER NON-ADV)(NOT 1 PrfPrc);
	## Buod1d1u lea dál gárvvis.
	## Dalle lea lunddolas1, ahte...

SELECT (A Pl Nom) IF (*-1 COPULAS BARRIER NON-ADV LINK 0 V-PL3)(NOT 0 N)
	(NOT 1 PrfPrc OR ("go" Pcle));
	## Died1ut mat leat dárbbas1lac1c1at deavdin dihtii vánhenovddasvástadusa...
	## Álgohálduiváldin galgá leat gárz1z1it go geavahus dolos1 áiggi rájes.

 # Verbs are disambiguated later - "leat" in the last example is an infinitive. 
 # But the last part of the rule fixes the last example. 


SELECT (A Pl Nom) IF (-1 (N Pl Nom))(*-2 COPULAS BARRIER NON-ADV-PCLE LINK 0 Pl3)
	(NOT 1 PrfPrc);
	## ...vai leago eanavuovdinorgánat geatnegasat luobahit eatnamiid...


