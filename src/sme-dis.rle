# eanaš/eanas
# - - - - - - -
SELECT Adv IF (*-1 NP-HEAD-NOM BARRIER CLB OR CS OR CP)
	(0 ("eanas") OR ("eanaš"))(NOT *1 N BARRIER NPNH);
	## Muhto báinnan dat gal eanaš lea.

SELECT Adv IF (*-1 COPULAS BARRIER NON-ADV-PCLE LINK *-1 NP-HEAD-NOM BARRIER 
	NON-ADV-PCLE)(0 ("eanaš") OR ("eanas"))(NOT 1 N LINK 1 N);
	## Áššemeannudeapmi lea eanaš dárogillii.
	## Sámegiella lea eanaš oktavuođain unnitlogugiella. (NOT 1 N LINK 1 N)!

SELECT Adv IF (*-1 NP-HEAD-NOM BARRIER NON-ADV-PCLE LINK NOT 0 PrfPrc 
	LINK *-1 COPULAS BARRIER NON-ADV-PCLE)(0 ("eanaš") OR ("eanas"));
	## Oahpahusossodagas lea áššemeannudeapmi eanaš dárogillii.

SELECT Adv IF (-1 COPULAS)(0 ("eanas") OR ("eanaš"))(1 N)(NOT 1 TIME)
	(*2 CS OR EOS BARRIER N);  
	## Dál lea eanaš ovttamielalasvuohta, ahte...
	## Ovdal ledje eanas vuonaid bivdán, muhto go skoavdefatnasat bohte... 
	## Ealus lea eanaš álddut.

SELECT Adv IF (0 ("eanaš") OR ("eanas"))(1 PrfPrc);
	## Nuppádassii leat čielga gáibadusat eaktodáhtolaš oskui, mii 
	## guoská giddodahkii, eanaš vuođđoduvvon registrerema oktavuođas. 

