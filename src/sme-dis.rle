SELECT Gen IF (-1 BOS OR CS)(1 CC)(2 NP-HEAD-GEN LINK *1 (N Nom) BARRIER NPNH
	LINK NOT 1 Loc);
SELECT Gen IF (1 CC)(*2C NP-HEAD-GEN BARRIER NON-ADJ LINK 1 Po OR PREGEN);
