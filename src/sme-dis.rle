SELECT Loc IF (NOT *-1 (Gen @>N) BARRIER S-BOUNDARY2)(0 (Sg Com))(1 XGO BARRIER NOT-COMMA LINK *1 Loc BARRIER NPNH LINK 1 COMMA)(NOT *1 VFIN OR AUX BARRIER SV-BOUNDARY2);
	# Those rules are locative specific. They choose a locative if no other coordination candidates are existent, which means no genitive modifiers, no other arguments or adjuncts. The (NOT *-1 (@>N) constraints is supposed to exclude genitive modifier coordination.

SELECT Com IF (NOT *-1 (Gen @>N) BARRIER S-BOUNDARY2)(0 (Pl Loc))(1 SEAMMAX LINK NOT 0 Qst BARRIER NOT-COMMA LINK 1 ("go") OR ("dego") OR ("dugo") LINK *1 Com BARRIER NPNH LINK 1 COMMA) ;

SELECT Com IF (NOT *-1 (Gen @>N) BARRIER S-BOUNDARY2)(0 (Pl Loc))(1 XGO BARRIER NOT-COMMA LINK *1 Com BARRIER NPNH LINK 1 COMMA);

