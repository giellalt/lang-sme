SELECT Nom IF (*-1 CRD BARRIER NPNH LINK *-1 Nom BARRIER WORD LINK NOT 0 PrfPrc)(1 CS OR EOS OR (PUNCT RIGHT))(NOT 0 Inf LINK -1 CC LINK -1 Inf);
