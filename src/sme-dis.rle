# maid
# - - -
SELECT Adv IF (-1 (Pron Rel))(NOT -1 (Pron Rel Pl Acc));
    ## ... mat maid leat politihkalaš bellodagat.

SELECT Adv IF (-1 INF-VERB)(0 ("maid"))(*1 Inf BARRIER CS OR CP);
	## Mun háliidan maid mannat.

SELECT Adv IF (-1 ("leat"))(0 ("maid"))(*1 PrfPrc BARRIER ("leat") OR CS OR CP);
	## Lea maid lassin gávnnahan metodalaččat váttisin mearridit...

SELECT Adv IF (-1 ("leat"))(0 ("maid"))(*1 (A Nom) BARRIER NOT-ADV);
	## Geainnut ledje maid guhkit rabas.

SELECT Adv IF (-1 N LINK *-1 BOS OR CS OR (":" CLB) BARRIER NPNHA)(0 ("maid"))
	(NOT *1 VFIN LINK *1C VFIN BARRIER CS OR CP);
	## Suomas maid lei dilli váttis.
	## Nubbi maid fuobmáimet lei ahte... (Interr, hence the last condition.) 

SELECT Adv IF (*-1 V-PL3 BARRIER NOT-ADV-PCLE)(0 ("maid"))(*1 NP-HEAD-PL-NOM 
	BARRIER NPNHA LINK NOT *1 V-PL3 BARRIER NOT-ADV);
	## Riggodagaide gullet maid fuođđot ja lottit.

SELECT (Rel Sg) IF (-1 ("buot"))(0 ("maid"));
    ## Buot maid dii dahkabehtet, lea buorre.

REMOVE Adv  IF (0 ("maid"))(*1 VFIN BARRIER CS OR CP LINK *1 COMMA 
	BARRIER CS OR CP LINK 1 VFIN)(NOT -1 (N Pl) LINK *2 V-PL3 BARRIER NOT-ADV);
REMOVE Interj  IF (0 ("maid"))(*1 VFIN BARRIER CS OR CP LINK *1 COMMA 
	BARRIER CS OR CP LINK 1 VFIN)(NOT -1 (N Pl) LINK *2 V-PL3 BARRIER NOT-ADV);
	## Dan maid dalle dárbbaša fievrridit, ferte olmmoš noađuhit.

SELECT Rel IF (*-1 VFIN BARRIER CS OR CP LINK 0C VERB)(0 ("maid"))
	(*1 VFIN BARRIER CLB OR CS OR CP)
	(NOT *1 Inf BARRIER VERB OR Nom LINK *-1 INF-VERB BARRIER VERB)
	(NOT -1 ("leat" Pl3) LINK *2 (N Pl Nom) BARRIER NPNHA);
	## Don galggat doallat dan maid leat vurdnon Herrái.

SELECT Adv IF (-2 BOS OR CS OR (":" CLB))(-1 (Pron Pers) OR N)(0 ("maid"))
	(NOT *1 VFIN LINK *1C VFIN BARRIER CS OR CP);
	## Sidjide maid lei Ruija.

SELECT Adv IF (-1 COPULAS LINK 0 Pl3)(0 ("maid"))(*1 (N Pl Nom) BARRIER NPNHA);
SELECT Adv IF (-1 Loc LINK *-1 COPULAS BARRIER NPNHA OR VERB)(0 ("maid"))
	(*1 (N Nom) BARRIER NPNHA); 
	## Romssa universitehtas leat maid eará plánat álggahit fálaldagaid.
	## Leago stáhtas maid geatnegasvuohta doarjut doalahit dán kultuvrra.

SELECT Adv IF (*-1 CS BARRIER VFIN OR CLB)(NOT -1 ("go"))(0 ("maid"))
	(*1 VFIN LINK NOT *1 VFIN);
	##... nu ahte dat maid siskkilda ávnnalaš kultureavttuid.    

SELECT (Pron Interr) IF (-1 BOS OR PUNCT)(1 Nom)(*1 ("?"));

REMOVE Interj IF (1 CC)(2 (Pron Interr));
REMOVE Adv IF (1 CC)(2 (Pron Interr));
 	## Maid mii dál vihtaniiguin?
	## Lea registreren maid ja man ollu lea borran. 

SELECT Adv IF (0 ("maid"))(NOT *1 VFIN);
SELECT Adv IF (0 ("maid"))(NOT *1 VFIN BARRIER CC OR CS OR CP);
	## Dán oktavuođas čujuhuvvo maid oanáduslistui ja gáldolistui.
	## Sápmelaččat oasálaste maid dasa, muhto lassin ledje sis vel eará 
	## resursat.

SELECT Adv IF (*-1 INF-VERB BARRIER VERB)(0 ("maid"))(*1 Inf BARRIER VERB);
	## Sápmelačcat galge maid vearu máksit Danmárku-Norgii. 
 
SELECT Adv IF (*-1 (V Sg3) BARRIER NOT-ADV-PCLE LINK NOT *-1 Nom BARRIER CS)
