SELECT PrfPrc IF (*-1 REALCOPULAS BARRIER SV-BOUNDARY2 OR CC LINK NOT 0 PrfPrc LINK 0 VFIN)(NOT 0 Actio LINK *1 PrfPrc BARRIER NOT-ADV-PCLE LINK NOT 0 (PrfPrc @>N))(NOT 0 Ess LINK 1 (A Nom) OR PrfPrc)(NOT 0 ESS-ADVL-N)(NOT 1 ("dihte") OR ("dihtii"))(NOT 1 COMMA OR CC LINK *1 Actio BARRIER NPNH)(NOT 1 ("veara"));
