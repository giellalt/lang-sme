SELECT Loc IF (NOT *-1 COM-HEAD BARRIER S-BOUNDARY2)(0 ORGANIZATION LINK *-1 HUMAN BARRIER SV-BOUNDARY2 OR COMMA)(NOT *1 COM-HEAD BARRIER SV-BOUNDARY2);
