SELECT Loc IF (*-1 BOS OR CS OR REALCLB BARRIER NPNHAI)(NOT 0 (A Nom))
	(NOT 0 (N Num Nom) LINK *1 (V Du3) BARRIER NOT-ADV-PCLE)
	(*1 ("leat") BARRIER (@SUBJ) OR (@OBJ) OR (@SPRED) OR (@OPRED) );
#	(*1 ("leat") BARRIER NOT-ADV-PCLE-NEG ); # Check this rule with the positive barrier.
