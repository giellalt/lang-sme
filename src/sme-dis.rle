REMOVE Prop IF (0 ("sápmi" Gen))(1 ("plána") OR ("doaibma#plána") OR ("perspektiiva") OR ("mánná") OR ("álbmot") OR ("bargi") OR ("geavaheaddji") OR ("kultuvra") OR ("servodat"));
