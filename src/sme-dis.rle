	## iskkadeami ruhtadeapmi ja Sámi dearvvašvuođadutkanguovddáža 
	## doaimmaid viiddideapmi váldojit ovdan 2002 bušeahta oktavuođas.

REMOVE PrfPrc IF (1 CC)(2 Actio)(3 ("veara"));
	## ...buot dan mii lea ráhkistan ja gudnejahttin veara.

	# Verbal versus nominal actio
	# - - - - - - - - - - - - - -

SELECT (V Actio Loc) IF (*-1 ("ádjánit") OR ("beassat") OR ("biehttalit") OR ("dolkat") OR ("eastadit") OR ("geargat") OR ("gieldit") OR ("heaitit") OR ("hehttet") OR ("jaska") OR ("máššat") OR ("váibat") OR ("vajálduhttit") BARRIER VERB);
