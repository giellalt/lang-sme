# Special rules
# =============

# Accusative or genitive in front of ALU and in front of adjectives
# -----------------------------------------------------------------

SELECT Acc IF (0 NUMERALS)(1 MEASURE)(2 ALU OR A);
	## Visti lei vihtta mehter alu.

SELECT Acc IF (-1 (Num Acc))(0 MEASURE)(1 ALU OR A);
	## Min viessu lea vihtta mehtara alu.
	## Sus lea njeallje mehtara guhkkosaš biila.

SELECT Acc IF (-1 (Num Acc))(0 MEASURE)(1 Pr);
	## Ádjá viessu lea guokte miilla bajábeal márkana.

SELECT Acc IF (-1 (Num Acc))(0 TIME)(1 ("boaris"));
	## Mánná lea golbma jagi boaris.

SELECT Gen IF (-1 (Num Gen))(0 MEASURE)(1 A OR ALU);
	## Visti lei viđa mehter alu.
	## Dohko manai viđa mehter govdosaš geaidnu.

SELECT Gen IF (-1 (Num Gen))(0 TIME)(1 ("boaris"));
	## Son lea viđa jagi boaris.

SELECT Acc IF (-1 (Num Acc))(0 TIME)(1 ("maŋŋil"));
	## Dat dáhpáhuvai golbma jagi maŋŋil.

SELECT Acc IF (0 Num)(1 (N Acc))(2 ("velggolaš"));
SELECT Acc IF (-1 Num)(1 ("velggolaš"));
	## Ánde lea munnje vihttalot ruvnno velggolaš.

SELECT Gen IF (0 ("okta"))(1 ALU);
	## Dábálaččat leat joddut sullii ovtta guhku.

SELECT Acc IF (0 Num)(1 ("gitta"))(2 (Num Acc));
	## Dát ledje gáddenuohtit mat ledje 180 gitta 200 sala guhku, ja 15 gitta 
	## 20 sala čieŋu.


# Locative, not PxSg3
# -------------------

SELECT Loc IF (NOT *-1 Sg3)(0 PxSg3);
	## Mun borren áhkus luossaláibbi.


# Genitive
# ========
 # Since disambiguation between accusative and genitive is very difficult, 
 # let us pick out as many genitives as possible here.

# Various genitive rules
# ----------------------

# Genitive after quantifier
# - - - - - - - - - - - - - 
SELECT Gen IF (*-1C (Num Nom) BARRIER NPNH OR Gen OR CC OR PUNCT OR REALCLB 
	LINK NOT 0 ("okta") OR Ord)(NOT 0 VERB OR ("nr") OR ("nummar") OR ("s"));
	## Golbma čáppa, doaimmalaš nieidda livžo áhčiset riegádanbeaivve.

SELECT Gen IF (*-1 (Num Nom) BARRIER NPNH OR Gen OR PUNCT OR REALCLB LINK 
	NOT 0 ("okta") OR Ord LINK *-1 ("leat") BARRIER NOT-ADV-PCLE)
	(NOT *1 V-TRANS-ACT BARRIER CLB OR CC OR CS OR CP);
	## Joavkkus leat vihtta oahppi Kárášjogas.

SELECT Gen IF (*-1 (Num Nom) BARRIER NPNH OR Gen OR PUNCT OR REALCLB LINK 
	NOT 0 ("okta") OR Ord LINK -1 CC OR CLB LINK *-1 (Num Nom) BARRIER NOT-NP)
	(NOT 0 Num LINK -1 CC OR CLB);
	## Joavkkus leat vihtta oahppi Kárášjogas, vihtta oahppi Muosáhis ja 
	## vihtta Bearalvágis.
 
SELECT Gen IF (-1 CC)(-2 Cmpnd)(*-3 NUMERALS BARRIER NPNH OR PUNCT LINK 0 Nom);
	## Máŋga sámi dearvvašvuođa- ja sosiálabargi vásihit dan. 

SELECT Gen IF (*-1 (Num Nom) BARRIER NOT-NP OR CLB)(NOT -1 CC)
	(*1 V-PL3 BARRIER NOT-ADV-PCLE);
	## Máŋga Romssa universitehta dutkanbirrasa leat ásahan ovttasbargguid...

SELECT Gen IF (*-1 NUMERALS BARRIER NPNH OR CC OR CLB OR PUNCT OR ("ruvdnu") LINK
	0 Nom LINK NOT -1 ABBR OR CLB)
	(NOT *-1 (Num Acc) BARRIER NPNH LINK *-1 Nom BARRIER CC OR CLB OR CS OR CP)
	(NOT *-1 V-TRANS-ACT BARRIER VERB OR CS OR CP OR CC OR CLB)
	(*1 Gen BARRIER NPNH);
	## Máŋga Romssa universitehta dutkanbirrasa leat ásahan ovttasbargguid...


# Genitive in front of postposition
# - - - - - - - - - - - - - - - - - 
SELECT Gen IF (NOT 0 Adv OR PrfPrc)(1C Po)(NOT 1 Px);    
SELECT Gen IF (NOT -1 ("§"))(0 Num)(*1 (N Gen) BARRIER NPNH LINK 1 Po);
	## Lávdegottiid čoahkkimat dollojit giddejuvvon uvssaid duohken.
	## Divššohasas lea vuoigatvuohta oažžut dearvvašvuođadilis árvvo-
	## štallojuvvot 30 bargobeaivvi sisa.

SELECT Gen IF (*-1 BOS BARRIER NOT-CC)(1 (Po Qst));
	## Girdi mieldego don manat?

SELECT Gen IF (*-1 BOS BARRIER NOT-CC)(1 Po)(2 go);
	## Girdi mielde go don manat?

SELECT Gen IF (1 CC)(*2 Gen BARRIER NPNH LINK 1 GASKAL);
	## Juos fágabargi ja geavaheaddji gaskka leat dasa lassin gielalaš
	## dahje kultuvrralaš gulahallanváttisvuođat,...

SELECT (V Actio Gen) IF (1 ("dihte" Po) OR ("dihtii"))(*1 (N Acc) BARRIER NOT-NP);
	## Vuovdon earit máhcahovvojit sámi guovlluide nannen dihte báhcán doaluid.

SELECT (V Actio Gen) IF (1 CC)(2 (V Actio Gen))(3 ("dihte" Po) OR ("dihtii"));
	## ...seailluhan, nannen ja ovddidan dihte sámi servodaga.

SELECT Gen IF (0 Actio)(1C Po);
	## Válddán dan ášši guorahallan vuollái.

SELECT Gen IF (*-1 Po OR PREGEN BARRIER NOT-NP OR PUNCT OR REALCLB)
	(NOT 1 COMMA LINK 1 Gen LINK NOT *1 CC BARRIER Po)(NOT 1 Pers)
	(*1 Po OR PREGEN BARRIER NOT-NP OR REALCLB);
	## ...dan vuolde sisdoalu birra divššohasjournálain, čállima, divodeami,
	## sihkkuma, vurkema, sirdima, journála beassanvejolašvuođa ja dušša-
	## deami birra.

SELECT Gen IF (1 (PUNCT LEFT))(*2 (PUNCT RIGHT) LINK 1 Po);
	## Dearvvašvuođaregisttarláhka lea sierraláhka heaggadieđuidlága (cuoŋo-
	## mánu 14. b. 2000 láhka nr 31 heaggadieđuid gieđahallama birra) ektui.

SELECT Gen IF (1 CC)(*2 NP-HEAD-GEN BARRIER NPNHA LINK 1 Po);
	## Seammás áiggun maiddái ovdanbuktit dihto reflekšuvnnaid sihke eaŋkil 
	## rievttálaš geavatlaš gažaldagaid ja eambbo prinsihpalaš gažaldagaid hárrái.


# Genitive after preposition
# - - - - - - - - - - - - - -
SELECT Gen IF (*-1 Pr BARRIER NPNH OR REALCLB OR CONTRA)(NOT 0 VFIN)
	(1 NP-BORDER OR CC OR COMMA OR ("ieš")); 
	## Earret mu ii giige lea boahtán.
	## Sáhka su birra viidánii miehtá Syria, ja ...
	## ...gaskal vejolaš ja dohkálaš iskkadan- ja dikšunvugiid.
	## ...ja vulggii rastá jávrri iežas gávpogii. 

SELECT Gen IF (*-1 Pr BARRIER NOT-NP OR Po)(*1 Interr BARRIER WORD);
	## Sáhttá dadjat, ahte jagi 1978 láhka balansere gaskkal sámi 
	## vuoigatvuođaid boarásmuvvan oainnu, mii lei ráđđejeaddjin 
	## "vigihis ávkkástallanrievtti áigodagas", ja eanet ođđaáigásat 
	## vuoigatvuođaid dovddasteaddji oainnu.

SELECT Gen IF (-1 Pr)(0 Num);
	## Jus divššohas lea badjel 16 jagi boaris... 

SELECT Gen IF (-3 GASKAL)(-2 (Num Gen))(-1 CC)(0 Num);
	## Jus divššohas lea gaskal 16 ja 18 jagi boaris,...


# Genitive inside a two-part Po
# - - - - - - - - - - - - - - -
SELECT Gen IF (-1 POST-A)(1 N)(2 POST-B);
	## Nu leat 40 buolvva Dávveda rájes Babylonia fáŋgavuođa rádjái.


# Genitive attribute of adposition.
# - - - - - - - - - - - - - - - - -
SELECT Gen IF (1 ("bealde" Pr) OR ("beale" Pr) OR ("beallái" Pr) OR ("geahčai" Pr)
	OR ("geahčen" Pr) OR ("geaže" Pr));
	## Ánde boahtá don bealde joga.


# Genitive in front of "guvlui"
# - - - - - - - - - - - - - - -
SELECT Gen IF (NOT 0 Dem OR Loc)(1 ("guovlu" N Sg Ill));
	## Áddjá lei nohkkan, láse guvlui oaivi.


# Genitive in front of "láhkái"
# - - - - - - - - - - - - - - - 
SELECT (Actio Gen) IF (1 ("láhki" N Sg Ill));
	## ...go dearvvašvuođabargit iežaset árvvoštallama mielde gávnnahit...
	## Dat ráhkaduvvo gáttis gárvvisin ja gessojuvvo vatnasii suohppun
	## láhkái.

SELECT (N Gen) IF (1 ("láhki" N Sg Ill));
	## Dat lea áhčis láhkai.


# Genitive with "dan mađe N"
# - - - - - - - - - - - - -
SELECT (Gen Sg) IF (0 ("dat"))(2 ("mađe"));


# Genitive with "veara"
# - - - - - - - - - - -
SELECT Gen IF (NOT 0 Nom)(*1 COPULAS BARRIER V-NOT-AUX OR CS 
	LINK *1 ("veara") BARRIER VERB OR CS);
	## Min ii leat veara dohko vuolgit.
	## Dasgo bargi lea bálkkás veara. # NOT 0 Nom!


# Genitive in comparison
# ----------------------
SELECT Gen IF (NOT *-1 V-TRANS-ACT BARRIER NOT-ADV-PCLE)(NOT 0 VERB)(1 (A Comp))
	(NOT 1 VERB OR Adv)(NOT 2 go);
	## Mun jáhkán du iežan falibun.

	
# Genitive relative pronouns
# - - - - - - - - - - - - - -
SELECT (Pron Interr Gen) IF (NOT 0 Indef OR ("mii"))(1 N)
	(*1 COPULAS BARRIER CLB)(NOT *1 V-TRANS-ACT BARRIER CLB);
        ## ...ja gii galgá árvvoštallat guđe dieđut galget leat journálas.
	## ...ja guđe árvvoštallamiid vuođul doaibmabidju lea álggahuvvon.

SELECT (Pron Interr Gen) IF (0 ("mii"))(1 (N Nom))
	(*1 COPULAS BARRIER CLB)(NOT *1 V-TRANS-ACT BARRIER CLB);
 	## Jerusalemis lea láttu man namma hebreagillii les Betesda.


# Genitive indefinite pronouns
# - - - - - - - - - - - - - - -
SELECT Gen IF (0 Indef)(1 (N Ill) OR (N Loc) OR (N Com));
	## De galget ollu dearvvašvuođabargit iešguđet fidnojoavkkuin dan
	## geavahit. 


# Genitive in proper name sequences
# - - - - - - - - - - - - - - - - - 
REMOVE Acc IF (NOT *-1 V-TRANS-ACT BARRIER NOT-ADV-PCLE)(0 Prop)(1 Prop)
	(NOT 1 ADVLCASE);          
	## Dat lea Norgga Röros ja Máhte Piera.

# The idea behind the following rule pair is to that in a (Spec + N) 
# sequence, the Prop is the possessor rather than the other way around.
# XXX these two rules should be generalised to star notation.
REMOVE Acc IF (*-1 V-TRANS-ACT BARRIER NOT-ADV-PCLE)(0 Prop)(*1 (N Acc) BARRIER NPNH);#(NOT *1 Po BARRIER N);
	## Mun oainnán Máhte beatnaga Máreha biilla duohken.
	## Mun oainnán kánske Máhte beatnaga Máreha biilla duohken.
	## Son oaidná min Máhte biilla duohken.
	#  0 = Máhte

#REMOVE Acc IF (*-1 V-TRANS-ACT BARRIER NOT-ADV-PCLE)(0 Prop)(1 Gen LINK 1 Po BARRIER N);
	## Mun oainnán Máhte beatnaga Máreha biilla duohken.




# Genitive after "mii" and "mihkkege"
# - - - - - - - - - - - - - - - - - -
SELECT (N Pl Gen) IF (-1 ("mii") OR ("gii"))(1 NP-BORDER)(*1 ("?")); 
	## Maid ipmašiid doppe dagat?
 # Embedded questions are also possible, but harder to formulate a rule. 

SELECT (N Pl Gen) IF (-1 ("mihkkege"));
	## Iige njuovvan ja bierggu rádjan leat mihkkege dearvvašvuođa-
	## váttisvuođaid.


# Genitive subjects of non-finite verbs
# - - - - - - - - - - - - - - - - - - -
REMOVE Gen IF (*-1 CP OR CS)(0 TV)(1 Actio)(NOT 1 Ess OR Com)(NOT *1 VFIN BARRIER CP OR CS);
    ## Ii leat šiitimis ahte Stuorradiggi dattetge dán stuorradiggeáigodagas
    ## oažžu gieđahallamii máŋga ášši main lea stuorra mearkkašupmi sámiide.
    # Has to stand in front of the next rule (SELECT Gen)

SELECT Gen IF (NOT *-1 ("leat") BARRIER CS)(NOT 0 Interr OR Dem)(1 Actio)
	(NOT 1 Ess OR Com)(NOT 1 (V Actio Loc))
	(NOT 1 PrfPrc LINK *1 NP-HEAD-ACC BARRIER NPNH)(NOT 2 Po);
	## Eatni dahkan láibi lei buorre.

SELECT Gen IF (0 Interr)(1 (Actio Gen))(2 N);
	## Gean ráhkadan fanas dat lea?

SELECT Gen IF (*-1 ("leat") BARRIER NPNHA)(1 (V Actio Loc));
	## Dat ii leat geafes olbmo nagodeames. 

SELECT Gen IF (*-1 (@OBJ) BARRIER VERB LINK *-1 V-TRANS-ACT BARRIER NPNHA)
	(1 Ger);
	## Son muitalii dan buot olbmuid guladettiin.


# Accusative after actio (exceptions to next rule)
# - - - - - - - - - - - - - - - - - - - - - - - - 
SELECT Acc IF (*-1 ("dihte") OR ("dihtii") BARRIER NPNH LINK -1 Actio)
	(1 NP-BORDER);
	## ... diehtin dihtii iežas dili ja veahki sisdoalu.

SELECT Acc IF (*-1C Actio BARRIER HNOUN-NOM OR (Pron Nom) OR VERB OR CC OR CS OR CP 
	LINK NOT 0 Cmpnd OR Gen OR IV)(NOT -1 CC LINK -1 Actio LINK NOT 0 Cmpnd)
	(NOT 0 TIME OR ROUTE OR Loc)(NOT 0 Num LINK -1 ("§"))(1 NP-BORDER)(NOT 1 N); 
	## ...dahje biehttalit botkemis nealgudeami mii lea jođus. 


# Accusative after "vuoi" (exceptions to REMOVE Acc-rule below)
# - - - - - - - - - - - - - - - - - - - - - - - - -
SELECT Acc IF (*-1 ("vuoi") BARRIER NPNH)
	(NOT 0 TIME LINK *-1 (N Acc) BARRIER ("vuoi"))
	(NOT *1 N BARRIER NPNH OR CC OR CLB);
	## Vuoi du, Betsaida!

SELECT Acc IF (*-1 ("vuoi") BARRIER NOT-NP OR CLB OR CP)
	(NOT 0 TIME LINK *-1 (N Acc) BARRIER ("vuoi"))(1 NP-BORDER);
	## Vuoi dán máilmmi ja dan fillehusaid!
 
SELECT Acc IF (*-1 ("vuoi") BARRIER NOT-NP)(0 Dem);
	## Vuoi surgat dan gándda!

SELECT Acc IF (*-1 CC BARRIER NPNH LINK -1 Acc LINK *-1 ("vuoi") BARRIER NOT-NP)
	(NOT 0 Dem OR Pers);
	## Vuoi áhpehemiid ja njamaheddjiid daid beivviid! 

SELECT Acc IF (*-1 Pers BARRIER NPNH OR CLB LINK *-1 ("vuoi") BARRIER NOT-ADV);
	##  Muhto vuoi din riggáid, dasgo dii lehpet juo ožžon iludet.


# Accusative after adjectives and nouns
# - - - - - - - - - - - - - - - - - - -

SELECT Acc IF (*-1 A BARRIER NPNH OR CC OR CLB OR PUNCT LINK 0 Nom LINK 
	NOT 0 Attr OR ("buorre") OR Adv OR N)(NOT 0 Px OR PrfPrc OR Adv)
	(1 EOS OR CLB); #(NOT 0 TIME);
	## Mánát leat oažžulat guliid.

SELECT Acc IF (-1 ("ahkit"));
	## Mánás lea ahkit eatni.


# Genitive (or nominative) in clause without full verb
# - - - - - - - - - - - - - - - - - - - - - - - - - - -
REMOVE Acc IF (NOT *-1 V-TRANS-ACT BARRIER CS)
	(NOT *-1 (Pass PrfPrc) BARRIER NPNH LINK *-1 V-TRANS-ACT BARRIER NPNHA)
	(NOT -1 CC LINK -1 Acc LINK -1 ("ollu"))(NOT 0 TIME)
	(NOT 0 NUMERALS LINK 1 TIME OR MEASURE)
	(NOT *1 V-TRANS-ACT BARRIER CS LINK 0 VFIN OR (@-FMAINV)) # Ignores PrcN>
	(NOT *1 CLB BARRIER NOT-NP LINK *1 (Pron Acc) BARRIER NOT-ADV-PCLE);
	## Leago doppe kafea?
	## Amma duot lei Niillasa guollebiila?
	## Láhka divššohasvuoigatvuođaid birra.
	## ...jus divššohas oažžu ávkki dearvvašvuođaveahkis. 
	## Sámedikki čoahkkimat dollojuvvojit rabas uvssaid.

 # This rule also (correctly) removes Acc from many elements that will 
 # eventually be analysed as verbs.

REMOVE Acc IF (*-1 CP BARRIER VERB)(NOT 0 TIME)
	(*1 COPULAS BARRIER V-NOT-AUX LINK NOT *1 VERB);
	## ...ja makkár váldi dain nuppádassii berre leat.
	## ...main sámi ja sámi guoskevaš fáttát leat guovddážis.
 # This rule removes Acc in subordinate clauses that has no full verb.

REMOVE Acc IF (*-1 COPULAS BARRIER NOT-AA LINK *-1 (@SUBJ) BARRIER V-NOT-AUX 
	LINK *-1 BOS OR CLB BARRIER NPNH)(1 CLB OR EOS);
	## Ale váldde, dat lea mu!


# Genitive in front of "oktavuođas" etc.
# - - - - - - - - - - - - - - - - - - -
SELECT Gen IF (1 PREGEN);
	## ...go sii bargguset oktavuođas Sámedikkis ožžot dakkár dieđuid.
	## ...geat barget erenoamážit sámi guoskevaš čuolbmačilgehusaiguin.


# Genitive between Dem and N
# --------------------------
 # Here I have left out all rules that would involve an Acc/Gen demonstrative,
 # since these demonstratives could be objects not belonging with the N.
 
REMOVE Acc IF (*-1 (Dem Nom) BARRIER NPNH)(*1C (N Nom) BARRIER NPNH OR Dem);
	## Dát eaiggáduššanvuoigatvuođa doaba de geahččaluvvo...

REMOVE Acc IF (*-1 (Dem Sg Com) BARRIER NPNH)(*1 (N Sg Com) BARRIER NP-BORDER OR Dem);
 	## No examples in corpus, but the rule should work. 

REMOVE Acc IF (*-1 (Dem Pl Ill) BARRIER NPNH)(*1 (N Pl Ill) BARRIER NP-BORDER OR Dem);
	## Dearvvašvuođaregisttarláhka gusto daidda dearvvašvuođa-
	## hálddahusa dearvvašvuođadieđuid gieđahallamiidda.

REMOVE Acc IF (*-1 (Dem Pl Loc) BARRIER NPNH)(*1 (N Pl Loc) BARRIER NP-BORDER OR Dem);
	## No examples in corpus.

REMOVE Acc IF (*-1 (Dem Pl Com) BARRIER NPNH)(*1 (N Pl Com) BARRIER NP-BORDER OR Dem);
	##

REMOVE Acc IF (*-1 ("oktan") OR ("ovttas") OR ("ovttasráđiid") BARRIER NPNH)
	(*1 (N Com) BARRIER NP-BORDER);
	## Eamiálbmotguovddáža ásaheami sáhttá geahččat ovttas universitehta
	## Áivan dutkamiid guovddáža ohcamiin.


# Genitive in temporal adverbials
# -------------------------------
 # A 'measure' time adverbial is Acc, others are Gen. 

SELECT (@GN>) IF (0 MANNU)(1 (Num Ord));    
	##  Dál lea miessemánu 1. beaivvi.

SELECT Gen IF (-1 Ord)(0 ("beaivi") OR ("vahkku"));
	## Evaluerenraporta addojuvvui GOD:ii geassemánu 20. beaivvi 2000.

SELECT Gen IF (-3 MANNU)(-2 Ord)(-1 ("beaivi") OR ("b" ABBR))(0 Num);
	## Sámedikki bargiidčoahkkimis Guovdageainnus juovlamánu 7. beaivvi 2001
	## mearridii...

SELECT Gen IF (-1 ("boahtte") OR ("boahtit" PrsPrc) OR ("ovddit" Attr))(0 TIME);
	## Boađe boahtte vahku.

SELECT Gen IF (-1 COMMA)(0 TIME)(1 EOS);
	## Mun vuolggán ihttin, bearjadaga.

SELECT Gen IF (-1 BOS OR CS)(0 BEAIVI);
	## Bearjadaga mii vuolgit oaggut.

SELECT Gen IF (-1 NP-BORDER)(0 BEAIVI);
	## Mun boađán bearjadaga.
 # The idea here is that the BEAIVI should not be modified by e.g. 'olles'.

SELECT Gen IF (0 Card)(1 (N Gen));
	## Dat lea duháhiid jagiid boaris.

SELECT Gen IF (0 Num)(1 TIME)(1 ADVLCASE);
	## Journálat sáhttet lágiduvvot fylkkamánne rádjui 10 jahkái.

SELECT Gen If (-1 (@ADVL))(NOT -1 Pr OR Attr)(0 TIME);
	## Diddi vuodjala detnui mihcamárain maŋás ja guvžá fas čavččabeallái geasi.
	## Čuonžžá goargŋu detnui čakčageasi.

SELECT Gen IF (-1 ("juohke") OR ("seammá" Attr))(0 TIME); 
	## ...álbmotjoavkku mat ovdal dahje seammá áiggi...
 # Other attributes similar?

SELECT Gen IF (0 ("jahki"))(1 Num); 
	## Jagi 2002 bušeahttačoahkkima,...   

SELECT Gen IF (NOT 0 VERB)(1 ("áigi"))(NOT 1 (Sg Loc) LINK 1 ("áigi" Sg Ill));
	## Čoahkkimis lea lávdegoddi árvalan konfereanssa áiggi, báikki ja fáttáid.
	## ...oppa dan áiggi go dáláš konvenšuvdna lea doaibman.

SELECT Gen IF (-1 Gen)(0 ("áigi"));
	## ...gonagasa Herodesa áiggi,..

SELECT Gen IF (0 ("hávvi"))(*1C Acc BARRIER CS OR CP);
	## Váilevaš vejolašvuohta doaimmahit kultuvrra ávnnaslaš beliid sáhttá
	## muhtun háviid mearkkašit álbmotriekterihkkuma.

SELECT Gen IF (0 ("hávvi"))(NOT *-1 V-TRANS-ACT BARRIER CS)
	(NOT *1 V-TRANS-ACT BARRIER CS OR CP);
	## Fágalaš sudjadeapmi daid háviid go doavttir lea dahkan biehttaleami.

SELECT Gen IF (*-1 V-PASS BARRIER VFIN OR CS)(0 ("hávvi"))
	(NOT *1 V-NOT-AUX BARRIER CS OR CP);
	## Muhtun háviid buhtaduvvo meassamiid ruđalaš vahát.

 # The time expression "hávvi" is not Acc, but the problem is the homonymous noun. 

SELECT Gen IF (*-1 BOS OR CS BARRIER NPNHA)
	(0 ("áigi") OR ("bodda") OR ("geardi") OR ("hávvi"));
	## Eará háviid ii leat hoahpu journálačállimiin.

SELECT Gen IF (1 ("riegádan#beaivi"));
	## Golbma čáppa, doaimmalaš nieidda livžo áhčiset riegádanbeaivve.

SELECT Gen IF (-1 (Num Gen))(0 ("geardi"));
	## Galggat golmma geardde beaivvis borrat ovtta tableahta.

	
# Genitive in place adverbials
# ----------------------------
SELECT Gen IF (*-1 MOVEMENT-VERB BARRIER NPNHA)(0 ROUTE);
	## ...ja sii manne eará luotta ruoktot eatnamasaset.


# Genitive specifiers
# -------------------

REMOVE (N Acc) IF (*-1C NP-BORDER BARRIER WORD)(NOT -1 V)(NOT 1 VFIN)
	(*1 N BARRIER NPNH OR CC LINK *1 VFIN BARRIER CC OR CS OR CP)
	(NOT *1 (@SUBJ) BARRIER NOT-NP OR CC LINK *1 V-TRANS-ACT BARRIER 
