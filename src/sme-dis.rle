	## Mun in leat vel borran dan guolis maidege.

REMOVE (@OBJ) IF (-1 V-TRANS-ACT BARRIER NOT-ADV-PCLE LINK *-1 (@OBJ) BARRIER
	NOT-ADV-PCLE)(0 Adv);
	## In sáhtege buohkaid biebmat gallás.

SELECT (@OBJ) IF (*-1 Inf BARRIER NPNHA LINK 0 V-TRANS-ACT LINK NOT *-1 (@OBJ)
	BARRIER V-NOT-AUX OR CS OR CP)(NOT 0 Loc LINK *-1 (@OBJ) BARRIER VERB)
	(1 (@OPRED) LINK 1 NP-BORDER);
	## Divššohasbearráigeahčči galgá dahkat ortnega dovddusin.

SELECT (@OBJ) IF (*-1 V-TRANS-ACT BARRIER CS OR CP OR go OR (@OBJ) 
	LINK NOT *-1 (@OBJ) BARRIER CS OR CP)(NOT 0 Adv)
	(NOT 0 Nom LINK *-1 (TV N Gen) BARRIER NPNH)(NOT 0 TIME)
	(NOT 0 Dem LINK *1 N BARRIER NPNH)(NOT 0 Loc LINK *-1 (IV) BARRIER VERB)
	(NOT 0 (Acc PxSg3) LINK 0 Loc)(NOT 1 TV)
	(NOT *1 (@OBJ) BARRIER CC OR CS OR CP)
	(NOT 0 ("nr") LINK -1 N OR A OR ACR OR ABBR);
	## Mun oidnen guovžža golbma kilomehtara dábbelis.

REMOVE (@GN>) IF (*-1 V-TRANS-ACT BARRIER NOT-ADV-PCLE LINK 0 VERB 
	LINK NOT *-1 (@OBJ) BARRIER CS OR CP OR CC)(1 (@GP>));
	## ...ja maiddái muđui guorahallat soahpameahttunvuođa 
	## vuoigatvuođa ja duođalaš geavaheami gaskkas.

REMOVE (@GN>) IF (1 (@-FMAINV))(1 V-TRANS-ACT);
	## Divššohas dahje su ovddasteaddji galgá leat váidalusa vuolláičállán.

SELECT (@OBJ) IF (*-1 V-TRANS-ACT BARRIER VERB OR CS OR go)
	(NOT *-1 (@OBJ) BARRIER CS)(NOT 0 Loc)(NOT 0 Dem LINK *1 N BARRIER NPNH)
	(*1 ADVLCASE BARRIER NOT-ADV-PCLE LINK NOT *1 (@OBJ) BARRIER CS);
	## De olahit olbmot luosa buođđogežiin.

SELECT (@OBJ) IF (*-1 V-TRANS-ACT BARRIER NOT-ADV-PCLE)(1 Num)(2 TIME);
	## Go fylkkadoavttir lea seailluhan journála 10 jagi,...

SELECT (@OBJ) IF (NOT *-1 VFIN BARRIER CS)
	(1 COMMA LINK *1 COMMA LINK *1 V-TRANS-ACT BARRIER V-NOT-AUX);
	## Muhto gittabivdosa, nugo buođu ja njaŋgofierpmi, oažžu son atnit 
	## dušše dan čázis mii sutnje lea mearriduvvon.

SELECT (@OBJ) IF (*-1 ("deavdit") BARRIER NPNHA);
	## Go mánná lea deavdán 12 jagi,..

SELECT (@OBJ) IF (0 TIME)(*1 ("deavdit") BARRIER NOT-ADV-PCLE);
	## ...maŋŋá go lea 16 jagi deavdán.

SELECT (@OBJ) IF (1 (TV Sup) LINK NOT *1 (@OBJ) BARRIER CS OR CC);
	## Boađe sisa gáfe jugažit.

SELECT (@OBJ) IF (1 (TV Actio Loc))(NOT 1 N)(NOT *2 (@OBJ) BARRIER CS);
	## Manin don gielddát mu dan muitaleames?
 # This rule would work just fine if it were not for the fact that the 
 # verbal actio "-eames" (instead of "-eamis") is often used where we 
 # would expect a nominal actio.

SELECT (@OBJ) IF (-1 ("ollu") LINK *-1 V-TRANS-ACT BARRIER NOT-ADV-PCLE)
	(0 TIME)(1 NP-BORDER);
	## Dat gáibida ollu áiggi.

REMOVE (@GN>) IF (1 (PUNCT LEFT) LINK *1 (PUNCT RIGHT) LINK 1 NOT-NP);
	## Molssaevttolaš dálkkasdiehtaga našunála dutkanguovddáš ja Sámi 
	## dearvvašvuođadutkanguovddáš (goappašat fágabirrasat gullet Romssa 
	## universitehta vuollái) berrejit sáhttit oassálastit dán bargui.


# Coordinated objects
# - - - - - - - - - -

SELECT (@OBJ) IF (0 TIME)(1 CC)(*2 (@OBJ) BARRIER NPNH);
	## Dat gáibida almmatge sihke ollu áiggi ja resurssaid.

REMOVE (@GN>) IF (1 CC)(2C (@OBJ));
	## Ferte váldit vuhtii máná ági ja láttasvuođa ja muđui eará beliid.

SELECT (@OBJ) IF (-2C (@OBJ))(-1 CC)(1 Gen)(2 Po);
	## Jođiheaddjit galget čuovvolit mearrádusaid ja njuolggadusaid
	## giellageavaheame hárrái.


# Verbal functions
# ----------------

SELECT (@+FAUXV) IF (*1 Inf OR PrfPrc OR (Actio Ess) BARRIER CS OR CP OR VFIN);
	## Máhtte áigu heaitit vuola juhkamis.

SELECT (@+FAUXV) IF (*1 ConNeg BARRIER CS OR CP OR Neg);
	## Allos dattege šatta nu go mun dáhtun, muhto nugo don dáhtut.

REMOVE (@+FAUXV) IF (0 AUX-OR-MAIN)(NOT *1 Inf OR PrfPrc OR (Actio Ess) OR ConNeg 
	BARRIER CS OR CP);
	## Maid Máret áiggui goarrumis?

SELECT (@-FAUXV) IF (*1 Inf OR PrfPrc OR (Actio Ess) BARRIER CS OR CP);
	## In beassan dal deatnojohtima oaidnit.

REMOVE (@-FAUXV) IF (0 (@-FMAINV));
	## Sápmelaččat ledje orron dáin guovlluin 1600-logu birrasiin. 

SELECT (@-FMAINV) IF (*-1 (@SUBJ) BARRIER CS OR CP LINK 0 Sg)(0 Inf);
	## Vuoigatvuohta journála geahččat.

SELECT (@+FMAINV) IF (*-1 (@OBJ) BARRIER NOT-ADV-PCLE LINK *-1 (@SUBJ) 
	BARRIER NOT-ADV-PCLE LINK 0 Sg)(0 Sg3);
	## Juohke háve go olmmoš joddosaji molsu...

SELECT (@+FMAINV) IF (NOT *-2 VFIN BARRIER CS)(-1 (@ADVL))(NOT 0 Inf)
	(NOT *1 VFIN BARRIER CS);
	## Dalle bidjet buođđogeaži.


# Other grammatical functions
# ---------------------------


# Genitive
# - - - - -

REMOVE (@GN>) IF (1 CC)(2 ("dat" Gen));
	## Romssa universitehta sámi strategiijaplána ja dan ruhtadeami...

SELECT (@GN>) IF (NOT 0 Dem OR Interr OR PrsPrc OR (@ADVL))(*1C (@SUBJ) BARRIER NPNH)
	(NOT 1 CC OR COMMA);
	## Romssa universitehta sámi studeanttaid mátkedoarjjaortnet lea 
	## máŋga jagi leamaš mielde ásaheame...		# 4 hits here!!

REMOVE (@GN>) IF (0 (@TITLE));
	## ...maid profehta Jeremia lei sárdnon.

REMOVE (@GP<) IF (*-1C Po BARRIER NPNH);
	## Son lea dat gean birra profehta Jesaja sardnu.

REMOVE (@GP<) IF (NOT *-1 Pr BARRIER NPNHA);
	## ...ovdalgo 1883 láhka ásahuvvui.

SELECT (@GN>) IF (-1C (Num Gen))(NOT 0 Loc)(1 N);
	## Unnimus 135 jagi geavahus lea doarvái.

SELECT (@GN>) IF (NOT *-1 V-TRANS-ACT BARRIER CS)(0 N)(1 N)(*2 V-TRANS-ACT BARRIER
	V-NOT-AUX OR CC OR CS OR CP)(*2 (@OBJ) BARRIER CS OR CP);
	## Máŋga dehálaš servodatásahusa Norgga servodagas leat jitnosit dovddastan 
	## morálalaš ja politihkalaš ovddasvástádusaset duolbmamiin.

REMOVE (@OBJ) IF (-2 (A Attr) LINK NOT *-1 V-TRANS-ACT BARRIER CC OR CS OR CP)
	(-1 (N Actio));
	## ...ja nu leat mielde oppalaš gelbbolašvuođahuksemis
	## eamiálbmogiid saji ja vuoigatvuođaid ektui.
 # The Actio is nominal if preceded by (A Attr). Then a following noun is not 
 # the object of that Actio.

SELECT (@GN>) IF (NOT -1 NP-BORDER)(NOT -1 TIME)(0 TIME)(NOT 0 ("geardi"))(1 NOTIME);
	## Nugo namuhuvvui, lea nannoseamos ágga dása guhkes áiggi geavahus. 


# Attributes and quantifiers
# - - - - - - - - - - - - - -

SELECT (Prop Attr) IF (1 Prop)(1 (@SPRED));
	## Mun lean Marit Julien.

SELECT (@QN>) IF (0 Num)(*1 (@OBJ) BARRIER NPNH);
	## Dat gáibidii mis 5000 ruvnno.

SELECT (@QN>) IF (1 ("jahkásaš"));
	## Stuoret mánná, viđa-guđa jahkásaš, gal riide.

SELECT (@AN>) IF (0 ("veara"))(1 N);
	## Dattetge ii leat muitaluvvon makkárge namahan veara geavahanriiddu 
	## birra dálolaččaid ja boazodoalu gaskkas.

REMOVE (@AN>) IF (NOT *1 N OR A BARRIER NPNH);
	## Sii eat sáhte čielgasit diehtán ahte... 

SELECT (@NumN<) IF (-2 (@NumN<))(-1 CC);
	## ...gč. dearvvašvuođabargiidlága kapihttal 6 ja 7.
	
REMOVE (@QN>) IF (NOT 0 ("okta" Ess) LINK *1 Ess BARRIER NPNH)(0 (@ADVL))
    (1 N OR A OR Pron) ;	
	## Ráđđehus oktii áigodagas galgá bidjat ovdan dieđáhusa Stuorradiggái

SELECT  (@QN>) IF (0 ("okta" Ess) LINK *1 Ess BARRIER NPNH) ;
    ## Son vuodjá oktan guollin.
    
REMOVE (@QN>) IF (0 Nom)(1 ("dbm") OR ("diibmu"))(2 Num);



# Adverbials
# - - - - - -

SELECT (@ADVL) IF (0 (@SUBJ))(*-1C (@SUBJ) BARRIER CS OR CP OR CC);
	## Boazu dáidá buot ovddimus leat fievrun adnojuvvot dan láhkái ahte...
 
SELECT (@ADVL) IF (NOT 0 ("okta" Ess) LINK *1 Ess BARRIER NPNH)(0 Num)
    (*-1 V-NOT-AUX LINK 0 IV);
	## Fierbmi bistá guokte-golbma jagi.

SELECT (@ADVL) IF (*-2 (@OBJ) BARRIER CC OR CLB OR CS OR CP OR Pr)(-1 NUMERALS)
	(0 TIME OR MEASURE);
	## Mii doalvvuimet ealu guokte miilla dan beaivve. 

SELECT (@ADVL) IF (*-1 INF-VERB BARRIER NOT-ADV-PCLE)(0 Num)(1 TIME)
	(*2 Inf BARRIER NOT-ADV-PCLE);
	## ...mas sáhttá ain máŋga jagi addit gieldda ássiide boaldin...

SELECT (@ADVL) IF (*-1 (@OBJ) BARRIER NOT-ADV-PCLE OR go)(0 Num)(1 TIME);
	## Go fylkkadoavttir lea seailluhan journála 10 jagi, de...

SELECT (@ADVL) IF (0 ("diibmu") OR ("dbm"))(1 Num);
	## Mun boađán diibmu vihtta.

SELECT (@ADVL) IF (NOT *-1 ("deavdit") BARRIER VERB OR CC OR CLB OR CS OR CP)
	(-1 Num LINK NOT 0 (@GP<))(0 TIME LINK NOT 0 Nom)
	(NOT *1 ("deavdit") BARRIER NOT-ADV-PCLE)(1C NOT-NP)(NOT 1 ("dassá"));
	## Dábálaš noađđeráidduin vázzá olmmoš guokte- golbma tiimmu ovdal go
	## suhpoda.

SELECT (@ADVL) IF (0 TIME)(1 TIME);
	## Galggat golmma geardde beaivvis borrat ovtta tableahta.

SELECT (@ADVL) IF (*-1 VERB BARRIER NOT-ADV-PCLE)(0 ("jahki"))(1 Num);
	## ...ja ráhkadii jagi 1991 fylkkagieldda sámi dearvvašvuođa- ja 
	## sosiálaplána.

REMOVE (@SUBJ) IF (0 (@ADVL))(*1C (@SUBJ) BARRIER CC OR CLB OR CS OR CP);
	## Mo dal juo dilli leašge,...


# Various
# - - - -

REMOVE (@SUBJ) IF (*-1 (@SUBJ) BARRIER CC OR CS OR CLB LINK NOT 0 Gen)(0 Gen);
	## Salomo, gean eadni lei Uria áhkká,...

REMOVE (@OBJ) IF (0 Gen)(*1 Ill BARRIER NPNH LINK *1 V-TRANS-ACT BARRIER NOT-ADV-PCLE
	OR PUNCT OR CLB LINK 0 V-NOT-AUX LINK *1 Inf BARRIER NOT-ADV-PCLE);
