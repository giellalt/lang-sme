SELECT Nom IF (-2 Nom)(-1 NEGFOC);
	## Eai ovdabarggut, teoria iige geavat doarjjo klausula áddejumi. 

SELECT Nom IF (-1 PUNCT-LEFT)(0 KLASS LINK NOT 0C ADVLCASE)(*1 PUNCT-RIGHT BARRIER Po OR TV OR N);
