SELECT (@SUBJ) IF (*-1 COPULAS BARRIER NPNHA LINK NOT *-1 (@SUBJ) BARRIER S-BOUNDARY2)(NOT 0 Ess)(NOT 1 COMMA)(*1 (A Nom) BARRIER NOT-ADV-PCLE OR go);
	## De lea cicha gárvvis.

SELECT (@SUBJ) IF (*-1 COPULAS BARRIER NPNH LINK *-1 ("veara") BARRIER NOT-ADV-PCLE);
	## Man ollu eanet veara lea olmmoš go sávza!

SELECT (@SUBJ) IF (0 (N Sg Nom) LINK 0 TIME)(*1 V-SG3 BARRIER NOT-ADV-PCLE);
	## Sii eai diehtán ahte mánnu šaddá.

SELECT (@SUBJ) IF (0 ("guovttis" Nom))(*1 Du3 BARRIER SV-BOUNDARY2);	
