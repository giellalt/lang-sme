SELECT ("giella") IF (-1 Attr);
	## Sámegiella hállet áidna giellan mánát, geat eai leat vel álgán skuvlii.

# guovddáš
# - - - - -
SELECT Attr IF (0 ("guovddáš"))(1 N); 
	## Guovddáš doaibmabidju lea vuođđudit sámegiela álgooahpahusa.

# guovttes
# - - - - 
REMOVE ("guovttes") IF (1 (Sg Gen));
REMOVE ("guovttes") IF (*1 (N Sg Gen) BARRIER NPNH);
	## Viesus leat guokte olbmo.
	## Sáhtášii juohkit guokte 5 mánnosaš stipeandda čállit ohcamiid. 

# hálidit, háliidit vs. hálihit
# - - - - - - - - - - - - - - -

REMOVE ("hálihit") IF (0 ("hálidit") OR ("háliidit"))(*1 (@-FMAINV) BARRIER CS OR CLB);
    ## Son ii hálit dan dahkat.

# imaš
# - - - 
SELECT N IF (-1 VFIN OR ConNeg OR A OR Dem)(0 ("imaš"));
	## Lei imaš, go buvddas eai vuovdde albma varramárffiid.
	## dakkár imaš ja ártegis dilli ahte ...

# jahke#čuohti
# - - - - - - -
SELECT ("jahke#čuohti");
	## Finnmárkolaččat ledje atnán vuvddiid jahkečuđiid.

# lága vs láhka
# - - - -
    
SELECT ("lága") IF (0 ("lága"))(-1 ("eará") OR ("iežá"))(*-1 BOS LINK *1C ("lága")) ;
    ## Lága gáibida menddo olu eará lágaid ektui.
    ## Sii bidjet menddo stuora gáibádusaid min lágii eará lágaid ektui.

# láhka
# - - -
  # The point here is to disambiguate "láhka" from "lága".
	
SELECT ("láhka") IF (0 ("láhka"))(*1 ("fápmu") LINK 1 ("boahtin") BARRIER V-NOT-COP);

SELECT ("láhka") IF (0 ("láhka") LINK *1 LAHKA-NOUN BARRIER V-NOT-COP);
	## Norgga stáda lea iešguđet lágaid ja láhkaásahusaid bokte geatnegahtton.
	## Sosiálabálvalusaid lága § 4-3.
    	
SELECT ("láhka") IF (0 ("láhka"))(*-1 LAHKA-NOUN OR ("gč") BARRIER V-NOT-COP) ;
	## Gč. lága suoidnemánu b. 1992 nr. 97.	
    ## Fápmuiboahtin ja eará lágaid rievdadusat.
    ## Gonagas addá láhkaásahusa lága geavaheami birra.

SELECT ("láhka") IF (0 ("láhka"))(-1 ("rievdadit") OR ("jorgalit") LINK 0 Pass);
SELECT ("láhka") IF (0 ("láhka"))(*1 ("rievdadit") OR ("jorgalit") BARRIER V-NOT-AUX LINK 0 Pass);
	## Rievdaduvvon láhka....
	## Ođđa lágat galget jorgaluvvot sámegillii.

SELECT ("láhka") IF (*-1 MANNU LINK 1 Ord LINK 1 ABBR LINK 1 Num)(0 ("láhka"));
	## Dat lea dahkkon skábmamánu 28. b. 1898 lága mielde.
		
SELECT ("láhka") IF (0 ("láhka" Loc))(*-1 BOS LINK *1 LAHKA-VERB-PASS LINK 0 Pass) ;
	## Dat lea mearriduvvon lágas 33.
    ## Ii adnon norgga lágas daid attáldagaid birra maid dáččat mákse iežaset gonagassii.

SELECT ("láhka") IF (*1 ("mearridit") OR ("čuoldit") BARRIER VFIN)
	(0 ("láhka" Sg Gen))(*1 Po BARRIER V);
	## Dat lea mearriduvvon lága 3 mielde.
	
SELECT ("láhka") IF (-1 (@GN>) LINK 0 LAHKA-MOD)(0 ("lága"));
