# Special rules
# =============

# Accusative or genitive in front of ALU
# --------------------------

SELECT Acc IF (0 NUMERALS)(*1 ALU BARRIER NON-NP);
	## Visti lei vihtta mehter alu.

SELECT Gen IF (0 ("okta"))(1 ALU);
	## Dábálaččat leat joddut sullii ovtta guhku.


# Genitive
# ========
 # Since disambiguation between accusative and genitive is very difficult, let us
 # pick out as many genitives as possible here.

# Various genitive rules
# ----------------------

# Genitive after quantifier
# - - - - - - - - - - - - - 
SELECT Gen IF (*-1C NUMERALS BARRIER NPNH OR Gen OR CC OR PUNCT OR REALCLB 
	LINK NOT 0 Ord)(NOT 0 VERB);  
	## Vihtta romes beatnaga oaguhedje bohccuid duoddaris.
 # Gen as barrier because of: ## Dan eatnamis oažžu ovtta gusa fuođđara.

SELECT Gen IF (*-1C NUMERALS BARRIER NPNH OR Gen OR PUNCT OR REALCLB LINK NOT 0 Ord 
	LINK *-1 VFIN BARRIER NON-ADV-PCLE);
	## Muhtomin ferte guokte gurpasa bidjat oktan noađđebeallin.
 
SELECT Gen IF (-1 CC)(-2 Cmpnd)(*-3 NUMERALS BARRIER NPNH OR PUNCT);
	## Máŋga sámi dearvvašvuođa- ja sosiálabargi vásihit dan. 

SELECT Gen IF (*-1 (Num Nom) BARRIER NON-NP OR CLB)(*1 V-PL3 BARRIER NON-ADV-PCLE);
	## Máŋga Romssa universitehta dutkanbirrasa leat ásahan ovttasbargguid...

SELECT Gen IF (*-1 NUMERALS BARRIER NPNH OR CC OR CLB OR PUNCT OR ("ruvdnu")
	LINK NOT -1 ABBR)(*1 Gen BARRIER NPNH);
	## Máŋga Romssa universitehta dutkanbirrasa leat ásahan ovttasbargguid...


# Genitive in front of postposition
# - - - - - - - - - - - - - - - - - 
SELECT Gen IF (NOT 0 Adv OR PrfPrc)(1C Po)(NOT 1 Px);    
SELECT Gen IF (NOT -1 ("§"))(0 Num)(*1 (N Gen) BARRIER NPNH LINK 1 Po);
	## Lávdegottiid čoahkkimat dollojit giddejuvvon uvssaid duohken.
	## Divššohasas lea vuoigatvuohta oažžut dearvvašvuođadilis árvvo-
	## štallojuvvot 30 bargobeaivvi sisa.

SELECT Gen IF (1 CC)(*2 Gen BARRIER NPNH LINK 1 GASKAL);
	## Juos fágabargi ja geavaheaddji gaskka leat dasa lassin gielalaš
	## dahje kultuvrralaš gulahallanváttisvuođat,...

SELECT Gen IF (1 ("dihte" Po) OR ("dihtii"));
	## Vuovdon earit máhcahovvojit sámi guovlluide nannen dihte báhcán doaluid.

SELECT Gen IF (0 Actio)(1C Po);
	## Válddán dan ášši guorahallan vuollái.

SELECT Gen IF (*-1 Po OR PREGEN BARRIER NON-NP OR PUNCT OR REALCLB)
	(*1 Po OR PREGEN BARRIER NON-NP OR REALCLB);
	## ...dan vuolde sisdoalu birra divššohasjournálain, čállima, divodeami,
	## sihkkuma, vurkema, sirdima, journála beassanvejolašvuođa ja dušša-
	## deami birra.

SELECT Gen IF (1 (PUNCT LEFT))(*2 (PUNCT RIGHT) LINK 1 Po);
	## Dearvvašvuođaregisttarláhka lea sierraláhka heaggadieđuidlága (cuoŋo-
	## mánu 14. b. 2000 láhka nr 31 heaggadieđuid gieđahallama birra) ektui.


# Genitive after preposition
# - - - - - - - - - - - - - -
SELECT Gen IF (*-1 Pr BARRIER NPNH OR REALCLB)(NOT 0 VFIN)
	(1 NP-BORDER OR CC OR COMMA OR ("ieš")); 
	## Earret mu ii giige lea boahtán.
	## Sáhka su birra viidánii miehtá Syria, ja ...
	## ...gaskal vejolaš ja dohkálaš iskkadan- ja dikšunvugiid.
	## ...ja vulggii rastá jávrri iežas gávpogii. 

SELECT Gen IF (*-1 Pr BARRIER NON-NP)(*1 Interr BARRIER WORD);
	## Sáhttá dadjat, ahte jagi 1978 láhka balansere gaskkal sámi 
	## vuoigatvuođaid boarásmuvvan oainnu, mii lei ráđđejeaddjin 
	## "vigihis ávkkástallanrievtti áigodagas", ja eanet ođđaáigásat 
	## vuoigatvuođaid dovddasteaddji oainnu.

SELECT Gen IF (-1 Pr)(0 Num);
	## Jus divššohas lea badjel 16 jagi boaris... 

SELECT Gen IF (-3 GASKAL)(-2 (Num Gen))(-1 CC)(0 Num);
	## Jus divššohas lea gaskal 16 ja 18 jagi boaris,...


# Genitive inside a two-part Po
# - - - - - - - - - - - - - - -
SELECT Gen IF (-1 POST-A)(1 N)(2 POST-B);
	## Nu leat 40 buolvva Dávveda rájes Babylonia fáŋgavuođa rádjái.


# Genitive attribute of adposition.
# - - - - - - - - - - - - - - - - -
SELECT Gen IF (1 ("bealde" Pr) OR ("beale" Pr) OR ("beallái" Pr) OR ("geahčai" Pr)
	OR ("geahčen" Pr) OR ("geaže" Pr));
	## Ánde boahtá don bealde joga.


# Genitive with "dan mađe N"
# - - - - - - - - - - - - -
SELECT (Gen Sg) IF (0 ("dat"))(2 ("mađe"));

	
# Genitive relative pronouns
# - - - - - - - - - - - - - -
SELECT (Pron Interr Gen) IF (NOT 0 Indef)(1 N)(*1 COPULAS BARRIER CLB)
	(NOT *1 V-NON-AUX-PASS BARRIER CLB);
        ## Oidnen mátkkošteaddji, gean namma lei Keilhau.


# Genitive indefinite pronouns
# - - - - - - - - - - - - - - -
SELECT Gen IF (0 Indef)(1 (N Ill) OR (N Loc) OR (N Com));
	## De galget ollu dearvvašvuođabargit iešguđet fidnojoavkkuin dan
	## geavahit. 


# Genitive in proper name sequences
# - - - - - - - - - - - - - - - - - 
REMOVE Acc IF (0 Prop)(1 Prop);          
	## Dat lea Norgga Röros ja Máhte Piera.
  # Hmm, very general indeed!


# Genitive after "mii" and "mihkkege"
# - - - - - - - - - - - - - - - - - -
SELECT (N Pl Gen) IF (-1 ("mii"))(1 NP-BORDER)(*1 ("?")); 
	## Maid ipmašiid doppe dagat?
 # Embedded questions are also possible, but harder to formulate a rule. 

SELECT (N Pl Gen) IF (-1 ("mihkkege"));
	## Iige njuovvan ja bierggu rádjan leat mihkkege dearvvašvuođa-
	## váttisvuođaid.


# Genitive in front of actio
# - - - - - - - - - - - - - -
SELECT Gen IF (NOT *-1 ("leat") BARRIER CS)(NOT 0 Interr)(1 Actio)(NOT 1 Ess)
	(NOT 1 (V Actio Loc))(NOT 2 Po);
	## Divššohas sáhttá geassit iežas miehtama ruovttoluotta.


# Accusative after actio (exceptions to next rule)
# - - - - - - - - - - - - - - - - - - - - - - - - 
SELECT Acc IF (*-1 ("dihte") OR ("dihtii") BARRIER NPNH LINK -1 Actio)
	(1 NP-BORDER);
	## ... diehtin dihtii iežas dili ja veahki sisdoalu.

SELECT Acc IF (*-1C Actio BARRIER HNOUN-NOM OR (Pron Nom) OR VERB OR CS OR CP 
	LINK NOT 0 Cmpnd OR Gen)(NOT -1 CC LINK -1 Actio LINK NOT 0 Cmpnd)
	(NOT 0 TIME)(NOT 0 Num LINK -1 ("§"))(1 NP-BORDER)(NOT 1 N); 
	## ...dahje biehttalit botkemis nealgudeami mii lea jođus. 


# Genitive in front of "láhkái"
# - - - - - - - - - - - - - - - 
SELECT (Actio Gen) IF (1 ("láhki" N Sg Ill));
	## ...go dearvvašvuođabargit iežaset árvvoštallama mielde gávnnahit...
	## Dat ráhkaduvvo gáttis gárvvisin ja gessojuvvo vatnasii suohppun
	## láhkái.


# Accusative after "vuoi" (exceptions to REMOVE Acc-rule below)
# - - - - - - - - - - - - - - - - - - - - - - - - -
SELECT Acc IF (*-1 ("vuoi") BARRIER NPNH)(NOT 0 TIME LINK *-1 (N Acc) BARRIER ("vuoi"))
	(NOT *1 N BARRIER NPNH OR CC OR CLB);
	## Vuoi du, Betsaida!

SELECT Acc IF (*-1 ("vuoi") BARRIER NON-NP OR CLB OR CP)
	(NOT 0 TIME LINK *-1 (N Acc) BARRIER ("vuoi"))(1 NP-BORDER);
	## Vuoi dán máilmmi ja dan fillehusaid!
 
SELECT Acc IF (*-1 ("vuoi") BARRIER NON-NP)(0 Dem);
	## Vuoi surgat dan gándda!

SELECT Acc IF (*-1 CC BARRIER NPNH LINK -1 Acc LINK *-1 ("vuoi") BARRIER NON-NP)
	(NOT 0 Dem OR Pers);
	## Vuoi áhpehemiid ja njamaheddjiid daid beivviid! 

SELECT Acc IF (*-1 Pers BARRIER NPNH OR CLB LINK *-1 ("vuoi") BARRIER NON-ADV);
	##  Muhto vuoi din riggáid, dasgo dii lehpet juo ožžon iludet.


# Genitive (or nominative) in clause without full verb
# - - - - - - - - - - - - - - - - - - - - - - - - - - -
REMOVE Acc IF (NOT *-1 V-NON-AUX-PASS BARRIER CS)(NOT 0 TIME)
	(NOT *1 V-NON-AUX-PASS BARRIER CS)
	(NOT *1 CLB BARRIER NON-NP LINK *1 (Pron Acc) BARRIER NON-ADV-PCLE);
	## Leago doppe kafea?
	## Amma duot lei Niillasa guollebiila?
	## Láhka divššohasvuoigatvuođaid birra.
	## ...jus divššohas oažžu ávkki dearvvašvuođaveahkis. 
	## Sámedikki čoahkkimat dollojuvvojit rabas uvssaid.

