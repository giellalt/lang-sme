# Mappings of minor categories
# ----------------------------

MAP (@CC) TARGET CC ;
MAP (@CS) TARGET CS ;
MAP (@INTERJ) TARGET Interj ;
MAP (@PCLE) TARGET Pcle ;


# Determiner mappings
# -------------------

MAP (@DN>) TARGET Dem IF (*1 Num OR Pron OR A OR N BARRIER NPNH);


# Genitive mappings
# -----------------

MAP (@GN>) TARGET Gen IF (NOT 0 ALU)(*1 N BARRIER NPNH) ;
MAP (@GP>) TARGET Gen IF (1 Po) ;
MAP (@<GP) TARGET Gen IF (*-1 Pr BARRIER NPNH) ;

MAP (@<GQ) TARGET Gen IF (*-1 NUMERALS BARRIER NPNH);

 
# Qantifier mappings
# ------------------

MAP (@SUBJ-QH @OBJ-QH) TARGET NUMERALS IF (0 Nom)(*1 Gen BARRIER NPNH)
	(NOT *1 ALU BARRIER NON-NP);

MAP (@QN>) TARGET Num IF (*1 Ill OR Loc OR Com OR Ess BARRIER NPNH) ;

MAP (@QN>) TARGET Num IF (0 Acc)(*1 ALU BARRIER NON-NP);


# Attribute mappings
# ------------------

MAP (@PronN>) TARGET (Pron Attr) ;
MAP (@PronN>) TARGET Indef IF (*1 N BARRIER NPNH);

MAP (@AN>) TARGET (A Attr) ;
MAP (@AN>) TARGET ("buorre") IF (*1 N BARRIER NPNH);

MAP (@AN>) TARGET PrfPrc IF (*-1 PrfPrc BARRIER NPNHA LINK 0 V-NON-AUX)
	(*1C N BARRIER NON-ADJ);
	## Ii leat boahtán mannan váhkus.

MAP (@AN>) TARGET PrfPrc IF (*-1 BOS OR REALCLB BARRIER NPNH)(NOT 0 ("leat"))
	(*1 N BARRIER NPNH);

MAP (@AN>) TARGET PrfPrc IF (NOT *-1 ("leat") OR Neg BARRIER CS)(NOT 0 ("leat"))
	(*1 N BARRIER NPNH);
 
MAP (@PronN<) TARGET (Indef Pl Nom) IF (*-1 PPRON-DU-PL OR (N Pl) LINK 0 Nom
	BARRIER CS);
	## Soai leaba goappašagat nu jallat.


# Fragment mappings
# -----------------

MAP (@HNOUN) TARGET (N Nom) IF (NOT *-1 VFIN)(1 Inf)(NOT *2 VFIN)
	(NOT 0 Pl LINK 1 V-PL3)(NOT *-1 CC BARRIER NPNH LINK -1 Nom);
	## Stipeanda čállit prošeaktaohcamiid.

MAP (@VOC) TARGET Nom IF (-1 BOS OR REALCLB)(1 COMMA)(2 VFIN);
	## Ustiban, in mun daga dutnje vearrut.


# Verb mappings
# -------------
MAP (@+FAUXV @+FMAINV) TARGET ("orrut") IF (NOT *-1 ("leat") OR INF-VERB OR Neg 
	BARRIER VERB OR CS OR CP)(0 VFIN);
	## Sii orro darfegođiid siste.

MAP (@-FAUXV @-FMAINV) TARGET ("orrut");
	## Olbmot eai orron čadnon dušše iežaset mearragáddái bivdit.

MAP (@-FAUXV @-FMAINV) TARGET AUX-OR-MAIN IF (*-1 INF-VERB BARRIER VFIN)
	(0 Inf);
	## Son galgá beassat buktit oaivilis.

MAP (@+FAUXV @+FMAINV) TARGET AUX-OR-MAIN IF (NOT *-1 VFIN BARRIER CS OR CP)
	(0 VFIN);
	## Maid Máret áiggui goarrumis?

MAP (@-FAUXV @-FMAINV) TARGET AUX-OR-MAIN IF (NOT 0 VFIN);
	## In beassan dal deatnojohtima oaidnit.

MAP (@-FAUXV) TARGET AUX IF (*-1 Neg)(0 ConNeg OR PrfPrc);
	## Eat mii sáhte vuolgit.

MAP (@+FAUXV) TARGET AUX IF (0 VFIN);
	## Don šattat vel joksat Sirpmás gohcciid.

MAP (@-FAUXV) TARGET AUX ;
	## Mun in šaddan vuolgit.

MAP (@+FMAINV) TARGET VFIN-NON-AUX ;
	## Boađe boahtte vahku.

MAP (@-FMAINV) TARGET PrfPrc IF (*-1 VFIN) ;
	##

MAP (@-FMAINV) TARGET PrfPrc IF (0 ("leapma" Dimin));
	##

MAP (@-FMAINV) TARGET Inf;
MAP (@-FMAINV) TARGET Inf IF (*-1 VFIN);
MAP (@-FMAINV) TARGET ConNeg ;
MAP (@-FMAINV) TARGET (V Actio Ess);


# Argument mappings
# -----------------

MAP (@SUBJ @SPRED) TARGET Nom ;

MAP (@SPRED) TARGET ALU ;

MAP (@OBJ) TARGET Acc ;


# Adverbial mappings
# ------------------

MAP (@ADVL) TARGET Adv ;
MAP (@ADVL) TARGET Po ;
MAP (@ADVL) TARGET Pr ;

MAP (@ADVL) TARGET Ill ;
MAP (@ADVL) TARGET Loc ;
MAP (@ADVL) TARGET Com ;
MAP (@ADVL @SPRED @OPRED) TARGET Ess ;

MAP (@ADVL) TARGET Gen IF (0 TIME);


# Default mapping
