# Finite or not
# =============

SELECT VFIN IF (*-1 BOS OR CS BARRIER VERB)(0 INF-VERB)
	(*1 Inf BARRIER VFIN OR CS OR CP);
	## Áigugo fas skuvlii álgit? 
	## Sáhttit maid namahit ahte ...
 # Looks brave but works!


SELECT VFIN IF (NOT *-1C VFIN BARRIER CS OR CP)(-1 Loc)(0 LOC-VERB); 
	## Nuppi lađđasis čuovvu, ahte...

REMOVE VFIN IF (NOT -2 BOS OR CLB)(-1 Inf)(NOT 0 Qst);
	## Lága juksanmearrin lea váikkuhit sihkkarastit álbmogii 
	## ovttadássásaš dearvvašvuođaveahki.
 # Neg conditions because of:
	## Suhpodit daddjojuvvo dat go dušše nođiid váldá olmmoš eret...
	## Mearrideaddjin dasa ferte danin leat addojuvvogo... 

REMOVE VFIN IF (*-1 Nom BARRIER NON-ADV-PCLE OR CLB LINK *-1C VFIN 
	BARRIER NON-ADV-PCLE OR CP OR CLB);
	## Lea dárbu nannet doaimmaid mat ovddidit sámi dutkama.

REMOVE VFIN IF (*-1 PrfPrc BARRIER HNOUN OR (Pron Nom) OR VERB OR CC OR CS OR CP 
	OR REALCLB LINK *-1 ("leat") BARRIER NON-ADV-PCLE);
  	## Sin badjel geat orrot jápmima eatnamis ja suoivanis, lea ihtán čuovggas.
	## Gii lea cuigen din báhtarit boahttevaš moarreduomus?

REMOVE VFIN IF (*-1C VFIN BARRIER CC OR CS OR CP OR CLB OR PUNCT LINK NOT *-1 Interr
	BARRIER NON-NP)
	(*1 CC OR CLB OR CS OR CP OR EOS BARRIER NP-HEAD-NOM OR NP-HEAD-ACC);
	## Muhto sáđuid son boaldá čáskatmeahttun dolas.

REMOVE VFIN IF (NOT *-1 Interr BARRIER VERB)
	(*1C VFIN BARRIER CLB OR CC OR CS OR CP OR PUNCT OR go OR ("dego") OR 
	("galle" Num) LINK NOT 0 Qst)(NOT *1 Neg BARRIER VFIN)
	(NOT *1 VFIN BARRIER CLB OR CC OR CS OR CP LINK 1 go);
	## Sámegillii gálbet universitehta lea šaddan realitehta.

REMOVE VFIN IF (-1 ("nu"))(*1 ("go" Pcle) BARRIER NON-NA); 
	## No longer needed?

REMOVE Inf IF (-1 ("nu"))(*1 ("go" Pcle) BARRIER NON-NA); 
	## No longer needed?

REMOVE ConNeg IF (*-1 PrfPrc BARRIER NON-ADV-PCLE);
	## Go geavahus ii duođaštuvvon leat álggahuvvon ovdal go... 


# Indicative Negative
# -------------------
  # Unique are Sg1, Sg3, Du1, Du2, Du3, Pl1, Pl2, Pl3
  # Sg2 "it" = ABBR.

# Select Neg Sg2 instead of ABBR
REMOVE ABBR IF (*-1 ConNeg OR PrfPrc)(0 Neg); ## Dáppe don leamaš it.
REMOVE ABBR IF (0 Neg)(*1 ConNeg OR PrfPrc);  ## Don it leat dál.


# Infinitive
# ----------
SELECT Inf IF (*-2 INF-VERB BARRIER VFIN OR NP-HEAD-NOM OR CS)(-1 Po)
	(NOT *-1 CP BARRIER CS OR VFIN);
	## Dearvvašvuođabargit galget divššohasa agi, silolaš dilálašvuođa,
	## láddama ja vásáhusduogáža ektui láhčit dilálašvuođaid buoremus
	## lági mielde.

SELECT Inf IF (NOT *-2 CC BARRIER NPNH LINK -1 Nom)(-1C (Sg Nom))
	(NOT 0 V-PL3 LINK *1 NP-HEAD-PL-NOM BARRIER NPNHA);
	## Mánáin lea vuoigatvuohta oažžut dearvvašvuođaiskkadeami.
	## Mus ii leat báljo dilli vuolgit.

SELECT Inf IF (-1 Acc)(NOT -1 (Pl Nom))(*1 Acc BARRIER NPNH);
SELECT Inf IF (-1 Acc)(NOT -1 Interr)(*1 Ill BARRIER NPNH);
	## Son vuolggahii bálvaleaddjiidis rávkat bovdejuvvon gussiid heajaide.
	## ...leago dieđiheamis doarvái vuođđu ášši váldit meannudeapmái.

SELECT Inf IF (-1 COMMA LINK *-1 (Pron Interr) LINK *-1 INF-VERB BARRIER VERB);
	## Dalle fertejit buotlágan ávnnaslaš eavttut maid dárbbaša vai
	## duođalaččat galgá sáhttit kultuvrras dikšut, gullat dása.

SELECT Inf IF (-1 COMMA LINK *-1 (Pron Interr) LINK *-1 (Pron Interr) 
	LINK *-1 INF-VERB BARRIER VERB);
	## Eai galgga sii geat gullet dakkáraš minoritehtaide ovttas eará 
	## láhttuiguin mat gullet sin jovkui, manahit vuoigatvuođaideaset dikšut 
	## iežaset kultuvra, dovddastit ja geavahit iežaset oskku, dahje geavahit 
	## iežaset giela.


# Coordinated infinitives
# - - - - - - - - - - - -
SELECT Inf (*-1 COMMA BARRIER VERB OR MO LINK -1C Inf)(NOT 0 (Neg Imprt))
	(NOT *-1 (Pl Nom) BARRIER CS)(NOT *1 (Pl Nom) BARRIER CS);
	## Mun in leat boahtán gomihit, muhto ollašuhttit.
	## Son galgá stuorrut, mun fas uhccut.

SELECT Inf IF (*-1 CC BARRIER VFIN OR CS LINK *-1C Inf BARRIER WORD); 
	## Hearrá Ipmilat galggat gudnejahttit, ja áivvestassii su bálvalit.

SELECT Inf IF (*-2C Inf BARRIER VERB)(-1 CC);
	## Duođalaš nana jáhku geažil lea divššohas vuoigatvuohta biehttalit
	## vuostáváldimis vara dahje varabuktagiid dahje biehttalit botkemis
	## nealgudeami mii lea jođus.
 
SELECT Inf IF (*-3 Interr LINK *-1 Inf BARRIER VERB)(-2 COMMA)(-1 CC);
	## Divššohasbearráigeahčis lea vuoigatvuohta buktit oaiviliiddis
	## dilálašvuođaid birra mat gullet bearráigeahčči bargoviidodahkii,
	## ja evttohit konkrehtalaš buoridandoaimmaid.


# Indicative or imperative
# ========================

REMOVE Imprt IF (-1 BOS)(0 Adv)(*1 ("?") BARRIER CS); 
	## Gosa don dál?

REMOVE Imprt IF (*-1 CS OR CP BARRIER PUNCT)(NOT -1 CLB OR ("de"))
	(NOT -1 CC LINK *-1 Imprt BARRIER CS);
	## Jus divššohas roasmmohuvvá dahje oažžu duođalaš váttisvuođaid,...

 # Very unlikely that an imperative should show up in an embedded clause, except 
 # in quotations (...de son celkkii: Manni eret!). But it can be preceded by an 
 # embedded clause, in which case we normally find a comma in front of the impera-
 # tive.


REMOVE Imprt IF (-1 Adv)(NOT -1 ("nu") OR ("de"));
	## Dalle skáhppo buođđobiergasiid.

REMOVE Imprt IF (-1 Imprt)(NOT 0 ConNeg); 
	## Geahča lagat Hálddašanjoavkku čielggadeami.

SELECT Imprt IF (-2C (N Nom) LINK *-1 BOS OR PUNCT BARRIER NPNH)(-1 COMMA)
	(NOT 0 MOD-ASP OR Pass)(NOT 0 (N Sg Nom) LINK *1 V-SG3 BARRIER CS OR VFIN)
	(NOT 0 (N Pl Nom) LINK *1 V-PL3 BARRIER CS OR VFIN)(NOT *1 ("?"));
	## Ustiban, daga dan man várás don leat boahtán!.

SELECT Imprt IF (-2C (N Nom) LINK *-1 COMMA BARRIER NPNH LINK -1 (N Nom) 
	LINK *-1 BOS OR PUNCT BARRIER NPNH)(-1 COMMA)
	(NOT 0 AUX OR Pass OR (N Nom))(NOT *1 ("?"));
	## Hearra, Davveda bárdni, árpmit mu!

SELECT Imprt IF (*-2 ("dadjat") OR ("cealkit") OR ("vástidit") BARRIER VERB)
	(-1 PUNCT)(NOT 0 Pass)(NOT 0 MOD-ASP);
	## Jesus celkkii sutnje: "Várut dadjamis maidege geasage dan birra!" 

REMOVE Imprt IF (-1 Interr OR CP OR CS)(NOT -1 ("de"));
	## Rohkadallet sin ovddas geat doarrádallet din.

REMOVE Imprt IF (*-1 VERB BARRIER NON-ADV OR CLB LINK NOT 0 Neg);
	## Eamiálbmotriektái gullá maiddái muhtun muddui njulget vahágiid,...


# Some imperatives according to person and number.
# ------------------------------------------------

REMOVE (Imprt Sg2) IF (0 V-SG2)(NOT *1 COMMA OR CC BARRIER VFIN LINK 1C (Imprt Sg2));
	## Don gii gaikkodat tempela ja hukset dan fas ođđasit golmma beaivvis!

SELECT (Imprt Sg2) IF (NOT *-1 V-SG2 BARRIER VFIN)(*-1 PUNCT OR CLB 
	BARRIER NON-ADV-PCLE)(1 (PxSg2));
	## Nahket miehkát fas dohppii.

REMOVE (V Ind Prs Sg3) IF (*-1 BOS BARRIER NON-PCLE)(0 (V Imprt Prs Sg2))
	(NOT 0 A)(NOT 1 ("."));
 	## Na attes munnje daid.


REMOVE (Imprt Du2) IF (*1 (Pron Pers Nom) BARRIER CS OR CP OR PUNCT LINK NOT 0 Du2);
REMOVE (Imprt Du2) IF (*1 Sg2 BARRIER CS OR CP OR PUNCT);
	## Dat guhte goargü eará báikki, son lea suola ja rievvár.
	## Velá okta váilu dus.

SELECT (Imprt Pl2) IF (*1 (PxPl2) BARRIER CC OR CS OR CP);
	## Váldet mu geassá badjelasadet.


# Imperative in coordination
# --------------------------
SELECT Imprt IF (NOT 0 V-SG3 LINK -1 COMMA LINK *-1 V-SG3 BARRIER VFIN OR CS)
	(-1 BOS OR COMMA)(*1 COMMA LINK 1 Imprt)(NOT 0 (A Attr) LINK 1 N);
	## Bohtet, goddot su, de mii oažžut su árbbi.

SELECT Imprt IF (NOT *-1 VFIN-NOT-IMP BARRIER CS OR CP)(NOT 0 PrfPrc)
	(*1 CC BARRIER VFIN LINK *1C Imprt BARRIER VFIN OR CS OR CP);
	## Čoavdi daid ja bukti mu lusa!

SELECT Imprt IF (-1 COMMA OR CC)(NOT 0 Pron)(*-2 Imprt BARRIER VFIN LINK NOT *-1 CP 
	BARRIER PUNCT OR REALCLB); 
	## Mannet dal ja guorahallet dán máná hárrái!

SELECT Imprt IF (*-2C Imprt)(-1 COMMA);
	## Allet almmi bokte, dasgo dat lea Ipmila truvdnu, allet ge eatnama bokte...

SELECT (Imprt Sg2) IF (*1 CC OR COMMA BARRIER VFIN LINK *1C (Imprt Sg2)
	BARRIER VFIN OR CS OR CP);
	## Gohčo bargiid boahtit ja atte dáidda bálkká!

SELECT (Imprt Sg2) IF (*-1 CC BARRIER NON-ADV LINK *-1C (Imprt Sg2));
	## Váldde dan mii gullá dutnje ja mana!
 
SELECT (Imprt Pl2) IF (*1 CC OR COMMA BARRIER VFIN LINK *1C (Imprt Pl2)
	BARRIER VFIN OR CS OR CP);
	## Čatnet su gieđaid ja julggiid ja bálkestehket su olggobeallái..
 
REMOVE Imprt IF (*-1 CC BARRIER NON-ADV-PCLE OR REALCLB LINK NOT *-1 Imprt)
	(NOT -1 CC LINK -1 BOS);
	## Sus lea goaivu gieđas ja áigu buhtistit gortniidis.


# Rule that finds a coordinated Imprt across a relative clause:
# - - - - - - - - - - - - - - - - - - - - - - - - - - - - - - -
SELECT (Imprt Pl2) IF (-1 CC OR COMMA)(*-2 VFIN BARRIER CS OR CP OR CONTRA 
	LINK *-1 (Pron Interr) BARRIER CS LINK *-1 (Imprt Pl2) 
	BARRIER VFIN OR CS OR CP);
	## Buressivdnidehket sin geat garrudit din, dahket buriid sidjiide
	## geat vašuhit din, ja rohkadallet sin ovddas geat doarrádallet din.


# Negative imperative
# -------------------
  # Neg Imprt Pl2 = allet V Inf = Prs Pl1 = Prs Pl3

SELECT (Neg Imprt Pl2) IF (*1 ConNeg BARRIER Neg); 
	## Allet ge gátte ahte ...



# Verbs according to person and number
# ====================================

# Sg1
# ---
 # Prs Sg1 = PtcPrf (odd-syllables), and Prs Sg1 = V Actio (even-syllables)
 # Past Sg1 is unique.

SELECT Sg1 IF (0 LEAN) (*1 PrfPrc BARRIER CS);
	## Dál lean cealkán dan dudnuide.

SELECT Sg1 IF (NOT *-1 VFIN BARRIER CS OR PUNCT OR REALCLB)(0 LEAN);
	## Jesus celkkii: "Oanehis áiggi lean velá din luhtte."

SELECT V-SG1 IF (NOT *-1 VFIN OR PrfPrc BARRIER CS OR REALCLB)(NOT 0 Pass)
	(NOT 0 (N Nom) LINK 0 LEX-N)(NOT *1 VFIN BARRIER CS);
	## Oađidettiin gullen olbmo boahtimin.

SELECT V-SG1 IF (*-1 CS BARRIER VFIN LINK -1 BOS)(*1 COMMA BARRIER VFIN);
	## Ovdal go čielggadan dan lagabui,...

SELECT V-SG1 IF (*-1 CC BARRIER VERB LINK *-1C V-SG1 BARRIER VERB)(NOT 0 Pass);
	## Das maŋŋá dajan juoidá álbmotrievtti mearkkašumi birra, ja geavahan
	## das veahkkin ođđaseamos riektegáldu.

REMOVE (V Sg1) IF (*-1 CC BARRIER VERB LINK NOT *-1 V-SG1);
	## Olbmot ledje ija dolastallan ja oggon.

REMOVE V-SG1 IF (0 PrfPrc)(*1 N BARRIER NPNH LINK *1C VFIN BARRIER NON-NP);
	## Dálá fiinnuhan riektejurddašanvuogi mielde ii eahpiduvvo ahte 
	## dán livččii galgan dahkat.


# Sg2
# ---
 # Sg2 = Inf (even syllables, copula)
 # Note the homonymy with derived verbs (jearat = jearadit + ...
 # Prt Sg2 = Prs Pl3 = Imprt Prs Pl2 (even syll)    vulget
 # Prt Sg2 = Prs Pl3 = Prs Pl1 = Inf (odd syll)   muitalit
 # Prt Sg2 = Prs Pl3                 (contracted) fertejit


SELECT V-SG2 IF (*-2C V-SG2 BARRIER VFIN OR CS OR CP OR PUNCT OR REALCLB)
	(-1 Interr)(NOT -1 (Pl Nom));
	## Don leat garra olmmái gii láddjet doppe gosa it leat gilván.

SELECT Prt IF (*1 IKTE BARRIER VFIN OR CS OR CP);
	## Vulget go ikte?

SELECT V-SG2 IF (*-1 CC BARRIER VFIN LINK *-1C V-SG2 BARRIER VFIN OR Inf OR CS);
	## Dasgo sániidat mielde don dahkkojuvvot friddjan, ja sániidat mielde
	## dubmejuvvot.

SELECT V-SG2 IF (*-1 CC BARRIER NON-ADV LINK *-1 CP LINK *-1C V-SG2 BARRIER
	NON-ADV-PCLE);
	## Don oainnát mo olbmot bahkkejit du birra, ja de jearat vel...

SELECT (V Sg2) IF (*-2 (V Sg2) BARRIER VFIN)(-1 CS)(NOT *1 PxPl3);
	## Don it beasa doppe olggos ovdal go leat máksán.

REMOVE (V Sg2) IF (1 CC)(2 VFIN)(NOT 2 Sg2);
	## Buođđomuoraid maid ráhkadit olbmot gárvvisin dassá go deatnu
	## leahkkasa, čuolddaid čohket ja ráhkadit oar-julggiid.


# Sg3
# ---
 # Prs Sg3 = ConNeg (even)
 # Prs Sg3 = ConNeg = Imprt Sg2 (contracted)
 # Prs Sg3 = VGen
 # Prt Sg3 = Sg Ill for certain words!!
 # Prt Sg3 = Imprt Prs Sg3 (leai)

REMOVE (V Sg3) IF (0 (Sg Nom))(*1C (V Sg3) BARRIER CLB OR CC OR CS OR CP OR go
	LINK NOT 1 ("go" Pcle));
	## Man uhccán osku dus lea!

SELECT V-SG3 IF (*-1 (Sg Nom) BARRIER NON-ADV-PCLE)(NOT 0 Inf OR (N Pl Nom))
	(NOT 0 Ill LINK *-1 Gen BARRIER NON-ADV-PCLE);
	## ...ja de olmmoš suohppu fávleoalggi nu guhkás vulos go...

SELECT V-SG3 IF (*-1 (Pron Sg3 Nom) BARRIER NON-ADV-PCLE);
	## Dat áigu leat ruovttus.

SELECT V-SG3 IF (*-1 Loc BARRIER NON-ADV-PCLE)(*1 NP-HEAD-SG-NOM BARRIER NPNHA);
	## Dát leat sii geat sirrejit olbmuid; sii leat máilmmálaččat 
	## ja sis váilu Vuoigŋa.

SELECT V-SG3 IF (*-1 ("gii" Pron Interr Sg Loc) BARRIER CS OR CP); 
	## Sus geas ii leat, váldojuvvo velá dat ge eret mii sus lea.

SELECT V-SG3 IF (*-1 BOS OR CS BARRIER Nom)(NOT 0 Adv)
	(*1 (Sg3 Nom) OR (Pron Sg Nom) BARRIER NON-ADV-PCLE LINK NOT 0 Refl);
	## ...ahte vuoigatvuhtii automáhtalaččat gullá dat mii guorrana...

SELECT (V Sg3) IF (*-1 BOS OR CS BARRIER Nom)(NOT 0 Adv OR Attr)
	(*1 (N Sg Nom) OR (A Sg Nom) BARRIER NPNHA);
	## Dán guovtti báhkkoma alde orru oppa láhka ja profehtat.

SELECT V-SG3 IF (-1 CLB)(NOT 0 Adv)(*1 (N Sg Nom) BARRIER NPNH);
	## Earáide fas, geat eai máhte sámegiela, orru vástádus eahpečielggas. 

SELECT V-SG3 IF (*-1 (N Sg Nom) BARRIER NON-ADV-PCLE)(0 (N Sg Ill))
	(*1 Acc BARRIER NPNH);
	## Dalle isit gohčui bálvaleaddji fas lusas.

REMOVE Ill IF (NOT *-1 ("seammá") OR ("eará") BARRIER NPNH)(1 ("go" Pcle));
