# Po or Pr?
# ---------

SELECT Po IF (-1 (PUNCT RIGHT))(*-2 (PUNCT LEFT) LINK -1 Gen);
	## ...sáhttá dulkot telestudio dahje govvatelefovnna (ruoná
	## nummira) bokte.

REMOVE Po IF (NOT -1 (N Gen) OR (A Gen) OR (Num Gen) OR (Pron Gen) OR (Actio Gen));
REMOVE Po IF (-1 N)(NOT -1 Gen);   
REMOVE Pr IF (NOT *1 N OR A OR Num OR Pron BARRIER NPNH);
	## Jus divššohas lea badjel 16 jagi boaris,... 
	## Leansmánni lea kommišuvnnas fárus.
	## Na mun gal hupmen dan birra.

SELECT Pr IF (NOT -1 Gen OR (Pron Interr Nom))(NOT 0 A OR VFIN)
	(*1 (N Gen) BARRIER NPNH LINK 0 HNOUN);
SELECT Pr IF (NOT -1 Gen)(1 (Pron Gen));

REMOVE Pr IF (NOT 1 (Num Gen))(NOT *1 (N Gen) BARRIER NPNH);
	## Olbmuin bisttii luossaguolli miehtá dálvvi. 
	## Ovdal dan aiggi ledje sápmelaččat boahtán dán guvlui.
	## Gonagas addá láhkaásahusa lága geavaheami birra Svalbarddas.

SELECT Po IF (*-1 BOS OR CS OR CLB BARRIER NPNH)(-1 Gen)(NOT 0 ("guovddáš"));
SELECT Po IF (-1 Gen)(1 CC)(NOT -1 Acc LINK *-1 CC BARRIER NON-NP LINK -1 Acc LINK
	-1 V-NON-AUX-PASS BARRIER NPNHA);
SELECT Po IF (-1 Gen)(NOT -1 VERB OR Adv)(1 TIME); 
	## Dán vuostá sáhttá dadjat, ahte ...
	## Jos sáddo deavdá buođu vuollái dahje jos sáddo hávddarda buođu vuolde... 
	## Sámi guovlluin ožžo Sámeparlameantta Suoma beallái jagi 1976.

SELECT Po IF (-1 (Interr Gen));
	## Vuoi dan olbmo gean bokte fillehusat bohtet.

SELECT Pr IF (NOT 0 Attr OR VFIN)(*1 (N Gen) OR (Pron Gen) BARRIER PUNCT 
	LINK NOT 0 Cmpnd LINK NOT *1 N OR Po BARRIER CC OR CP OR CS OR CLB);
SELECT Pr IF (1 (Num Gen));
	## Sáhttá dahttege vurket journála olggobealde doaimma. 
	## Vuollel 12 jahkásaš mánáin ii leat iešheanalis geahččanvuoigatvuohta.

REMOVE Pr IF (NOT 1 (Pron Gen));
	## Geavatlaš sivaid geažil čatnasa bajimus submi namuhuvvon máksomearrái. 

SELECT Po IF (-1 Gen)(NOT -1 VFIN OR ("leat" PrfPrc) OR Adv)(NOT -1 Acc LINK *-1 CC 
	BARRIER NON-NP LINK -1 Acc LINK *-1 V-NON-AUX-PASS BARRIER NPNHA)
	(NOT 0 Attr OR ("guovddáš"))(NOT 1C Po);
	## Daid ovddas gal in máksán maidege.

SELECT Po IF (-1 Num)(0 ("rádjai")); # Make general rule!
REMOVE Po IF (NOT -1 Gen OR Num);
	## ... paragráfaide 3-18 rájes 3-21 rádjái.
	## Jus divššohas dasa miehtá,... 


# Removing some readings that compete with Po
# - - - - - - - - - - - - - - - - - - - - - -
REMOVE N IF (-1 Gen)(0 Po)(NOT 0 ("guovddáš"))(NOT 1C Po);
REMOVE V IF (-1 Gen)(0 Po)(NOT 1C Po);
	## ...maid ektui sáhtii leat buori oskkus.
	## Gávpot mii lea vári alde, ii sáhte leat čihkosis.


 
