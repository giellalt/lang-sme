SELECT:vaiCCNegQst2 (CC @CNP) IF (-1 N OR A OR Adv)(0 ("vai"))(*1 N OR A OR Adv BARRIER NOT-NPMOD LINK NOT 0 VFIN LINK NEGATE *2C VFIN BARRIER S-BOUNDARY OR CS) ; # (*1 ("?")); 
