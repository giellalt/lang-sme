SELECT:NumNomSubj (Num Sg Nom)(0 NUM LINK -1 V + Pl3  LINK NOT 0 Imprt LINK NEGATE *-1 (N Pl Nom) OR (Pers Pl3) OR (Coll Nom) OR Sem/Group + Nom OR NUM + Nom BARRIER SV-BOUNDARY)(NEGATE 1 Gen LINK 1 Po) ; 
