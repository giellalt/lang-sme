# This is only a tool for making the following sets, bacause of that the symbols (|) and (-) in the set don´t function as one had expected.
SET V-TRANS-ACT = V-TRANS - (TV Der/PassS) - (TV Der/PassL);
 #XXX Never acc-obj for a.l. or actor?
 # All active verbs with a TV tag, including AUX-OR-MAIN.
 
SET V-TRANS-ACT-NOT-AUX = V-TRANS - Der/Pass - AUX ;

SET V-TRANS-ACT-NOT-ACT = V-TRANS - Der/Pass - Der/n - Actio - VActor - PrsPrc - Attr OR (TV Actio Ess) ;

SET V-TRANS-ACT-NOT-ACT-NOT-AUX = V-TRANS - Der/Pass - Der/n - Actio - VActor - AUX OR (Actio Ess) ;


