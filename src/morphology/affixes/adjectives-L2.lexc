LEXICON BUORRE  !!= * __@CODE@__  For this adj only
! Positive
 +A:%>X7 NomVadj "good A" ;
 +A:%>X1 EssVadj "good A" ;
 +A:%>X7 EssVadj ;
 +A:%>   GOAHTI-OBLadj "good A" ;
 +A+Sg+Nom+Err/Orth:%> Px3V ;
 +A+Sg+Gen+Err/Orth:%>X8 Px3V ;
 +A+Pl+Nom+Err/Orth:%>X5jd%> Px1C ;
 +A+Pl+Nom+Err/Orth:%>%^DISIMPjd%> Px1C ;
! +A+Use/-Spell: VUOHTA ;         ! ... to A > N -vuohta derivation - L2

! Comparative and superlative
 +A+Comp:%>X8b BUStem ;
 +A+Superl:%>X8muss BUOREMUS ;

! Derivation and compounding
! +A+Use/-Spell:»X7# NAMAT ; ! comp-only adj. Here since buorre has no Attr, not compound. - L2
! +A#+Use/-Spell:»X7# NAMAT ; ! comp-only adj. Here since buorre has no Attr
 +A#+Use/Circ:»X7# ALIT ;  ! both comp and independent adj. Here for ditto reason.

LEXICON BUOROT  !!= * __@CODE@__ SUB, Southern dialect

 +A+Comp+Sg+Nom+Err/Orth:%>buorot K ;
! +A+Comp+Sg+Acc+Err/Orth:%>buorot K ;
! +A+Comp+Sg+Gen+Err/Orth:%>buorot K ;
 +A+Comp+Attr+Err/Orth:%>buorot K ;  


LEXICON ALKI  !!= * __@CODE@__  Bisyll V-Adj, -es-Attr, no WeG.
 +A:%>W2s ATTR ;
 +A:%> LAIKI0 ;
 +A:%> BU/MUS ; ! no CG in comparative?

LEXICON SEARRA  !!= * __@CODE@__ Bisyll. V-Adj's with s-Attr in WeG.
 +A:%>W1s ATTR ;
 +A:%> LAIKI0 ;
 +A:%>X4 BU/MUS ;

LEXICON HOHPI  !!= * __@CODE@__ Bisyll. V-Adj's with s-Attr. in WeG & Adv.
 +A:%>X4s ATTR ; ! X4 => WeG, not W1 => no vow ch
 +A:%> LAIKI0 ;
 +A:%>X4 BU/MUS ;
 +A:%> ADVV ;

LEXICON LAIKI  !!= * __@CODE@__ Bisyll. V-Adj's with es-Attr. in WeG & Adv.
 +A:%>W1s ATTR ;
 +A+Err/Orth:%>s ATTR ;
