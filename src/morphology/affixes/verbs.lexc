!!! 2a. special kind of u-verbs
!   ------------------------------------------

LEXICON DEAKCU_TV   !!= * __@CODE@__ as BORRA for u-verbs with dim -astit, and a-verbs with dim -istit that are hardcoded
 +V+TV:%> DIEHTIStem ;
 +V+TV: DeverbalVerbsBOLTU ;


LEXICON BOAZZU_IV  !!= * __@CODE@__ as DIEVVA_IV for u-verbs with dim -astit, and a-verbs with dim -istit that are hardcoded
 +V+IV:%> BOAHTIStem ;
 +V+IV: DeverbalVerbsBINDU ;


LEXICON BINDU_IV  !!= * __@CODE@__ as DIEVVA (but without short passive) for u-verbs with dim -astit, that are hardcoded
 +V+IV:%>Y7j SG3PASS ;
 +V+IV+Der3+Der/PassL+V+IV+Der4+Der/supmi+N+Sg+Ill+Use/-PLX:»Y7supmái K ;
 +V+IV:%> BOAHTIINCH ;
 +V+IV: DeverbalVerbsBINDU ;

LEXICON DAHTU_TV  !!= * __@CODE@__  As diehti, but -ut verbs, thus without short passive
 +V+TV:%>Y7j PASSIVE ;
 +V+TV+Der3+Der/PassL+V+IV+Der4+Der/supmi+N+Sg+Ill+Use/-PLX:»Y7supmái K ;
 +V+TV:%> DIEHTIINCH ;
 +V+TV: DeverbalVerbsBORRA ;

LEXICON BOLTU_TV  !!= * __@CODE@__  As DAHTU_TV but with dim -astit that are harcoded
 +V+TV:%>Y7j PASSIVE ;
 +V+TV+Der3+Der/PassL+V+IV+Der4+Der/supmi+N+Sg+Ill+Use/-PLX:»Y7supmái K ;
 +V+TV:%> DIEHTIINCH ;
 +V+TV: DeverbalVerbsBOLTU ;

LEXICON ALLU_IV  !!= * __@CODE@__  -ut verbs, thus without short passive
 +V+IV:%>Y7j SG3PASS ;
 +V+IV:%> BOAHTIINCH ;
 +V+IV: DeverbalVerbsDIEVVA ;


