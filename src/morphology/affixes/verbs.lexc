!    ---------------------------------
!! !!Basic lexica for bisyllabic verbs
!    ---------------------------------


!! !Modals
! ------

!! These are treated separately because
!!  modals do not participate in derivation

LEXICON GALGA_IV !!= * __@CODE@__ only dáidit, galgat
 +V+IV+Ind+Prs:%> PrsV ;
 +V+IV+Ind+Prt:%> PrtV ;
 +V+IV+Pot+Prs:%>Q7ž PotC ;
 +V+IV+Cond:%> CondV1 ;
 +V+IV:%> NominalFormsV1 ;
 +V+IV:%>X2 NominalFormsV4 ;
 +V+IV+Der4+Der/muš+N+Use/-Spell:»mušš MUSH ;



LEXICON FERTE_IV !!= * __@CODE@__  only fertet and bállet
 +V+IV+Ind+Prs:%>Y5 VSufDOHPPE ;
 +V+IV+Cond:%>Y5 CondV2 ;
 +V+IV+Pot+Prs:%>Y5ž PotC ;
 +V+IV+Ind:%>j CSufDOHPPE ;
 +V+IV+Ind+Prt:%>Y6 PrtV1 ;
 +V+IV:%> NominalFormsV4 ;
 +V+IV:%> NominalFormsV1 ;
 +V+IV+Der4+Der/muš+N+Use/-PLX:»mušš MUSH ;
 +V+IV+Der4+Der/NomAct+A:»m BOAHTALADDAN ; !Turi: fertema



!! !Ordinary bisyllabic verbs
!!! 1. DEFAULT SECTION
!   -------------------------


LEXICON DIEHTI_TV       !!= * __@CODE@__ Bisyllabic i-verbs with Personal Passive
 +V+TV:%> DIEHTIStem ;
 +V+TV: DeverbalVerbsDIEHTI ;
 
 
LEXICON BORRA_TV       !!= * __@CODE@__ Bisyllabic a- and u-verbs with Personal Passive
 +V+TV:%> DIEHTIStem ;
 +V+TV: DeverbalVerbsBORRA ;
 +V+TV+PrfPrc+Err/Orth:Q1n K ;
 
 
LEXICON BOAHTI_IV  !!= * __@CODE@__  Bisyllabic i-verbs without Personal Passive but with Der/NomAg
 +V+IV:%> BOAHTIStem ;
 +V+IV: DeverbalVerbsBOAHTI ;

LEXICON DIEVVA_IV  !!= * __@CODE@__  Bisyllabic a- and u-verbs without Personal Passive but with Der/NomAg
 +V+IV:%> BOAHTIStem ;
 +V+IV: DeverbalVerbsDIEVVA ;
 +V+IV+PrfPrc+Err/Orth-a-á:Q1n K ;






!! !Bisyllabic verbs
!!! 2. SPECIAL SECTION:
!!! 2a. special kind of u-verbs
!!! 2b. Already derived words - no deverbal verbs
!!! 2c. impersonals
!!! 2d. lexicas tweaked (mostly) for speller reasons
!!! 2e. lexicas for special compound behavious
!!! 2f. other special lexicas
